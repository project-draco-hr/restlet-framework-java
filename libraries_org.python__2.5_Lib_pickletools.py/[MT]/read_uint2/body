def read_uint2(f):
    "\n    >>> import StringIO\n    >>> read_uint2(StringIO.StringIO('\\xff\\x00'))\n    255\n    >>> read_uint2(StringIO.StringIO('\\xff\\xff'))\n    65535\n    "
    data = f.read(2)
    if (len(data) == 2):
        return _unpack('<H', data)[0]
    raise ValueError('not enough data in stream to read uint2')
