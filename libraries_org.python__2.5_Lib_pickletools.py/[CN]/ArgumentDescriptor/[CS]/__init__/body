def __init__(self, name, n, reader, doc):
    assert isinstance(name, str)
    self.name = name
    assert (isinstance(n, int) and ((n >= 0) or (n in (UP_TO_NEWLINE, TAKEN_FROM_ARGUMENT1, TAKEN_FROM_ARGUMENT4))))
    self.n = n
    self.reader = reader
    assert isinstance(doc, str)
    self.doc = doc
