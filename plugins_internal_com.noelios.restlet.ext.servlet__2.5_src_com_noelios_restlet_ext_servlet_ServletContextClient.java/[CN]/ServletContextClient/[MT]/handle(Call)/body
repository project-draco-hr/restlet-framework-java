{
  if (call.getResourceRef().getScheme().equalsIgnoreCase("context")) {
    ContextReference cr=new ContextReference(call.getResourceRef());
    if (cr.getAuthorityType() == AuthorityType.WEB_APPLICATION) {
      handleServletContext(call);
    }
 else {
      super.handle(call);
    }
  }
 else {
    super.handle(call);
  }
}
