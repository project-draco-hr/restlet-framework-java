{
  if (this.directoryRedirection) {
    if (this.originalRef != null) {
      getResponse().redirectPermanent(this.originalRef.getIdentifier() + "/");
    }
 else {
      getResponse().redirectPermanent(getRequest().getResourceRef().getIdentifier() + "/");
    }
  }
 else {
    super.handleGet();
  }
}
