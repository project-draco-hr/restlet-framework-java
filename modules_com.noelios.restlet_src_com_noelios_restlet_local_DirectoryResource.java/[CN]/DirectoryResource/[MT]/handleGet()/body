{
  if (directoryRedirection && !indexTarget) {
    if (originalRef != null) {
      getResponse().redirectPermanent(originalRef.getIdentifier() + "/");
    }
 else {
      getResponse().redirectPermanent(getRequest().getResourceRef().getIdentifier() + "/");
    }
  }
 else {
    super.handleGet();
  }
}
