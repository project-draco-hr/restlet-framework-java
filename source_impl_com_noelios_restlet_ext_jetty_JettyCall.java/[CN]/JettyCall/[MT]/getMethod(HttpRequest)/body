{
  String method=request.getMethod();
  if (method.equals(HttpRequest.__DELETE))   return MethodEnum.DELETE;
 else   if (method.equals(HttpRequest.__GET))   return MethodEnum.GET;
 else   if (method.equals(HttpRequest.__POST))   return MethodEnum.POST;
 else   if (method.equals(HttpRequest.__PUT))   return MethodEnum.PUT;
 else   return new MethodImpl(method);
}
