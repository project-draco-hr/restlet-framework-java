def _raw_input(prompt='', stream=None):
    if (stream is None):
        stream = sys.stdout
    prompt = str(prompt)
    if prompt:
        stream.write(prompt)
        stream.flush()
    line = sys.stdin.readline()
    if (not line):
        raise EOFError
    if (line[(-1)] == '\n'):
        line = line[:(-1)]
    return line
