{
  boolean readNext=true;
  Parameter param=null;
  try {
    while (readNext) {
      param=readNextParameter();
      if (param != null) {
        form.add(param);
      }
 else {
        readNext=false;
      }
    }
  }
 catch (  final Exception ioe) {
    System.err.println("Unable to parse a form parameter. Skipping the remaining parameters.");
  }
}
