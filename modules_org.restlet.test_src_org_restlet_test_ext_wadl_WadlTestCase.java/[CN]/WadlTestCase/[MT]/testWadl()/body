{
  final WadlComponent comp=new WadlComponent("clap://class/org/restlet/test/ext/wadl/YahooSearch.wadl");
  final WadlApplication app=(WadlApplication)comp.getHosts().get(0).getRoutes().get(0).getNext();
  assertNotNull(app);
  assertEquals(app.getInboundRoot(),app.getRouter());
  assertNotNull(app.getInboundRoot());
}
