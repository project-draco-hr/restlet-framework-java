def test_basic():
    test_support.requires('network')
    import urllib
    socket.RAND_status()
    try:
        socket.RAND_egd(1)
    except TypeError:
        pass
    else:
        print "didn't raise TypeError"
    socket.RAND_add('this is a random string', 75.0)
    f = urllib.urlopen('https://sf.net')
    buf = f.read()
