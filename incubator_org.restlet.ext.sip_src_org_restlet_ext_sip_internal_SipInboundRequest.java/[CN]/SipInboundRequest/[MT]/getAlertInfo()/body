{
  if (!alertInfoAdded) {
    String header=(getHeaders() == null) ? null : getHeaders().getValues(SipConstants.HEADER_ALERT_INFO);
    if (header != null) {
      try {
        setAlertInfo(new AddressReader(header).readValue());
      }
 catch (      Exception e) {
        Context.getCurrentLogger().info(e.getMessage());
      }
    }
    alertInfoAdded=true;
  }
  return super.getAlertInfo();
}
