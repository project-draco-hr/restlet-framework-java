{
  List<Address> r=super.getRoutes();
  if (!routesAdded) {
    String header=(getHeaders() == null) ? null : getHeaders().getValues(SipConstants.HEADER_ROUTE);
    if (header != null) {
      try {
        r.addAll(new AddressReader(header).readValues());
      }
 catch (      Exception e) {
        Context.getCurrentLogger().info(e.getMessage());
      }
    }
    routesAdded=true;
  }
  return r;
}
