{
  this.shell.put("request",request);
  this.shell.put("response",response);
  this.shell.writeLine("a request is available in a variable called 'request'");
  this.shell.writeLine("a response is available in a variable called 'response'");
  this.shell.loop();
  super.handle(request,response);
  this.shell.writeLine("\nlistening...");
}
