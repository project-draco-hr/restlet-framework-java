{
  Engine engine=Engine.getInstance();
switch (cc) {
case INTERNAL:
    engine.getRegisteredClients().add(0,new org.restlet.engine.connector.HttpClientHelper(null));
  break;
case HTTP_CLIENT:
engine.getRegisteredClients().add(0,new org.restlet.ext.httpclient.HttpClientHelper(null));
break;
case NET:
engine.getRegisteredClients().add(0,new org.restlet.ext.net.HttpClientHelper(null));
break;
}
switch (sc) {
case INTERNAL:
engine.getRegisteredServers().add(0,new org.restlet.engine.connector.HttpServerHelper(null));
break;
case JETTY:
engine.getRegisteredServers().add(0,new org.restlet.ext.jetty.HttpServerHelper(null));
break;
case SIMPLE:
engine.getRegisteredServers().add(0,new org.restlet.ext.simple.HttpServerHelper(null));
break;
}
}
