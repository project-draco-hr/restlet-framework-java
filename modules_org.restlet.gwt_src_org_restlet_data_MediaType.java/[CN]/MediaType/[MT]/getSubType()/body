{
  String result=null;
  if (getName() != null) {
    int slash=getName().indexOf('/');
    if (slash == -1) {
      result="*";
    }
 else {
      int separator=getName().indexOf(';');
      if (separator == -1) {
        result=getName().substring(slash + 1);
      }
 else {
        result=getName().substring(slash + 1,separator);
      }
    }
  }
  return result;
}
