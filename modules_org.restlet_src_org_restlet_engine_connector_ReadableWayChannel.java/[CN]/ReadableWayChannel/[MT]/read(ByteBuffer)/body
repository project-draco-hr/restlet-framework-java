{
  int result=-1;
synchronized (getBuffer()) {
    if ((getBuffer() != null) && (getBuffer().hasRemaining())) {
      result=Math.min(getBuffer().remaining(),dst.remaining());
      for (int i=0; i < result; i++) {
        dst.put(getBuffer().get());
      }
    }
 else {
      result=getWrappedChannel().read(dst);
    }
  }
  return result;
}
