{
  final StringBuilder sb=new StringBuilder();
  CacheDirective directive;
  for (int i=0; i < directives.size(); i++) {
    if (i > 0) {
      sb.append(", ");
    }
    directive=directives.get(i);
    try {
      append(directive,sb);
    }
 catch (    IOException e) {
    }
  }
  return sb.toString();
}
