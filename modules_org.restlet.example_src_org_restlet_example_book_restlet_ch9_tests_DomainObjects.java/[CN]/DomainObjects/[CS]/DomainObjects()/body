  admin.setAdministrator(true);
  mailRoot.getUsers().add(admin);
  mailRoot.getMailboxes().add(createMailbox(user));
  mailRoot.getMailboxes().add(createMailbox(user));
  mailRoot.getMailboxes().add(createMailbox(admin));
}
