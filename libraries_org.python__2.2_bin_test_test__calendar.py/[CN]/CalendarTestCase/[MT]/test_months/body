def test_months(self):
    for attr in ('month_name', 'month_abbr'):
        value = getattr(calendar, attr)
        self.assertEqual(len(value), 13)
        self.assertEqual(len(value[:]), 13)
        self.assertEqual(value[0], '')
        d = {}
        for v in value:
            d[v] = 1
        self.assertEqual(len(d), 13)
