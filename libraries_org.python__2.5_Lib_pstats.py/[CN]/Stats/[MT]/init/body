def init(self, arg):
    self.all_callees = None
    self.files = []
    self.fcn_list = None
    self.total_tt = 0
    self.total_calls = 0
    self.prim_calls = 0
    self.max_name_len = 0
    self.top_level = {}
    self.stats = {}
    self.sort_arg_dict = {}
    self.load_stats(arg)
    trouble = 1
    try:
        self.get_top_level_stats()
        trouble = 0
    finally:
        if trouble:
            print  >> self.stream, 'Invalid timing data',
            if self.files:
                print  >> self.stream, self.files[(-1)],
            print  >> self.stream
