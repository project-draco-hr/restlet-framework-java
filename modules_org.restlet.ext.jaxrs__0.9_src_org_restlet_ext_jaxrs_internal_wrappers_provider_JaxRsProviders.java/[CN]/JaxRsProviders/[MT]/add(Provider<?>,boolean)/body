{
  if (provider.isWriter()) {
    if (defaultProvider)     this.messageBodyWriters.add(provider);
 else     this.messageBodyWriters.add(0,provider);
  }
  if (provider.isReader()) {
    if (defaultProvider)     this.messageBodyReaders.add(provider);
 else     this.messageBodyReaders.add(0,provider);
  }
  if (provider.isContextResolver())   this.contextResolvers.add(provider.getContextResolver());
  if (provider.isExceptionMapper())   this.add(provider.getExcMapper());
  this.all.add(provider);
}
