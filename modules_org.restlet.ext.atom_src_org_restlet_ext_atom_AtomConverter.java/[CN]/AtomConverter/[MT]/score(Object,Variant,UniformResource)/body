{
  float result=-1.0F;
  if (source instanceof Feed) {
    if (MediaType.APPLICATION_ATOM.isCompatible(target.getMediaType())) {
      result=1.0F;
    }
 else {
      result=0.5F;
    }
  }
 else   if (source instanceof Service) {
    if (MediaType.APPLICATION_ATOMPUB_SERVICE.isCompatible(target.getMediaType())) {
      result=1.0F;
    }
 else {
      result=0.5F;
    }
  }
  return result;
}
