def test_exercise_all_methods(self):
    'Exercising all methods once.'
    try:
        h = hmac.HMAC('my secret key')
        h.update('compute the hash of this text!')
        dig = h.digest()
        dig = h.hexdigest()
        h2 = h.copy()
    except:
        fail('Exception raised during normal usage of HMAC class.')
