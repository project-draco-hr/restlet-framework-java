def test_func_complex(self):

    def f((((x,),),), a, b, c):
        return (x, a, b, c)
    self.check(f, (3, 9, 8, 7), (((3,),),), 9, 8, 7)

    def f((((x,),),), a, b, c):
        return (x, a, b, c)
    self.check(f, (3, 9, 8, 7), (((3,),),), 9, 8, 7)

    def f(a, b, c, (((x,),),)):
        return (a, b, c, x)
    self.check(f, (9, 8, 7, 3), 9, 8, 7, (((3,),),))
