def test_controlcharacters(self):
    for i in range(128):
        c = chr(i)
        testString = ('string containing %s' % c)
        if ((i >= 32) or (c in '\r\n\t')):
            plistlib.writePlistToString(testString)
        else:
            self.assertRaises(ValueError, plistlib.writePlistToString, testString)
