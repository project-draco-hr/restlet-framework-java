{
  List<OptionTag> r=super.getRequires();
  if (!requiresAdded) {
    String header=(getHeaders() == null) ? null : getHeaders().getValues(SipConstants.HEADER_REQUIRE);
    if (header != null) {
      try {
        r.addAll(new OptionTagReader(header).readValues());
      }
 catch (      Exception e) {
        Context.getCurrentLogger().info(e.getMessage());
      }
    }
    requiresAdded=true;
  }
  return r;
}
