{
  if (!eventAdded) {
    String header=(getHeaders() == null) ? null : getHeaders().getValues(SipConstants.HEADER_EVENT);
    if (header != null) {
      try {
        setEvent(new EventReader(header).readValue());
      }
 catch (      Exception e) {
        Context.getCurrentLogger().info(e.getMessage());
      }
    }
    eventAdded=true;
  }
  return super.getEvent();
}
