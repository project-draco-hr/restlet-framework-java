{
  List<String> irt=super.getInReplyTo();
  if (!inReplyToAdded) {
    String header=(getHeaders() == null) ? null : getHeaders().getValues(SipConstants.HEADER_ALLOW_EVENTS);
    if (header != null) {
      try {
        irt.addAll(new HeaderReader<String>(header).readValues());
      }
 catch (      Exception e) {
        Context.getCurrentLogger().info(e.getMessage());
      }
    }
    inReplyToAdded=true;
  }
  return irt;
}
