{
  boolean result=false;
  if (this.name == null) {
    return target == null;
  }
  if (target instanceof Role) {
    Role r=(Role)target;
    result=this.name.equals(r.getName()) && getChildRoles().size() == r.getChildRoles().size();
    if (result && !getChildRoles().isEmpty()) {
      for (int i=0; result && i < getChildRoles().size(); i++) {
        result=getChildRoles().get(i).equals(r.getChildRoles().get(i));
      }
    }
  }
  return result;
}
