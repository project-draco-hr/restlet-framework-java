{
  boolean result=false;
  if (this.name == null) {
    return arg0 == null;
  }
  if (arg0 instanceof Role) {
    Role r=(Role)arg0;
    result=this.name.equals(r.getName());
    if (!getChildRoles().isEmpty() && getChildRoles().size() == r.getChildRoles().size()) {
      for (int i=0; result && i < getChildRoles().size(); i++) {
        result=getChildRoles().get(i).equals(r.getChildRoles().get(i));
      }
    }
  }
  return result;
}
