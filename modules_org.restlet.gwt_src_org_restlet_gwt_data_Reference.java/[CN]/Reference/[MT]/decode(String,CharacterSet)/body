{
  if (!CharacterSet.UTF_8.equals(characterSet)) {
    throw new IllegalArgumentException("Only UTF-8 URL encoding is supported under GWT");
  }
  String result=null;
  try {
    result=(characterSet == null) ? toDecode : URL.decode(toDecode);
  }
 catch (  NullPointerException npe) {
    System.err.println("Unable to decode the string with the UTF-8 character set.");
  }
  return result;
}
