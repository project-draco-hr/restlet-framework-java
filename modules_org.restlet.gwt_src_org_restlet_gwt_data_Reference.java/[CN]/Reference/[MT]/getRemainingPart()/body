{
  String result=null;
  final String all=toString(true,false);
  if (getBaseRef() != null) {
    final String base=getBaseRef().toString(true,false);
    if ((base != null) && all.startsWith(base)) {
      result=all.substring(base.length());
    }
  }
 else {
    result=all;
  }
  return result;
}
