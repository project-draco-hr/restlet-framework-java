{
  if (hasQuery()) {
    if (hasFragment()) {
      if (queryIndex < fragmentIndex) {
        return this.internalRef.substring(queryIndex + 1,fragmentIndex);
      }
 else {
        return null;
      }
    }
 else {
      return this.internalRef.substring(queryIndex + 1);
    }
  }
 else {
    return null;
  }
}
