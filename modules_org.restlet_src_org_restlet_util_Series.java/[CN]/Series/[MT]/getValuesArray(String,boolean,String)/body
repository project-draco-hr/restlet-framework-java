{
  String[] result=null;
  List<E> params=subList(name,ignoreCase);
  if (params.size() == 0) {
    result=new String[1];
    result[0]=defaultValue;
  }
 else {
    result=new String[params.size()];
    for (int i=0; i < params.size(); i++) {
      result[i]=params.get(i).getValue();
    }
  }
  return result;
}
