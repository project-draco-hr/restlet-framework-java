{
  List<String> segments=this.reference.getRelativeRef().getSegments();
  List<PathSegment> pathSegments=new ArrayList<PathSegment>(segments.size());
  int l=segments.size();
  for (int i=0; i < l; i++)   pathSegments.add(new JaxRsPathSegment(segments.get(i),decode,false,false,i));
  return pathSegments;
}
