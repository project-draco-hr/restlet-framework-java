{
  if (isStopped()) {
    try {
      start();
    }
 catch (    Exception e) {
      getContext().getLogger().log(Level.WARNING,UNABLE_TO_START,e);
      response.setStatus(Status.SERVER_ERROR_INTERNAL);
    }
    if (isStarted()) {
      response.setStatus(Status.SUCCESS_OK);
    }
 else {
      getContext().getLogger().log(Level.WARNING,UNABLE_TO_START);
      response.setStatus(Status.SERVER_ERROR_INTERNAL);
    }
  }
}
