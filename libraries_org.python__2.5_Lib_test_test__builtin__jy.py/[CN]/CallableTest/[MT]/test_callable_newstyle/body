def test_callable_newstyle(self):


    class Foo(object):
        pass
    self.assert_(callable(Foo))
    self.assert_((not callable(Foo())))


    class Bar(object):

        def __call__(self):
            return None
    self.assert_(callable(Bar()))


    class Baz(object):

        def __getattr__(self, name):
            return None
    self.assert_((not callable(Baz())))
