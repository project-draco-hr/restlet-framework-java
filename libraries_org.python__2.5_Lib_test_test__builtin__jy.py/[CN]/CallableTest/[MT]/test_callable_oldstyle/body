def test_callable_oldstyle(self):


    class Foo:
        pass
    self.assert_(callable(Foo))
    self.assert_((not callable(Foo())))


    class Bar:

        def __call__(self):
            return None
    self.assert_(callable(Bar()))


    class Baz:

        def __getattr__(self, name):
            return None
    self.assert_(callable(Baz()))
