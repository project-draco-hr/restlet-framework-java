def test_basic(self):
    open_url = urllib.urlopen('http://www.python.org/')
    for attr in ('read', 'readline', 'readlines', 'fileno', 'close', 'info', 'geturl'):
        self.assert_(hasattr(open_url, attr), ('object returned from urlopen lacks the %s attribute' % attr))
    try:
        self.assert_(open_url.read(), "calling 'read' failed")
    finally:
        open_url.close()
