def test_api_2(localedir, mofile):
    print 'test api 2'
    gettext.bindtextdomain('gettext', localedir)
    print (gettext.bindtextdomain('gettext') == localedir)
    gettext.textdomain('gettext')
    print gettext.textdomain()
    _ = gettext.gettext
    print _('albatross')
    print _(u'mullusk')
    print _('Raymond Luxury Yach-t')
    print _(u'nudge nudge')
    print _('albatross')
    print _(u'mullusk')
    print _('Raymond Luxury Yach-t')
    print _(u'nudge nudge')
    print _('albatross')
    print _(u'mullusk')
    print _('Raymond Luxury Yach-t')
    print _(u'nudge nudge')
    print _('albatross')
    print _(u'mullusk')
    print _('Raymond Luxury Yach-t')
    print _(u'nudge nudge')
    print _('This module provides internationalization and localization\nsupport for your Python programs by providing an interface to the GNU\ngettext message catalog library.')

    def _(message):
        return gettext.dgettext('gettext')
