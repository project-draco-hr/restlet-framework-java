{
  ChallengeResponse result=super.getChallengeResponse();
  if (!this.securityAdded) {
    final String authorization=getHttpCall().getRequestHeaders().getValues(HttpConstants.HEADER_AUTHORIZATION);
    result=AuthenticationUtils.parseAuthorizationHeader(this,authorization);
    setChallengeResponse(result);
    this.securityAdded=true;
  }
  return result;
}
