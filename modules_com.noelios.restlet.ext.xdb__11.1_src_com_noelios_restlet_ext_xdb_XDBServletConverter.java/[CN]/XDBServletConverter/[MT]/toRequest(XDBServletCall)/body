{
  HttpRequest result=super.toRequest(servletCall);
  String attributeName;
  for (Enumeration<String> namesEnum=servletCall.getRequest().getAttributeNames(); namesEnum.hasMoreElements(); ) {
    attributeName=namesEnum.nextElement();
    result.getAttributes().put(attributeName,servletCall.getRequest().getAttribute(attributeName));
  }
  return result;
}
