def rjust(s, width):
    'rjust(s, width) -> string\n\n    Return a right-justified version of s, in a field of the\n    specified width, padded with spaces as needed.  The string is\n    never truncated.\n\n    '
    n = (width - len(s))
    if (n <= 0):
        return s
    return ((' ' * n) + s)
