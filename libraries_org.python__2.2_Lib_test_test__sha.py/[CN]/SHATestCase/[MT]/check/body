def check(self, data, digest):
    obj = sha.new(data)
    computed = obj.hexdigest()
    self.assert_((computed == digest))
    computed_again = obj.hexdigest()
    self.assert_((computed == computed_again))
    digest = obj.digest()
    hexd = ''
    for c in digest:
        hexd += ('%02x' % ord(c))
    self.assert_((computed == hexd))
