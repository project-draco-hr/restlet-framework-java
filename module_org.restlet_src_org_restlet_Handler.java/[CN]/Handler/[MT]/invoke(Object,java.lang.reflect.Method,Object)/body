{
  Object result=null;
  if (method != null) {
    try {
      result=method.invoke(target,args);
    }
 catch (    IllegalArgumentException e) {
      getLogger().log(Level.WARNING,"Couldn't invoke the handle method for \"" + method + "\"",e);
    }
catch (    IllegalAccessException e) {
      getLogger().log(Level.WARNING,"Couldn't access the handle method for \"" + method + "\"",e);
    }
catch (    InvocationTargetException e) {
      getLogger().log(Level.WARNING,"Couldn't invoke the handle method for \"" + method + "\"",e);
    }
  }
  return result;
}
