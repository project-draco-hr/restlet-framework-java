def test_b32decode(self):
    eq = self.assertEqual
    eq(base64.b32decode(''), '')
    eq(base64.b32decode('AA======'), '\x00')
    eq(base64.b32decode('ME======'), 'a')
    eq(base64.b32decode('MFRA===='), 'ab')
    eq(base64.b32decode('MFRGG==='), 'abc')
    eq(base64.b32decode('MFRGGZA='), 'abcd')
    eq(base64.b32decode('MFRGGZDF'), 'abcde')
