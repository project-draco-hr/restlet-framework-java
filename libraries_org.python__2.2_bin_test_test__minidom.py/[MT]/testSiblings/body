def testSiblings():
    doc = parseString('<doc><?pi?>text?<elm/></doc>')
    root = doc.documentElement
    (pi, text, elm) = root.childNodes
    confirm(((pi.nextSibling is text) and (pi.previousSibling is None) and (text.nextSibling is elm) and (text.previousSibling is pi) and (elm.nextSibling is None) and (elm.previousSibling is text)), 'testSiblings')
    doc.unlink()
