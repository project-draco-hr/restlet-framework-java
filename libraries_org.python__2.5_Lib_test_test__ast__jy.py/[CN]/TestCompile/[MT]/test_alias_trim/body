def test_alias_trim(self):
    node = srcExprToTree('import os. path')
    self.assertEquals(node.body[0].names[0].name, 'os.path')
    node = srcExprToTree('import os .path')
    self.assertEquals(node.body[0].names[0].name, 'os.path')
    node = srcExprToTree('import os . path')
    self.assertEquals(node.body[0].names[0].name, 'os.path')
