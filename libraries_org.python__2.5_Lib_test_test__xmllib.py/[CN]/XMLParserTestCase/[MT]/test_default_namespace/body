def test_default_namespace(self):


    class H(xmllib.XMLParser):

        def unknown_starttag(self, name, attr):
            (self.name, self.attr) = (name, attr)
    h = H()
    h.feed(nsdoc)
    h.close()
    self.assertEquals(h.name, 'URI foo')
    self.assertEquals(h.attr, {'attr': 'val', })
