def __calc_timezone(self):
    try:
        time.tzset()
    except AttributeError:
        pass
    no_saving = frozenset(['utc', 'gmt', time.tzname[0].lower()])
    if time.daylight:
        has_saving = frozenset([time.tzname[1].lower()])
    else:
        has_saving = frozenset()
    self.timezone = (no_saving, has_saving)
