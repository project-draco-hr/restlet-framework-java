{
  Series<Cookie> result=super.getCookies();
  if (!this.cookiesAdded) {
    if (getHeaders() != null) {
      for (      String cookieValue : getHeaders().getValuesArray(HeaderConstants.HEADER_COOKIE)) {
        new CookieReader(cookieValue).addValues(result);
      }
    }
    this.cookiesAdded=true;
  }
  return result;
}
