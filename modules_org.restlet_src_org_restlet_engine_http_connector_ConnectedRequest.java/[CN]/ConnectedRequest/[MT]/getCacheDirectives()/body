{
  List<CacheDirective> result=super.getCacheDirectives();
  if (!this.cacheDirectivesAdded) {
    if (getHeaders() != null) {
      for (      String directive : getHeaders().getValuesArray(HeaderConstants.HEADER_CACHE_CONTROL)) {
        new CacheDirectiveReader(directive).addValues(result);
      }
    }
    this.cacheDirectivesAdded=true;
  }
  return result;
}
