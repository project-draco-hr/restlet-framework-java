{
  if (name.startsWith("W")) {
    this.weak=true;
    name=name.substring(1);
  }
 else {
    this.weak=false;
  }
  if (name.startsWith("\"") && name.endsWith("\"")) {
    this.tag=name.substring(1,name.length() - 1);
  }
 else   if (name.equals("*")) {
    this.tag="*";
  }
 else {
    throw new IllegalArgumentException("Invalid tag format detected: " + name);
  }
}
