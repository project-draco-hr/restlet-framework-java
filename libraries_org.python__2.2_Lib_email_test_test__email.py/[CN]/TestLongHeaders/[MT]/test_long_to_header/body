def test_long_to_header(self):
    eq = self.ndiffAssertEqual
    to = '"Someone Test #A" <someone@eecs.umich.edu>,<someone@eecs.umich.edu>,"Someone Test #B" <someone@umich.edu>, "Someone Test #C" <someone@eecs.umich.edu>, "Someone Test #D" <someone@eecs.umich.edu>'
    msg = Message()
    msg['To'] = to
    eq(msg.as_string(0), 'To: "Someone Test #A" <someone@eecs.umich.edu>, <someone@eecs.umich.edu>,\n\t"Someone Test #B" <someone@umich.edu>,\n\t"Someone Test #C" <someone@eecs.umich.edu>,\n\t"Someone Test #D" <someone@eecs.umich.edu>\n\n')
