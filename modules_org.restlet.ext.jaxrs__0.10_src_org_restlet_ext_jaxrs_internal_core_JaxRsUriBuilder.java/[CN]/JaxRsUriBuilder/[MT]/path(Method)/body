{
  if (method == null) {
    return this;
  }
  String validSegment;
  try {
    validSegment=Util.getPathTemplateWithoutRegExps(method);
  }
 catch (  final MissingAnnotationException e) {
    throw new IllegalArgumentException(e);
  }
catch (  final IllegalPathException e) {
    throw e.getCause();
  }
  addValidPathSegment(validSegment);
  return this;
}
