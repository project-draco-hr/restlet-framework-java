{
  List<String> output=new ArrayList<String>();
  if (properties != null) {
    for (    String key : properties.keySet()) {
      Property prop=properties.get(key);
      if (prop != null && prop.getRequired())       output.add(key);
    }
  }
  if (output.size() > 0)   return output;
 else   return null;
}
