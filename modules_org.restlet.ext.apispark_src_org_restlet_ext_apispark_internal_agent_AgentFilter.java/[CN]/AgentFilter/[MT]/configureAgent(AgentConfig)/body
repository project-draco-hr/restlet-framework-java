{
  agentConfig.validate();
  ModulesSettingsModule modulesSettingsModule=new ModulesSettingsModule(agentConfig);
  ModulesSettings modulesSettings=modulesSettingsModule.getModulesSettings();
  RestletChain agentRestletChain=AgentModulesHelper.buildFromSettings(agentConfig,modulesSettings,getContext());
  agentFirstRestlet=agentRestletChain.getFirst();
  agentLastRestlet=agentRestletChain.getLast();
}
