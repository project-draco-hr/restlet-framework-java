def __str__(self):
    if (self.port is None):
        p = ''
    else:
        p = (':' + self.port)
    limit = ((self.domain + p) + self.path)
    if (self.value is not None):
        namevalue = ('%s=%s' % (self.name, self.value))
    else:
        namevalue = self.name
    return ('<Cookie %s for %s>' % (namevalue, limit))
