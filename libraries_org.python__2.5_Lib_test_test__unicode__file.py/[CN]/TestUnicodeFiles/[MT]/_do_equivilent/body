def _do_equivilent(self, filename1, filename2):
    self.failUnless((type(filename1) != type(filename2)), 'No point checking equivalent filenames of the same type')
    self.failUnlessEqual(os.stat(filename1), os.stat(filename2))
    self.failUnlessEqual(os.lstat(filename1), os.lstat(filename2))
    self._do_copyish(filename1, filename2)
