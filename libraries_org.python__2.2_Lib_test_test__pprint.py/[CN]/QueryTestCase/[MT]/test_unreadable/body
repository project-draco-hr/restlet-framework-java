def test_unreadable(self):
    'Not recursive but not readable anyway.'
    verify = self.assert_
    for unreadable in (type(3), pprint, pprint.isrecursive):
        verify((not pprint.isrecursive(unreadable)), ('expected not isrecursive for ' + `unreadable`))
        verify((not pprint.isreadable(unreadable)), ('expected not isreadable for ' + `unreadable`))
