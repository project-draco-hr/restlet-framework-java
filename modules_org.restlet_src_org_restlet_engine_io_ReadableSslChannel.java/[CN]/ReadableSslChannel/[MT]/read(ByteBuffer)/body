{
  int result=0;
  int remaining=getPacketBuffer().remaining();
  if (remaining == 0) {
    getPacketBuffer().clear();
    remaining=getWrappedChannel().read(getPacketBuffer());
  }
  if (remaining > 0) {
    getManager().getEngine().unwrap(getPacketBuffer(),dst);
    result=remaining - getPacketBuffer().remaining();
  }
  return result;
}
