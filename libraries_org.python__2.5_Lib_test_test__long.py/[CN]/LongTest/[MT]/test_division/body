def test_division(self):
    digits = (range(1, (MAXDIGITS + 1)) + range(KARATSUBA_CUTOFF, (KARATSUBA_CUTOFF + 14)))
    digits.append((KARATSUBA_CUTOFF * 3))
    for lenx in digits:
        x = self.getran(lenx)
        for leny in digits:
            y = (self.getran(leny) or 1L)
            self.check_division(x, y)
