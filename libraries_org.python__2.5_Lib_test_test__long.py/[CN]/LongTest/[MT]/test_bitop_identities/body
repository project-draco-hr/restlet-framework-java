def test_bitop_identities(self):
    for x in special:
        self.check_bitop_identities_1(x)
    digits = xrange(1, (MAXDIGITS + 1))
    for lenx in digits:
        x = self.getran(lenx)
        self.check_bitop_identities_1(x)
        for leny in digits:
            y = self.getran(leny)
            self.check_bitop_identities_2(x, y)
            self.check_bitop_identities_3(x, y, self.getran(((lenx + leny) // 2)))
