{
  try {
    float result=Float.valueOf(quality);
    if (isQuality(result)) {
      return result;
    }
 else {
      throw new IllegalArgumentException("Invalid quality value detected. Value must be between 0 and 1.");
    }
  }
 catch (  NumberFormatException nfe) {
    throw new IllegalArgumentException("Invalid quality value detected. Value must be between 0 and 1.");
  }
}
