{
  List<MessageBodyWriter<?>> mbws=new ArrayList<MessageBodyWriter<?>>();
  for (  MessageBodyWriter<?> mbw : this.mbws) {
    if (mbw.supportsWrite(determinedResponseMediaType))     mbws.add(mbw);
  }
  for (  Iterable<MediaType> amts : accMediaTypes.listOfColls())   for (  MessageBodyWriter<?> mbw : mbws)   if (mbw.supportsWrite(amts))   return mbw;
  return null;
}
