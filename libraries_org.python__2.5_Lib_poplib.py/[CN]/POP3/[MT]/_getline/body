def _getline(self):
    line = self.file.readline()
    if (self._debugging > 1):
        print '*get*', repr(line)
    if (not line):
        raise error_proto('-ERR EOF')
    octets = len(line)
    if (line[(-2):] == CRLF):
        return (line[:(-2)], octets)
    if (line[0] == CR):
        return (line[1:(-1)], octets)
    return (line[:(-1)], octets)
