{
  if (this.clientName == null) {
    Parameter header;
    for (Iterator<Parameter> iter=getRequestHeaders().iterator(); iter.hasNext(); ) {
      header=iter.next();
      if (header.getName().equalsIgnoreCase(HEADER_USER_AGENT)) {
        this.clientName=header.getValue();
      }
    }
  }
  return this.clientName;
}
