{
  this.path=path;
  this.unmodifiable=unmodifiable;
  if (unmodifiable && !(matrixParameters instanceof UnmodifiableMultivaluedMap))   this.matrixParameters=new UnmodifiableMultivaluedMap<String,String>(matrixParameters,false);
  this.matrixParameters=matrixParameters;
  this.matrParamEncoded=null;
}
