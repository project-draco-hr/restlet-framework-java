{
  if (this.subTags == null) {
    this.subTags=new ArrayList<String>();
    if (getName() != null) {
      String[] tags=getName().split("-");
      if (tags.length > 0) {
        for (int i=1; i < tags.length; i++) {
          this.subTags.add(tags[i]);
        }
      }
    }
  }
  return this.subTags;
}
