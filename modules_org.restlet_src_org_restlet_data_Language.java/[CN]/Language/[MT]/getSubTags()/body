{
  List<String> v=this.subTags;
  if (v == null) {
synchronized (this) {
      v=this.subTags;
      if (v == null) {
        this.subTags=v=new CopyOnWriteArrayList<String>();
        if (getName() != null) {
          String[] tags=getName().split("-");
          if (tags.length > 0) {
            for (int i=1; i < tags.length; i++) {
              this.subTags.add(tags[i]);
            }
          }
        }
      }
    }
  }
  return v;
}
