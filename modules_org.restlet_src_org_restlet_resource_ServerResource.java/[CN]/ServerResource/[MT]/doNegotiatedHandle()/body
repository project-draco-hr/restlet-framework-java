{
  Representation result=null;
  List<Variant> variants=getAvailableVariants(getMethod());
  if ((variants != null) && (!variants.isEmpty())) {
    Variant preferredVariant=getClientInfo().getPreferredVariant(variants,(getApplication() == null) ? new MetadataService() : getApplication().getMetadataService());
    if (preferredVariant == null) {
      setStatus(Status.CLIENT_ERROR_NOT_ACCEPTABLE);
      result=describeVariants();
    }
 else {
      updateDimensions();
      if (preferredVariant instanceof VariantInfo) {
        result=doHandle(((VariantInfo)preferredVariant).getAnnotationInfo(),preferredVariant);
      }
 else {
        result=doHandle(preferredVariant);
      }
    }
  }
 else {
    result=doHandle();
  }
  return result;
}
