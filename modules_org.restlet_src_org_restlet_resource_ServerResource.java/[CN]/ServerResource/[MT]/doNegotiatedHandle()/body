{
  Representation result=null;
  if ((getVariants() != null) && (!getVariants().isEmpty())) {
    Variant preferredVariant=getClientInfo().getPreferredVariant(getVariants(),getMetadataService());
    if (preferredVariant == null) {
      doError(Status.CLIENT_ERROR_NOT_ACCEPTABLE);
      result=describeVariants();
    }
 else {
      updateDimensions();
      result=doHandle(preferredVariant);
    }
  }
 else {
    result=doHandle();
  }
  return result;
}
