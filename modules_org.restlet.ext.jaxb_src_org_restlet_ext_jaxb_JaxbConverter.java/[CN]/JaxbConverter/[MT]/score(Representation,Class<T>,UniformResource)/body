{
  float result=-1.0F;
  if (source != null) {
    if (source instanceof JaxbRepresentation<?>) {
      result=1.0F;
    }
 else     if (JaxbRepresentation.class.isAssignableFrom(target)) {
      result=1.0F;
    }
 else     if (isJaxbRootElementClass(target) || JaxbRepresentation.class.isAssignableFrom(source.getClass())) {
      result=1.0F;
    }
  }
  return result;
}
