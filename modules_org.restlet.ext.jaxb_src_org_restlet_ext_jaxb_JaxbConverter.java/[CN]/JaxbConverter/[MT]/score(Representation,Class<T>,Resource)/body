{
  float result=-1.0F;
  if (source != null) {
    if (source instanceof JaxbRepresentation<?>) {
      result=1.0F;
    }
 else     if (JaxbRepresentation.class.isAssignableFrom(source.getClass())) {
      result=1.0F;
    }
 else     if (isJaxbRootElementClass(target) || JaxbRepresentation.class.isAssignableFrom(target)) {
      if (MediaType.APPLICATION_ALL_XML.isCompatible(source.getMediaType())) {
        result=1.0F;
      }
 else       if (MediaType.APPLICATION_XML.isCompatible(source.getMediaType())) {
        result=1.0F;
      }
 else       if (MediaType.TEXT_XML.isCompatible(source.getMediaType())) {
        result=1.0F;
      }
 else {
        result=0.7F;
      }
    }
  }
  return result;
}
