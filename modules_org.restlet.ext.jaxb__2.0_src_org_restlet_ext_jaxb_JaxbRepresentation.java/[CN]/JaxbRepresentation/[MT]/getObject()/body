{
  if (this.object == null) {
    try {
      Unmarshaller u=new Unmarshaller(contextPath);
      if (getValidationEventHandler() != null) {
        u.setEventHandler(getValidationEventHandler());
      }
      this.object=u.unmarshal(xmlRepresentation.getStream());
    }
 catch (    Exception e) {
      logger.log(Level.WARNING,"Error while unmarshalling the XML representation with JAXB",e);
    }
  }
  return this.object;
}
