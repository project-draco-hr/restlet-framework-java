{
  try {
    this.connection.flushResponse();
  }
 catch (  IOException ex) {
    getLogger().log(Level.FINE,"Unable to flush the response",ex);
  }
  try {
    this.connection.completeResponse();
  }
 catch (  IOException ex) {
    getLogger().log(Level.FINE,"Unable to complete the response",ex);
  }
}
