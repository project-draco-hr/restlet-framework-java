{
  HandshakeStatus hs=getSslHandshakeStatus();
  if (hs != HandshakeStatus.NOT_HANDSHAKING) {
switch (getSslHandshakeStatus()) {
case FINISHED:
      onFinished();
    break;
case NEED_TASK:
  onNeedTask();
break;
case NEED_UNWRAP:
onUnwrap();
break;
case NEED_WRAP:
onWrap();
break;
case NOT_HANDSHAKING:
break;
}
}
}
