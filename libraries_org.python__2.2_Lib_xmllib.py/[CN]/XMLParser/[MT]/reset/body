def reset(self):
    self.rawdata = ''
    self.stack = []
    self.nomoretags = 0
    self.literal = 0
    self.lineno = 1
    self.__at_start = 1
    self.__seen_doctype = None
    self.__seen_starttag = 0
    self.__use_namespaces = 0
    self.__namespaces = {'xml': None, }
    if (self.elements is XMLParser.elements):
        self.__fixelements()
