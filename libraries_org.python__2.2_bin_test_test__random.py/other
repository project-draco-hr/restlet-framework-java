import test_support
import random
for seed in (1, 12, 123, 1234, 12345, 123456, 654321):
    for seeder in (random.seed, random.whseed):
        seeder(seed)
        x1 = random.random()
        y1 = random.gauss(0, 1)
        seeder(seed)
        x2 = random.random()
        y2 = random.gauss(0, 1)
        test_support.vereq(x1, x2)
        test_support.vereq(y1, y2)
