{
  try {
    if (!isStarted())     start();
    HttpClientCall httpCall=getConverter().toSpecific(this,request,response);
    getConverter().commit(httpCall,request,response);
  }
 catch (  Exception e) {
    logger.log(Level.WARNING,"Error while handling an HTTP client call: ",e.getMessage());
    logger.log(Level.INFO,"Error while handling an HTTP client call",e);
  }
}
