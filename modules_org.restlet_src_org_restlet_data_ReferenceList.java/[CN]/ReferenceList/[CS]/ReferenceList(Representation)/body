{
  BufferedReader br=null;
  try {
    br=new BufferedReader(uriList.getReader(),IoUtils.getBufferSize());
    String line=br.readLine();
    if ((line != null) && line.startsWith("#")) {
      setIdentifier(new Reference(line.substring(1).trim()));
      line=br.readLine();
    }
    while (line != null) {
      if (!line.startsWith("#")) {
        add(new Reference(line.trim()));
      }
      line=br.readLine();
    }
  }
  finally {
    if (br != null) {
      br.close();
    }
  }
}
