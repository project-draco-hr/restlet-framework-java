{
  super(null,new Buffer(connection.getPacketBufferSize(),connection.getHelper().isDirectBuffers()),source);
  if (Context.getCurrentLogger().isLoggable(Level.FINER)) {
    Context.getCurrentLogger().log(Level.FINER,"ReadableSslChannel created from: " + source + ". Registration: "+ getRegistration());
  }
  this.connection=connection;
}
