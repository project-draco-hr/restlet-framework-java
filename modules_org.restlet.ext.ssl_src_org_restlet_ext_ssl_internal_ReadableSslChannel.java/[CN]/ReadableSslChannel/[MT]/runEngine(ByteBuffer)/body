{
  if (getConnection().getLogger().isLoggable(Level.INFO)) {
    getConnection().getLogger().log(Level.INFO,"Unwrapping bytes with: " + getPacketBuffer());
  }
  SSLEngineResult result=getManager().getEngine().unwrap(getPacketBuffer(),applicationBuffer);
  int remaining=getPacketBuffer().remaining();
  if (remaining == 0) {
    setPacketBufferState(BufferState.FILLING);
    getPacketBuffer().clear();
  }
  return result;
}
