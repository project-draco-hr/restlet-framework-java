def __init__(self, examples, globs, name, filename, lineno, docstring):
    "\n        Create a new DocTest containing the given examples.  The\n        DocTest's globals are initialized with a copy of `globs`.\n        "
    assert (not isinstance(examples, basestring)), 'DocTest no longer accepts str; use DocTestParser instead'
    self.examples = examples
    self.docstring = docstring
    self.globs = globs.copy()
    self.name = name
    self.filename = filename
    self.lineno = lineno
