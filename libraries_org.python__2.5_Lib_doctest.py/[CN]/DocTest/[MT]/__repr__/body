def __repr__(self):
    if (len(self.examples) == 0):
        examples = 'no examples'
    elif (len(self.examples) == 1):
        examples = '1 example'
    else:
        examples = ('%d examples' % len(self.examples))
    return ('<DocTest %s from %s:%s (%s)>' % (self.name, self.filename, self.lineno, examples))
