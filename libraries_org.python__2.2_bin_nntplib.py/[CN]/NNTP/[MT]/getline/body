def getline(self):
    'Internal: return one line from the server, stripping CRLF.\n        Raise EOFError if the connection is closed.'
    line = self.file.readline()
    if (self.debugging > 1):
        print '*get*', `line`
    if (not line):
        raise EOFError
    if (line[(-2):] == CRLF):
        line = line[:(-2)]
    elif (line[(-1):] in CRLF):
        line = line[:(-1)]
    return line
