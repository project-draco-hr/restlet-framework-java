def _test():
    'Minimal test function.'
    s = NNTP('news', readermode='reader')
    (resp, count, first, last, name) = s.group('comp.lang.python')
    print resp
    print 'Group', name, 'has', count, 'articles, range', first, 'to', last
    (resp, subs) = s.xhdr('subject', ((first + '-') + last))
    print resp
    for item in subs:
        print ('%7s %s' % item)
    resp = s.quit()
    print resp
