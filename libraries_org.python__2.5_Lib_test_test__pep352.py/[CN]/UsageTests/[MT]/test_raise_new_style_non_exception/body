def test_raise_new_style_non_exception(self):


    class NewStyleClass(object):
        pass
    try:
        raise NewStyleClass
    except TypeError:
        pass
    except:
        self.fail('unable to raise new-style class')
    try:
        raise NewStyleClass()
    except TypeError:
        pass
    except:
        self.fail('unable to raise new-style class instance')
