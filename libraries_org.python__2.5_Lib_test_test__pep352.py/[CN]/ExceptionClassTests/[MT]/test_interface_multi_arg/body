def test_interface_multi_arg(self):
    arg_count = 3
    args = tuple(range(arg_count))
    exc = Exception(*args)
    results = ([len(exc.args), arg_count], [exc.args, args], [exc.message, ''], [str(exc), str(args)], [unicode(exc), unicode(args)], [repr(exc), (exc.__class__.__name__ + repr(exc.args))], [exc[(-1)], args[(-1)]])
    self.interface_test_driver(results)
