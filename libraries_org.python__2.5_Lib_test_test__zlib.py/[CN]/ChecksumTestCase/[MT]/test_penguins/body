def test_penguins(self):
    self.assertEqual32(zlib.crc32('penguin', 0), 3854672160L)
    self.assertEqual32(zlib.crc32('penguin', 1), 1136044692)
    if (not test_support.is_jython):
        self.assertEqual32(zlib.adler32('penguin', 0), 198116086)
    self.assertEqual32(zlib.adler32('penguin', 1), 198574839)
    self.assertEqual(zlib.crc32('penguin'), zlib.crc32('penguin', 0))
    self.assertEqual(zlib.adler32('penguin'), zlib.adler32('penguin', 1))
