{
  String result=null;
switch (authority) {
case CLAP_DEFAULT:
    result="";
  break;
case CLAP_CLASS:
result="class";
break;
case CLAP_SYSTEM:
result="system";
break;
case CLAP_THREAD:
result="thread";
break;
case RIAP_APPLICATION:
result="application";
break;
case RIAP_COMPONENT:
result="component";
break;
case RIAP_HOST:
result="host";
break;
}
return result;
}
