{
  Reference result=null;
  if (getSchemeProtocol().equals(Protocol.JAR)) {
    final String ssp=getSchemeSpecificPart();
    if (ssp != null) {
      final int separatorIndex=ssp.indexOf("!/");
      if (separatorIndex != -1) {
        result=new Reference(ssp.substring(0,separatorIndex));
      }
    }
  }
  return result;
}
