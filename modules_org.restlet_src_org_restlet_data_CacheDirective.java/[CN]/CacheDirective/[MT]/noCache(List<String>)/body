{
  StringBuilder sb=new StringBuilder();
  if (fieldNames != null) {
    for (int i=0; i < fieldNames.size(); i++) {
      sb.append("\"" + fieldNames.get(i) + "\"");
      if (i < fieldNames.size() - 1) {
        sb.append(',');
      }
    }
  }
  return new CacheDirective(HttpConstants.CACHE_NO_CACHE,sb.toString());
}
