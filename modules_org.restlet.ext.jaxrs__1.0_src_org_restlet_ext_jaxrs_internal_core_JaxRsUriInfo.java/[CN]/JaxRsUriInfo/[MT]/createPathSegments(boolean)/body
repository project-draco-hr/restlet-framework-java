{
  List<String> segments=this.reference.getRelativeRef().getSegments();
  int l=segments.size();
  List<PathSegment> pathSegments=new ArrayList<PathSegment>(l);
  for (int i=0; i < l; i++)   pathSegments.add(new JaxRsPathSegment(segments.get(i),true,decode,false,false,i));
  return Collections.unmodifiableList(pathSegments);
}
