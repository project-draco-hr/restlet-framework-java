{
  List<Variant> variants;
  for (  ConverterHelper ch : Engine.getInstance().getRegisteredConverters()) {
    variants=ch.getVariants(object.getClass());
    if (variants != null) {
      for (      Variant variant : variants) {
        if (variant.isCompatible(targetVariant)) {
          return ch;
        }
      }
    }
  }
  return null;
}
