{
  List<Class<?>> classes;
  for (  ConverterHelper ch : Engine.getInstance().getRegisteredConverters()) {
    classes=ch.getObjectClasses(sourceVariant);
    if (classes != null) {
      if (targetClass != null) {
        for (        Class<?> clazz : classes) {
          if (clazz.isAssignableFrom(targetClass)) {
            return ch;
          }
        }
      }
 else {
        return ch;
      }
    }
  }
  return null;
}
