def test_parse_ns_headers(self):
    from cookielib import parse_ns_headers
    self.assertEquals(parse_ns_headers(['foo=bar; path=/; domain']), [[('foo', 'bar'), ('path', '/'), ('domain', None), ('version', '0')]])
    self.assertEquals(parse_ns_headers(['foo=bar; expires=Foo Bar 12 33:22:11 2000']), [[('foo', 'bar'), ('expires', None), ('version', '0')]])
    self.assertEquals(parse_ns_headers(['foo']), [[('foo', None), ('version', '0')]])
    self.assertEquals(parse_ns_headers(['']), [])
