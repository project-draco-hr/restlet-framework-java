def test_large_update(self):
    aas = ('a' * 128)
    bees = ('b' * 127)
    cees = ('c' * 126)
    for name in self.supported_hash_names:
        m1 = hashlib.new(name)
        m1.update(aas)
        m1.update(bees)
        m1.update(cees)
        m2 = hashlib.new(name)
        m2.update(((aas + bees) + cees))
        self.assertEqual(m1.digest(), m2.digest())
