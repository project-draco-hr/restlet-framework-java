{
  if (useTcp) {
    if ((serverWrapperFactory != null) && !serverWrapperFactory.usesTcp()) {
      serverWrapperFactory=null;
    }
  }
 else {
    if ((serverWrapperFactory != null) && serverWrapperFactory.usesTcp()) {
      serverWrapperFactory=null;
    }
  }
  RestletServerTestCase.useTcp=useTcp;
}
