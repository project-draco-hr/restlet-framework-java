{
  T result=null;
  org.restlet.service.ConverterService cs=getConverterService();
  List<? extends Variant> variants=cs.getVariants(resultClass,null);
  ClientInfo clientInfo=getClientInfo();
  if (clientInfo.getAcceptedMediaTypes().isEmpty()) {
    cs.updatePreferences(clientInfo.getAcceptedMediaTypes(),resultClass);
  }
  result=toObject(handle(method,(entity == null) ? null : toRepresentation(entity,getConnegService().getPreferredVariant(variants,clientInfo,getMetadataService())),clientInfo),resultClass);
  return result;
}
