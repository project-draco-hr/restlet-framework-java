{
  if (call.getStatus() == null) {
    call.setStatus(Status.SUCCESS_OK);
  }
  if (!call.getStatus().equals(Status.SUCCESS_OK) && !call.getStatus().equals(Status.REDIRECTION_NOT_MODIFIED) && ((call.getOutput() == null) || overwrite)) {
    call.setOutput(getRepresentation(call.getStatus(),call));
  }
}
