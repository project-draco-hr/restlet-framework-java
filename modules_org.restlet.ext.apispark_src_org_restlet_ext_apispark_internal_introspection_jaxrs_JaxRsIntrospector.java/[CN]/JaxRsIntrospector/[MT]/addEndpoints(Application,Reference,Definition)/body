{
  ApplicationPath ap=application.getClass().getAnnotation(ApplicationPath.class);
  if (ap != null) {
    Endpoint endpoint=new Endpoint(ap.value());
    definition.getEndpoints().add(endpoint);
  }
  if (baseRef != null) {
    Endpoint endpoint=new Endpoint(baseRef.getHostDomain(),baseRef.getHostPort(),baseRef.getSchemeProtocol().getSchemeName(),baseRef.getPath(),null);
    definition.getEndpoints().add(endpoint);
  }
}
