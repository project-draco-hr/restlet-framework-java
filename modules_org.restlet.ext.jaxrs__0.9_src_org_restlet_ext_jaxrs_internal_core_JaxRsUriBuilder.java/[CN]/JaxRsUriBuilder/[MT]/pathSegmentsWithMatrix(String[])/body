{
  if (segments == null)   return new ArrayList<CharSequence>(0);
  int length=segments.length;
  List<CharSequence> r=new ArrayList<CharSequence>(length);
  if (length == 0)   return r;
  String s=segments[0];
  if (s == null || s.length() == 0)   return r;
  r.add(EncodeOrCheck.pathSegmentWithMatrix(s,encode));
  for (int i=1; i < length; i++)   r.add(EncodeOrCheck.pathSegmentWithMatrix(segments[i],encode));
  return r;
}
