{
  String result=null;
  try {
    result=URLDecoder.decode(toDecode,"UTF-8");
  }
 catch (  final UnsupportedEncodingException uee) {
    Logger.getLogger(Reference.class.getCanonicalName()).log(Level.WARNING,"Unable to decode the string with the UTF-8 character set.",uee);
  }
  return result;
}
