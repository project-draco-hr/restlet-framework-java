{
  if (query) {
    if (fragment) {
      return this.internalRef;
    }
    if (hasFragment()) {
      return this.internalRef.substring(0,fragmentIndex);
    }
    return this.internalRef;
  }
  if (fragment) {
    if (hasQuery()) {
      if (hasFragment()) {
        return this.internalRef.substring(0,queryIndex) + "#" + getFragment();
      }
      return this.internalRef.substring(0,queryIndex);
    }
    return this.internalRef;
  }
  if (hasQuery()) {
    return this.internalRef.substring(0,queryIndex);
  }
  if (hasFragment()) {
    return this.internalRef.substring(0,fragmentIndex);
  }
  return this.internalRef;
}
