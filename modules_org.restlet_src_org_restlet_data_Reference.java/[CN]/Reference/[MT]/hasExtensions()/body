{
  boolean result=false;
  String lastSegment=getLastSegment();
  if (lastSegment != null) {
    int extensionsIndex=lastSegment.indexOf('.');
    int matrixIndex=lastSegment.indexOf(';');
    result=(extensionsIndex != -1) && ((matrixIndex == -1) || (extensionsIndex < matrixIndex));
  }
  return result;
}
