{
  String result=null;
  if (toEncode != null) {
    try {
      result=URLEncoder.encode(toEncode,"UTF-8");
    }
 catch (    final UnsupportedEncodingException uee) {
      Context.getCurrentLogger().log(Level.WARNING,"Unable to encode the string with the UTF-8 character set.",uee);
    }
  }
  return result;
}
