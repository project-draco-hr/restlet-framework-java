def testParents():
    doc = parseString('<doc><elm1><elm2/><elm2><elm3/></elm2></elm1></doc>')
    root = doc.documentElement
    elm1 = root.childNodes[0]
    (elm2a, elm2b) = elm1.childNodes
    elm3 = elm2b.childNodes[0]
    confirm(((root.parentNode is doc) and (elm1.parentNode is root) and (elm2a.parentNode is elm1) and (elm2b.parentNode is elm1) and (elm3.parentNode is elm2b)), 'testParents')
    doc.unlink()
