def testNodeListItem():
    doc = parseString('<doc><e/><e/></doc>')
    children = doc.childNodes
    docelem = children[0]
    confirm(((children[0] is children.item(0)) and (children.item(1) is None) and (docelem.childNodes.item(0) is docelem.childNodes[0]) and (docelem.childNodes.item(1) is docelem.childNodes[1]) and (docelem.childNodes.item(0).childNodes.item(0) is None)), 'test NodeList.item()')
    doc.unlink()
