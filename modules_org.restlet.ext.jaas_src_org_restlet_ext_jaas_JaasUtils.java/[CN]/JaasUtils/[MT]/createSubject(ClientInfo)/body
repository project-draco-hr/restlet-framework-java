{
  Subject result=new Subject();
  if (clientInfo != null) {
    if (clientInfo.getUser() != null) {
      result.getPrincipals().add(new UserPrincipal(clientInfo.getUser()));
    }
    for (    Role role : clientInfo.getRoles()) {
      result.getPrincipals().add(new RolePrincipal(role));
    }
  }
  return result;
}
