{
  String result=null;
  if (reader != null) {
    try {
      final StringBuilder sb=new StringBuilder();
      final BufferedReader br=new BufferedReader(reader);
      int nextChar=br.read();
      while (nextChar != -1) {
        sb.append((char)nextChar);
        nextChar=br.read();
      }
      br.close();
      result=sb.toString();
    }
 catch (    final Exception e) {
    }
  }
  return result;
}
