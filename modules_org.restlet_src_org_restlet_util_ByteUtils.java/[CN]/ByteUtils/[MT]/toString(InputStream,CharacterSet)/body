{
  String result=null;
  if (inputStream != null) {
    try {
      if (characterSet != null) {
        result=toString(new InputStreamReader(inputStream,characterSet.getName()));
      }
 else {
        result=toString(new InputStreamReader(inputStream));
      }
    }
 catch (    final Exception e) {
    }
  }
  return result;
}
