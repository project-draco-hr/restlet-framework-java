{
  int result=0;
  Selector selector=null;
  SelectionKey selectionKey=null;
  try {
    if (bb.hasRemaining()) {
      result=bb.get();
    }
 else {
      int bytesRead=readChannel();
      if (bytesRead == 0) {
        selector=SelectorFactory.getSelector();
        if (selector != null) {
          selectionKey=selectableChannel.register(selector,SelectionKey.OP_READ);
          selector.select(10000);
        }
        bytesRead=readChannel();
      }
      if (bytesRead == 0) {
        result=-1;
      }
 else {
        result=bb.get();
      }
    }
  }
  finally {
    if (selectionKey != null) {
      selectionKey.cancel();
      selector.selectNow();
      SelectorFactory.returnSelector(selector);
    }
  }
  if (result == -1) {
    System.out.println();
  }
 else {
    System.out.print((char)result);
  }
  return result;
}
