{
  try {
    nettyContext.channel().config().setAutoRead(true);
    barrier.await(IoUtils.TIMEOUT_MS,TimeUnit.MILLISECONDS);
  }
 catch (  InterruptedException e) {
    e.printStackTrace();
  }
catch (  BrokenBarrierException e) {
    e.printStackTrace();
  }
catch (  TimeoutException e) {
    e.printStackTrace();
  }
}
