{
  if (response.getStatus() == null) {
    response.setStatus(Status.SUCCESS_OK);
  }
  if (!response.getStatus().equals(Status.SUCCESS_OK) && !response.getStatus().equals(Status.REDIRECTION_NOT_MODIFIED) && ((response.getEntity() == null) || overwrite)) {
    response.setEntity(getOutput(response.getStatus(),request,response));
  }
}
