{
  if (response.getStatus() == null) {
    response.setStatus(Status.SUCCESS_OK);
  }
  if (!response.getStatus().equals(Status.SUCCESS_OK) && !response.getStatus().equals(Status.REDIRECTION_NOT_MODIFIED) && ((response.getOutput() == null) || overwrite)) {
    response.setOutput(getRepresentation(response.getStatus(),request,response));
  }
}
