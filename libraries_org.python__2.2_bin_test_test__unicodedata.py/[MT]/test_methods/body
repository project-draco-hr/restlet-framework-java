def test_methods():
    h = sha.sha()
    for i in range(65536):
        char = unichr(i)
        data = [((char.isalnum() and u'1') or u'0'), ((char.isalpha() and u'1') or u'0'), ((char.isdecimal() and u'1') or u'0'), ((char.isdigit() and u'1') or u'0'), ((char.islower() and u'1') or u'0'), ((char.isnumeric() and u'1') or u'0'), ((char.isspace() and u'1') or u'0'), ((char.istitle() and u'1') or u'0'), ((char.isupper() and u'1') or u'0'), (((char + u'abc').isalnum() and u'1') or u'0'), (((char + u'abc').isalpha() and u'1') or u'0'), (((char + u'123').isdecimal() and u'1') or u'0'), (((char + u'123').isdigit() and u'1') or u'0'), (((char + u'abc').islower() and u'1') or u'0'), (((char + u'123').isnumeric() and u'1') or u'0'), (((char + u' \t').isspace() and u'1') or u'0'), (((char + u'abc').istitle() and u'1') or u'0'), (((char + u'ABC').isupper() and u'1') or u'0'), char.lower(), char.upper(), char.title(), (char + u'abc').lower(), (char + u'ABC').upper(), (char + u'abc').title(), (char + u'ABC').title()]
        h.update(u''.join(data).encode(encoding))
    return h.hexdigest()
