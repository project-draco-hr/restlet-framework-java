def test_weak_valued_delitem(self):
    d = weakref.WeakValueDictionary()
    o1 = Object('1')
    o2 = Object('2')
    d['something'] = o1
    d['something else'] = o2
    self.assert_((len(d) == 2))
    del d['something']
    self.assert_((len(d) == 1))
    self.assert_((d.items() == [('something else', o2)]))
