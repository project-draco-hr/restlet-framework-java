def getpwnam(name):
    '\n    getpwnam(name) -> (pw_name,pw_passwd,pw_uid,\n                        pw_gid,pw_gecos,pw_dir,pw_shell)\n    Return the password database entry for the given user name.\n    See pwd.__doc__ for more on password database entries.\n    '
    entry = _posix.getpwnam(name)
    if (not entry):
        raise KeyError(name)
    return struct_passwd(entry)
