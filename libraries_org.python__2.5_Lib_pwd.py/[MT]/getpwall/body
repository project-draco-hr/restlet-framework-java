def getpwall():
    '\n    getpwall() -> list_of_entries\n    Return a list of all available password database entries,\n    in arbitrary order.\n    See pwd.__doc__ for more on password database entries.\n    '
    entries = []
    while True:
        entry = _posix.getpwent()
        if (not entry):
            break
        entries.append(struct_passwd(entry))
    return entries
