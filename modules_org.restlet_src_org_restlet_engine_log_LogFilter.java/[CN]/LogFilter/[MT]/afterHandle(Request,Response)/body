{
  if (this.logLogger.isLoggable(Level.INFO)) {
    if (this.logTemplate != null) {
      this.logLogger.log(Level.INFO,format(request,response));
    }
 else {
      long startTime=(Long)request.getAttributes().get("org.restlet.startTime");
      int duration=(int)(System.currentTimeMillis() - startTime);
      this.logLogger.log(Level.INFO,formatDefault(request,response,duration));
    }
  }
}
