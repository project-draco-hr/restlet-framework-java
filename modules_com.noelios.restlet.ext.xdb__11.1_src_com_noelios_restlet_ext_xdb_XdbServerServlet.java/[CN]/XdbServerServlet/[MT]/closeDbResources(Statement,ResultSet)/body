{
  if (resultSet != null)   try {
    resultSet.close();
  }
 catch (  SQLException s) {
    s.printStackTrace(System.err);
  }
 finally {
    resultSet=null;
  }
  if (statement != null)   try {
    statement.close();
  }
 catch (  SQLException s) {
    s.printStackTrace(System.err);
  }
 finally {
    statement=null;
  }
}
