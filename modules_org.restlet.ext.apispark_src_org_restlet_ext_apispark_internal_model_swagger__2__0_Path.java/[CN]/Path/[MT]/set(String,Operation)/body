{
  if ("get".equals(method))   return get(op);
  if ("put".equals(method))   return put(op);
  if ("post".equals(method))   return post(op);
  if ("delete".equals(method))   return delete(op);
  if ("patch".equals(method))   return patch(op);
  if ("options".equals(method))   return options(op);
  return null;
}
