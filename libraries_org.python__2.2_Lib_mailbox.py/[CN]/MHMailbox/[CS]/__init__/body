def __init__(self, dirname, factory=rfc822.Message):
    import re
    pat = re.compile('^[1-9][0-9]*$')
    self.dirname = dirname
    list = os.listdir(self.dirname)
    list = filter(pat.match, list)
    list = map(long, list)
    list.sort()
    self.boxes = map(str, list)
    self.factory = factory
