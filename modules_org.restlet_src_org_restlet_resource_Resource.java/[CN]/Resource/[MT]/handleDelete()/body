{
  boolean canDelete=true;
  if (getRequest().getConditions().hasSome()) {
    Variant preferredVariant=null;
    if (isNegotiateContent()) {
      preferredVariant=getPreferredVariant();
    }
 else {
      List<Variant> variants=getVariants();
      if (variants.size() == 1) {
        preferredVariant=variants.get(0);
      }
 else {
        getResponse().setStatus(Status.CLIENT_ERROR_PRECONDITION_FAILED);
        canDelete=false;
      }
    }
    if (canDelete) {
      Status status=getRequest().getConditions().getStatus(getRequest().getMethod(),preferredVariant);
      if (status != null) {
        getResponse().setStatus(status);
        canDelete=false;
      }
    }
  }
  if (canDelete) {
    delete();
  }
}
