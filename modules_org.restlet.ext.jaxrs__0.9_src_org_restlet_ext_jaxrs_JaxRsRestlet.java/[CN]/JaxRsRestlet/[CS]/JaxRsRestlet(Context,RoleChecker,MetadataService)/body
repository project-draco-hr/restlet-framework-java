{
  super(context);
  this.extensionBackwardMapping=new ExtensionBackwardMapping(metadataService);
  this.excHandler=new ExceptionHandler(getLogger());
  this.wrapperFactory=new WrapperFactory(this.tlContext,this.entityProviders,this.contextResolvers,this.extensionBackwardMapping,getLogger());
  loadDefaultProviders();
  if (roleChecker != null) {
    setRoleChecker(roleChecker);
  }
 else {
    setRoleChecker(RoleChecker.REJECT_WITH_ERROR);
  }
}
