{
  boolean readNext=true;
  Parameter param=null;
  try {
    while (readNext) {
      param=readNextParameter();
      if (param != null) {
        form.add(param);
      }
 else {
        readNext=false;
      }
    }
  }
 catch (  final IOException ioe) {
    getLogger().log(Level.WARNING,"Unable to parse a form parameter. Skipping the remaining parameters.",ioe);
  }
  try {
    this.stream.close();
  }
 catch (  final IOException ioe) {
    getLogger().log(Level.WARNING,"Unable to close the form input stream",ioe);
  }
}
