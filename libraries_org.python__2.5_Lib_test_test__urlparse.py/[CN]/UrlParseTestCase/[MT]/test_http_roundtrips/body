def test_http_roundtrips(self):
    testcases = [('://www.python.org', ('www.python.org', '', '', '', ''), ('www.python.org', '', '', '')), ('://www.python.org#abc', ('www.python.org', '', '', '', 'abc'), ('www.python.org', '', '', 'abc')), ('://www.python.org?q=abc', ('www.python.org', '', '', 'q=abc', ''), ('www.python.org', '', 'q=abc', '')), ('://www.python.org/#abc', ('www.python.org', '/', '', '', 'abc'), ('www.python.org', '/', '', 'abc')), ('://a/b/c/d;p?q#f', ('a', '/b/c/d', 'p', 'q', 'f'), ('a', '/b/c/d;p', 'q', 'f'))]
    for scheme in ('http', 'https'):
        for (url, parsed, split) in testcases:
            url = (scheme + url)
            parsed = ((scheme,) + parsed)
            split = ((scheme,) + split)
            self.checkRoundtrips(url, parsed, split)
