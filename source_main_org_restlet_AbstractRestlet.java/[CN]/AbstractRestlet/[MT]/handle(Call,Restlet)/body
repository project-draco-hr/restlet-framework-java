{
  if (target != null) {
    if (target.isStopped()) {
      try {
        target.start();
      }
 catch (      Exception e) {
        logger.log(Level.WARNING,"Unable to start the next handler and to invoke it",e);
        call.setStatus(new DefaultStatus(Statuses.SERVER_ERROR_INTERNAL,e.getMessage()));
      }
    }
    if (target.isStarted()) {
      target.handle(call);
    }
  }
 else {
  }
}
