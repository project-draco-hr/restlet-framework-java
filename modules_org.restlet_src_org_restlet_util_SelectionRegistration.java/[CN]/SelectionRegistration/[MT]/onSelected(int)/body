{
  this.readyOperations=readyOperations;
  if (Context.getCurrentLogger().isLoggable(Level.INFO)) {
    Context.getCurrentLogger().log(Level.INFO,"NIO selection (interest | ready | cancelled | attach): " + toString());
  }
  if ((getListener() != null) && ((getReadyOperations() & getInterestOperations()) != 0)) {
    getListener().onSelected(this);
  }
}
