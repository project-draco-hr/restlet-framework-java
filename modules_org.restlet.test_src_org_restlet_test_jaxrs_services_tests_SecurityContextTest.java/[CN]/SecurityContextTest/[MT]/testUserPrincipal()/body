{
  if (!startServer(AllowAllAuthenticator.getInstance()))   return;
  Response response=get("userPrincipal");
  assertEquals(Status.SUCCESS_OK,response.getStatus());
  String entity=response.getEntity().getText();
  assertEquals("-",entity);
  response=get("userPrincipal",new ChallengeResponse(ChallengeScheme.HTTP_BASIC,"abc","def"));
  assertEquals(Status.SUCCESS_OK,response.getStatus());
  entity=response.getEntity().getText();
  assertEquals("abc",entity);
  response=get("userPrincipal",new ChallengeResponse(ChallengeScheme.HTTP_BASIC,"asdfsdfbc","def"));
  assertEquals(Status.SUCCESS_OK,response.getStatus());
  entity=response.getEntity().getText();
  assertEquals("asdfsdfbc",entity);
}
