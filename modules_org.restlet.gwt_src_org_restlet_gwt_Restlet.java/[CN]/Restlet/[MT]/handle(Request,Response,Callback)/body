{
  if (isStopped()) {
    try {
      start();
    }
 catch (    Exception e) {
      System.err.println(UNABLE_TO_START);
      response.setStatus(Status.SERVER_ERROR_INTERNAL);
    }
    if (!isStarted()) {
      System.err.println(UNABLE_TO_START);
      response.setStatus(Status.SERVER_ERROR_INTERNAL);
    }
  }
}
