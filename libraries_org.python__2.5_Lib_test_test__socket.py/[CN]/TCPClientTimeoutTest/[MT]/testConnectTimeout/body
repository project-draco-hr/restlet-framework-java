def testConnectTimeout(self):
    cli = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
    cli.settimeout(0.1)
    host = '192.168.192.168'
    try:
        cli.connect((host, 5000))
    except socket.timeout as st:
        pass
    except Exception as x:
        self.fail(('Client socket timeout should have raised socket.timeout, not %s' % str(x)))
    else:
        self.fail(("Client socket timeout should have raised\nsocket.timeout.  This tries to connect to %s in the assumption that it isn't\nused, but if it is on your network this failure is bogus." % host))
