{
  if (httpDate == null) {
    throw new IllegalArgumentException("Date is null");
  }
 else {
    SimpleDateFormat parser=new SimpleDateFormat(format,Locale.US);
    parser.setTimeZone(gmtTimeZone);
    try {
      return parser.parse(httpDate);
    }
 catch (    ParseException e) {
      return null;
    }
  }
}
