def test_bug453523(self):


    class C:

        def __lt__(self, other):
            if (L and (random.random() < 0.75)):
                L.pop()
            else:
                L.append(3)
            return (random.random() < 0.5)
    L = [C() for i in range(50)]
    self.assertRaises(ValueError, L.sort)
