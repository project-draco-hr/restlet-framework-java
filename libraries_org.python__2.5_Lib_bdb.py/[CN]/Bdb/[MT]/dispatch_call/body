def dispatch_call(self, frame, arg):
    if (self.botframe is None):
        self.botframe = frame.f_back
        return self.trace_dispatch
    if (not (self.stop_here(frame) or self.break_anywhere(frame))):
        return
    self.user_call(frame, arg)
    if self.quitting:
        raise BdbQuit
    return self.trace_dispatch
