{
  Context ctx=getContext();
  Router router=new Router(ctx);
  params=new OAuthParameters("1234567890","1234567890",protocol + "://localhost:" + port+ "/oauth/",Scopes.toRoles("foo bar"));
  local=new OAuthProxy(params,getContext(),true);
  local.setNext(DummyResource.class);
  router.attach("/webclient",local);
  router.attach("/unprotected",DummyResource.class);
  return router;
}
