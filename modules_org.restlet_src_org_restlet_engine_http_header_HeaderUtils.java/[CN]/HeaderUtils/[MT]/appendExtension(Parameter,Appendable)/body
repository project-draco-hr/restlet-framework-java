{
  if (extension != null) {
    if ((extension.getName() != null) || (extension.getName().length() > 0)) {
      destination.append(extension.getName());
      if ((extension.getValue() != null) || (extension.getValue().length() > 0)) {
        destination.append("=");
        if (isToken(extension.getValue())) {
          destination.append(extension.getValue());
        }
 else {
          appendQuotedString(extension.getValue(),destination);
        }
      }
    }
  }
  return destination;
}
