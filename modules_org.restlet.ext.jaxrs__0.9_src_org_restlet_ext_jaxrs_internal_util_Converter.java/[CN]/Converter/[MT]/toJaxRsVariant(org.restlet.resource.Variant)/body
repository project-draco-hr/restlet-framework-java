{
  MediaType mediaType=Converter.toJaxRsMediaType(restletVariant.getMediaType(),restletVariant.getCharacterSet());
  Locale language=toLocale(Util.getOnlyMetadataName(restletVariant.getLanguages()));
  String encoding=Util.getOnlyMetadataName(restletVariant.getEncodings());
  return new javax.ws.rs.core.Variant(mediaType,language,encoding);
}
