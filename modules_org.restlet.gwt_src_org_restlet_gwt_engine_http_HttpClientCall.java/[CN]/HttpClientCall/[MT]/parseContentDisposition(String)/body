{
  if (value != null) {
    String key="FILENAME=\"";
    int index=value.toUpperCase().indexOf(key);
    if (index > 0) {
      return value.substring(index + key.length(),value.length() - 1);
    }
 else {
      key="FILENAME=";
      index=value.toUpperCase().indexOf(key);
      if (index > 0) {
        return value.substring(index + key.length(),value.length());
      }
    }
  }
  return null;
}
