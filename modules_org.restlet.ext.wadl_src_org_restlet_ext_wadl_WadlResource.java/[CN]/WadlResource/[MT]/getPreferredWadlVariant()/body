{
  Variant result=null;
  final Application app=Application.getCurrent();
  Language language=null;
  if (app != null) {
    language=app.getMetadataService().getDefaultLanguage();
  }
  result=getRequest().getClientInfo().getPreferredVariant(getWadlVariants(),(getApplication() == null) ? null : getApplication().getMetadataService());
  return result;
}
