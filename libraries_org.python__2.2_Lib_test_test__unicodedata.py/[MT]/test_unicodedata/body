def test_unicodedata():
    h = sha.sha()
    for i in range(65536):
        char = unichr(i)
        data = [str(unicodedata.digit(char, (-1))), str(unicodedata.numeric(char, (-1))), str(unicodedata.decimal(char, (-1))), unicodedata.category(char), unicodedata.bidirectional(char), unicodedata.decomposition(char), str(unicodedata.mirrored(char)), str(unicodedata.combining(char))]
        h.update(''.join(data))
    return h.hexdigest()
