def quote(file):
    for c in file:
        if (c not in _safechars):
            break
    else:
        return file
    if ("'" not in file):
        return (("'" + file) + "'")
    res = ''
    for c in file:
        if (c in _funnychars):
            c = ('\\' + c)
        res = (res + c)
    return (('"' + res) + '"')
