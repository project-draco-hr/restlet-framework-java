{
  try {
    ScriptedTextRepresentationContainer container=new ScriptedTextRepresentationContainer(this);
    this.embeddedScript.run(writer,this.errorWriter,this.scriptEngines,new ScriptedTextRepresentationScriptContextController(this,container,this.embeddedScript.getContainerVariableName()),false);
  }
 catch (  ScriptException e) {
    IOException ioe=new IOException("Script exception");
    ioe.initCause(e);
    throw ioe;
  }
}
