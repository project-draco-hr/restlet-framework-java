{
  try {
    ConcurrentMap<String,ScriptContext> scriptContexts=new ConcurrentHashMap<String,ScriptContext>();
    this.embeddedScript.run(writer,this.errorWriter,false,scriptContexts,new ExposedScriptedTextRepresentationContainer(this),getScriptContextController(),false);
  }
 catch (  ScriptException e) {
    IOException ioe=new IOException("Script exception");
    ioe.initCause(e);
    throw ioe;
  }
}
