{
  try {
    this.compositeScript.run(false,writer,this.errorWriter,false,this.compositeScriptContext,new ExposedScriptedTextRepresentationContainer(this),getScriptContextController());
  }
 catch (  ScriptException e) {
    IOException ioe=new IOException("Script exception");
    ioe.initCause(e);
    throw ioe;
  }
}
