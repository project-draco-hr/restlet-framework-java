{
  try {
    ConcurrentMap<String,ScriptEngine> scriptEngines=new ConcurrentHashMap<String,ScriptEngine>();
    this.embeddedScript.run(writer,this.errorWriter,false,scriptEngines,new ExposedScriptedTextRepresentationContainer(this),getScriptContextController(),false);
  }
 catch (  ScriptException e) {
    IOException ioe=new IOException("Script exception");
    ioe.initCause(e);
    throw ioe;
  }
}
