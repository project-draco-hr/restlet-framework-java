{
  try {
    ScriptedTextRepresentationContainer container=new ScriptedTextRepresentationContainer(writer,this.errorWriter);
    this.embeddedScript.run(writer,this.errorWriter,this.scriptEngines,new ScriptedTextRepresentationScriptContextController(container,this.scriptContextController),false);
  }
 catch (  ScriptException e) {
    IOException ioe=new IOException("Script exception");
    ioe.initCause(e);
    throw ioe;
  }
}
