{
  if (this.defaultScriptEngineName == null) {
    ConcurrentMap<String,Object> attributes=Context.getCurrent().getAttributes();
    this.defaultScriptEngineName=(String)attributes.get("org.restlet.ext.script.ScriptedTextRepresentation.defaultScriptEngineName");
    if (this.defaultScriptEngineName == null) {
      this.defaultScriptEngineName="js";
    }
  }
  return this.defaultScriptEngineName;
}
