def is_suburi(self, base, test):
    'Check if test is below base in a URI tree\n\n        Both args must be URIs in reduced form.\n        '
    if (base == test):
        return 1
    if (base[0] != test[0]):
        return 0
    common = posixpath.commonprefix((base[1], test[1]))
    if (len(common) == len(base[1])):
        return 1
    return 0
