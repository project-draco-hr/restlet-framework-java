def get_algorithm_impls(self, algorithm):
    if (algorithm == 'MD5'):
        H = (lambda x, e=encode_digest: e(md5.new(x).digest()))
    elif (algorithm == 'SHA'):
        H = (lambda x, e=encode_digest: e(sha.new(x).digest()))
    KD = (lambda s, d, H=H: H(('%s:%s' % (s, d))))
    return (H, KD)
