def setUp(self):
    self.tempFiles = []
    self.registerFileForCleanUp(test_support.TESTFN)
    self.text = 'testing urllib.urlretrieve'
    try:
        FILE = file(test_support.TESTFN, 'wb')
        FILE.write(self.text)
        FILE.close()
    finally:
        try:
            FILE.close()
        except:
            pass
