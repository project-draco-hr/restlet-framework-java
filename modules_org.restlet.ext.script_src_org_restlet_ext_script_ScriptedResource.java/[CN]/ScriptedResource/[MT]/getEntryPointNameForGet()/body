{
  if (this.entryPointNameForGet == null) {
    ConcurrentMap<String,Object> attributes=getContext().getAttributes();
    this.entryPointNameForGet=(String)attributes.get("org.restlet.ext.script.ScriptedResource.entryPointNameForGet");
    if (this.entryPointNameForGet == null) {
      this.entryPointNameForGet="handleGet";
    }
  }
  return this.entryPointNameForGet;
}
