{
  if (this.entryPointNameForDelete == null) {
    ConcurrentMap<String,Object> attributes=getContext().getAttributes();
    this.entryPointNameForDelete=(String)attributes.get("org.restlet.ext.script.ScriptedResource.entryPointNameForDelete");
    if (this.entryPointNameForDelete == null) {
      this.entryPointNameForDelete="handleDelete";
    }
  }
  return this.entryPointNameForDelete;
}
