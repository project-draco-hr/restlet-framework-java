{
  if (this.representEntryPointName == null) {
    ConcurrentMap<String,Object> attributes=getContext().getAttributes();
    this.representEntryPointName=(String)attributes.get("org.restlet.ext.script.ScriptedResource.representEntryPointName");
    if (this.representEntryPointName == null) {
      this.representEntryPointName="represent";
    }
  }
  return this.representEntryPointName;
}
