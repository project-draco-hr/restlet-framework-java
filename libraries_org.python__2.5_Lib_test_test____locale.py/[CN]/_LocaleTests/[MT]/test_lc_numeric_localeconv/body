def test_lc_numeric_localeconv(self):
    for loc in candidate_locales:
        try:
            setlocale(LC_NUMERIC, loc)
        except Error:
            continue
        for (li, lc) in ((RADIXCHAR, 'decimal_point'), (THOUSEP, 'thousands_sep')):
            self.numeric_tester('localeconv', localeconv()[lc], lc, loc)
