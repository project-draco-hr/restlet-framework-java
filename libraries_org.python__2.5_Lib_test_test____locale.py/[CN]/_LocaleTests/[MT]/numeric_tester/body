def numeric_tester(self, calc_type, calc_value, data_type, used_locale):
    'Compare calculation against known value, if available'
    try:
        set_locale = setlocale(LC_NUMERIC)
    except Error:
        set_locale = '<not able to determine>'
    known_value = known_numerics.get(used_locale, ('', ''))[(data_type is 'thousands_sep')]
    if (known_value and calc_value):
        self.assertEquals(calc_value, known_value, (self.lc_numeric_err_msg % (calc_value, known_value, calc_type, data_type, set_locale, used_locale)))
