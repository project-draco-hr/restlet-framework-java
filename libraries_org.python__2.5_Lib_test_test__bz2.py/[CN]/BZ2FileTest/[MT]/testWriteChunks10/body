def testWriteChunks10(self):
    bz2f = BZ2File(self.filename, 'w')
    n = 0
    while 1:
        str = self.TEXT[(n * 10):((n + 1) * 10)]
        if (not str):
            break
        bz2f.write(str)
        n += 1
    bz2f.close()
    f = open(self.filename, 'rb')
    self.assertEqual(self.decompress(f.read()), self.TEXT)
    f.close()
