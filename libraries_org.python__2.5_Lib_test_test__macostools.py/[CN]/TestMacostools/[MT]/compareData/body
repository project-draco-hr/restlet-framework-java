def compareData(self):
    fp = open(test_support.TESTFN, 'r')
    data1 = fp.read()
    fp.close()
    fp = open(TESTFN2, 'r')
    data2 = fp.read()
    fp.close()
    if (data1 != data2):
        return 'Data forks differ'
    rfp = MacOS.openrf(test_support.TESTFN, '*rb')
    data1 = rfp.read(1000)
    rfp.close()
    rfp = MacOS.openrf(TESTFN2, '*rb')
    data2 = rfp.read(1000)
    rfp.close()
    if (data1 != data2):
        return 'Resource forks differ'
    return ''
