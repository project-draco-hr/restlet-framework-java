def test_starttag_junk_chars(self):
    self._parse_error('</>')
    self._parse_error('</$>')
    self._parse_error('</')
    self._parse_error('</a')
    self._parse_error('<a<a>')
    self._parse_error('</a<a>')
    self._parse_error('<!')
    self._parse_error('<a $>')
    self._parse_error('<a')
    self._parse_error("<a foo='bar'")
    self._parse_error("<a foo='bar")
    self._parse_error("<a foo='>'")
    self._parse_error("<a foo='>")
    self._parse_error('<a foo=>')
