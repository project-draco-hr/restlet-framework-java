{
  boolean result=super.control();
  SipRequest request;
  for (Iterator<SipRequest> iter=getRequests().values().iterator(); iter.hasNext(); ) {
    request=iter.next();
    if (request.hasTimedOut()) {
      Level level;
      if (request.isHandled()) {
        level=Level.FINE;
      }
 else {
        level=Level.INFO;
        SipResponse response=new SipResponse(request);
        response.setStatus(SipStatus.CLIENT_ERROR_REQUEST_TIMEOUT,"The SIP client connector has timeout due to lack of activity on this transaction: " + request.getTransaction());
        handleInbound(response,false);
      }
      getLogger().log(level,"This SIP transaction has timed out: " + request.getTransaction());
      iter.remove();
    }
  }
  return result;
}
