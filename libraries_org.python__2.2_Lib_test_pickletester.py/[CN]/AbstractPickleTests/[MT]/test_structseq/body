def test_structseq(self):
    import time
    t = time.localtime()
    s = self.dumps(t)
    u = self.loads(s)
    self.assertEqual(t, u)
    import os
    if hasattr(os, 'stat'):
        t = os.stat(os.curdir)
        s = self.dumps(t)
        u = self.loads(s)
        self.assertEqual(t, u)
    if hasattr(os, 'statvfs'):
        t = os.statvfs(os.curdir)
        s = self.dumps(t)
        u = self.loads(s)
        self.assertEqual(t, u)
