{
  User result=null;
  if (this.userName != null) {
    final Predicate<User> predicate=new Predicate<User>(){
      private static final long serialVersionUID=1L;
      @Override public boolean match(      User candidate){
        return UserResource.this.userName.equals(candidate.getName());
      }
    }
;
    final List<User> users=getContainer().query(predicate);
    if ((users != null) && (users.size() > 0)) {
      result=users.get(0);
    }
  }
  return result;
}
