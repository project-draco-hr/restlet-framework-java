def fromlist_test(name, init, listdata, typecode):
    if verbose:
        print (40 * '*')
        print name, '- type:', typecode
    a = array(typecode, init)
    if verbose:
        print 'initial:', a
        print 'fromlist source:', listdata
    a.fromlist(listdata)
    if verbose:
        print 'final:', a
    return a
