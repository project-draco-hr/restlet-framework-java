def check_parse_error(self, source):
    parser = EventCollector()
    try:
        parser.feed(source)
        parser.close()
    except sgmllib.SGMLParseError:
        pass
    else:
        self.fail(('expected SGMLParseError for %r\nReceived:\n%s' % (source, pprint.pformat(parser.get_events()))))
