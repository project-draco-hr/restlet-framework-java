{
  try {
    final HttpRequest request=getConverter().toRequest(httpCall);
    final HttpResponse response=new HttpResponse(httpCall,request);
    handle(request,response);
    getConverter().commit(response);
  }
 catch (  final Exception e) {
    getLogger().log(Level.WARNING,"Error while handling an HTTP server call: ",e.getMessage());
    getLogger().log(Level.INFO,"Error while handling an HTTP server call",e);
  }
}
