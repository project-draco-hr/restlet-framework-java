def __hash__(self):
    tzoff = self._utcoffset()
    if (tzoff is None):
        return hash(self.__getstate()[0])
    days = _ymd2ord(self.year, self.month, self.day)
    seconds = (((self.hour * 3600) + ((self.minute - tzoff) * 60)) + self.second)
    return hash(timedelta(days, seconds, self.microsecond))
