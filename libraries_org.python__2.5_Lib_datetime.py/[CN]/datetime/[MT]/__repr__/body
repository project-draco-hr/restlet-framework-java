def __repr__(self):
    'Convert to formal string, for repr().'
    L = [self.__year, self.__month, self.__day, self.__hour, self.__minute, self.__second, self.__microsecond]
    if (L[(-1)] == 0):
        del L[(-1)]
    if (L[(-1)] == 0):
        del L[(-1)]
    s = ', '.join(map(str, L))
    s = ('%s(%s)' % (('datetime.' + self.__class__.__name__), s))
    if (self._tzinfo is not None):
        assert (s[(-1):] == ')')
        s = ((s[:(-1)] + (', tzinfo=%r' % self._tzinfo)) + ')')
    return s
