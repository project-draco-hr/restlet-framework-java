def __repr__(self):
    if self.__microseconds:
        return ('%s(%d, %d, %d)' % (('datetime.' + self.__class__.__name__), self.__days, self.__seconds, self.__microseconds))
    if self.__seconds:
        return ('%s(%d, %d)' % (('datetime.' + self.__class__.__name__), self.__days, self.__seconds))
    return ('%s(%d)' % (('datetime.' + self.__class__.__name__), self.__days))
