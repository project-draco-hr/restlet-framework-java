def __reduce__(self):
    getinitargs = getattr(self, '__getinitargs__', None)
    if getinitargs:
        args = getinitargs()
    else:
        args = ()
    getstate = getattr(self, '__getstate__', None)
    if getstate:
        state = getstate()
    else:
        state = (getattr(self, '__dict__', None) or None)
    if (state is None):
        return (self.__class__, args)
    else:
        return (self.__class__, args, state)
