def test_main():
    tests = [ChdirTestCase, ImportTestCase, ImportPackageTestCase, ZipimportTestCase, PyCompileTestCase, ExecfileTestCase, ExecfileTracebackTestCase, ListdirTestCase, DirsTestCase, FilesTestCase]
    if WINDOWS:
        tests.append(WindowsChdirTestCase)
    if test_support.is_jython:
        tests.extend((ImportJavaClassTestCase, ImportJarTestCase))
    if test_support.is_resource_enabled('subprocess'):
        tests.append(SubprocessTestCase)
    test_support.run_unittest(*tests)
