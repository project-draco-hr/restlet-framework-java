def test_execfile(self):
    globals = {}
    execfile(self.filename1, globals)
    self.assertEqual(globals['result'], CODE1_RESULT)
    os.chdir(self.dir1)
    globals = {}
    execfile(self.basename1, globals)
    self.assertEqual(globals['result'], CODE1_RESULT)
    os.chdir(self.dir2)
    raises(IOError, 2, execfile, self.basename1, globals)
