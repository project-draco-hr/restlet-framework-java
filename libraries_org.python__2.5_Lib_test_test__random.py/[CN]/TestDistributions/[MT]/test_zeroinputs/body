def test_zeroinputs(self):
    g = random.Random()
    x = ([g.random() for i in xrange(50)] + ([0.0] * 5))
    g.random = x[:].pop
    g.uniform(1, 10)
    g.random = x[:].pop
    g.paretovariate(1.0)
    g.random = x[:].pop
    g.expovariate(1.0)
    g.random = x[:].pop
    g.weibullvariate(1.0, 1.0)
    g.random = x[:].pop
    g.normalvariate(0.0, 1.0)
    g.random = x[:].pop
    g.gauss(0.0, 1.0)
    g.random = x[:].pop
    g.lognormvariate(0.0, 1.0)
    g.random = x[:].pop
    g.vonmisesvariate(0.0, 1.0)
    g.random = x[:].pop
    g.gammavariate(0.01, 1.0)
    g.random = x[:].pop
    g.gammavariate(1.0, 1.0)
    g.random = x[:].pop
    g.gammavariate(200.0, 1.0)
    g.random = x[:].pop
    g.betavariate(3.0, 3.0)
