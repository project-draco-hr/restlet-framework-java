def test_bigrand_ranges(self):
    for i in [40, 80, 160, 200, 211, 250, 375, 512, 550]:
        start = self.gen.randrange((2 ** i))
        stop = self.gen.randrange((2 ** (i - 2)))
        if (stop <= start):
            return
        self.assert_((start <= self.gen.randrange(start, stop) < stop))
