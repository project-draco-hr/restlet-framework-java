def test_sample(self):
    N = 100
    population = xrange(N)
    for k in xrange((N + 1)):
        s = self.gen.sample(population, k)
        self.assertEqual(len(s), k)
        uniq = set(s)
        self.assertEqual(len(uniq), k)
        self.failUnless((uniq <= set(population)))
    self.assertEqual(self.gen.sample([], 0), [])
