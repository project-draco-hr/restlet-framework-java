def test_gauss(self):
    for seed in (1, 12, 123, 1234, 12345, 123456, 654321):
        self.gen.seed(seed)
        x1 = self.gen.random()
        y1 = self.gen.gauss(0, 1)
        self.gen.seed(seed)
        x2 = self.gen.random()
        y2 = self.gen.gauss(0, 1)
        self.assertEqual(x1, x2)
        self.assertEqual(y1, y2)
