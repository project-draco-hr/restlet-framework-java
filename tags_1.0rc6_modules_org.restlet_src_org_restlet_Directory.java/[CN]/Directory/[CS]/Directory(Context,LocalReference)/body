{
  super(context);
  if (rootLocalReference.getIdentifier().endsWith("/")) {
    this.rootRef=new Reference(rootLocalReference.getIdentifier());
  }
 else {
    this.rootRef=new Reference(rootLocalReference.getIdentifier() + "/");
  }
  this.deeplyAccessible=true;
  this.modifiable=false;
  this.listingAllowed=false;
}
