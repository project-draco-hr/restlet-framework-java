def test_iteritems_while_modifying_values(self):
    if (not hasattr(self.f, 'iteritems')):
        return
    di = self.d.iteritems()
    while 1:
        try:
            (k, v) = di.next()
            self.d[k] = ('modified ' + v)
        except StopIteration:
            break
    fi = self.f.iteritems()
    while 1:
        try:
            (k, v) = fi.next()
            self.f[k] = ('modified ' + v)
        except StopIteration:
            break
    self.test_mapping_iteration_methods()
