{
  Filter filter=getFilter();
  assertFalse(filter.hasTarget());
  filter.attach(getRestlet());
  filter.start();
  assertTrue(filter.isStarted());
  assertFalse(filter.isStopped());
  Call call=getCall();
  filter.handle(call);
  assertTrue(filter.hasTarget());
  filter.detach();
  assertFalse(filter.hasTarget());
}
