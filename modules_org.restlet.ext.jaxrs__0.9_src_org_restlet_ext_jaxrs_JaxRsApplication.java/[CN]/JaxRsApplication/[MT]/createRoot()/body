{
  Restlet restlet=jaxRsRouter;
  TunnelFilter tunnelFilter=new TunnelFilter(getContext());
  tunnelFilter.setNext(restlet);
  restlet=tunnelFilter;
  if (this.guard != null) {
    this.guard.setNext(restlet);
    restlet=this.guard;
  }
  if (preferHtml) {
    restlet=new HtmlPreferer(getContext(),restlet);
  }
  return restlet;
}
