{
  Logger result=null;
  Context context=getContext();
  if (context == null) {
    context=Context.getCurrent();
  }
  if (context != null) {
    result=context.getLogger();
  }
  if (result == null) {
    result=Logger.getLogger(getClass().getCanonicalName());
  }
  if (result == null) {
    result=Logger.getLogger("org.restlet.Restlet");
  }
  return result;
}
