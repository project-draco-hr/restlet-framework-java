def pylists():
    if verbose:
        print 'Testing Python subclass of list...'


    class C(list):

        def __getitem__(self, i):
            return (list.__getitem__(self, i) + 100)

        def __getslice__(self, i, j):
            return (i, j)
    a = C()
    a.extend([0, 1, 2])
    vereq(a[0], 100)
    vereq(a[1], 101)
    vereq(a[2], 102)
    vereq(a[100:200], (100, 200))
