def testrmul():
    if verbose:
        print 'Testing correct invocation of __rmul__...'


    class C(object):

        def __mul__(self, other):
            return 'mul'

        def __rmul__(self, other):
            return 'rmul'
    a = C()
    vereq((a * 2), 'mul')
    vereq((a * 2.2), 'mul')
    vereq((2 * a), 'rmul')
    vereq((2.2 * a), 'rmul')
