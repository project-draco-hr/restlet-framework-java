def staticmethods_in_c():
    if verbose:
        print 'Testing C-based static methods...'
    import xxsubtype as spam
    a = (1, 2, 3)
    d = {'abc': 123, }
    (x, a1, d1) = spam.spamlist.staticmeth(*a, **d)
    veris(x, None)
    vereq(a, a1)
    vereq(d, d1)
    (x, a1, d2) = spam.spamlist().staticmeth(*a, **d)
    veris(x, None)
    vereq(a, a1)
    vereq(d, d1)
