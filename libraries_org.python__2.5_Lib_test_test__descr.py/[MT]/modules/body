def modules():
    if verbose:
        print 'Testing uninitialized module objects...'
    from types import ModuleType as M
    m = M.__new__(M)
    str(m)
    vereq(hasattr(m, '__name__'), 0)
    vereq(hasattr(m, '__file__'), 0)
    vereq(hasattr(m, 'foo'), 0)
    vereq(m.__dict__, None)
    m.foo = 1
    vereq(m.__dict__, {'foo': 1, })
