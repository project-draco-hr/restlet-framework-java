def classmethods_in_c():
    if verbose:
        print 'Testing C-based class methods...'
    import xxsubtype as spam
    a = (1, 2, 3)
    d = {'abc': 123, }
    (x, a1, d1) = spam.spamlist.classmeth(*a, **d)
    veris(x, spam.spamlist)
    vereq(a, a1)
    vereq(d, d1)
    (x, a1, d1) = spam.spamlist().classmeth(*a, **d)
    veris(x, spam.spamlist)
    vereq(a, a1)
    vereq(d, d1)
