def test_func_doc():

    def f():
        pass
    verify((f.__doc__ is None))
    verify((f.func_doc is None))
    f.__doc__ = 'hello'
    verify((f.__doc__ == 'hello'))
    verify((f.func_doc == 'hello'))
    del f.__doc__
    verify((f.__doc__ is None))
    verify((f.func_doc is None))
    f.func_doc = 'world'
    verify((f.__doc__ == 'world'))
    verify((f.func_doc == 'world'))
    del f.func_doc
    verify((f.func_doc is None))
    verify((f.__doc__ is None))
