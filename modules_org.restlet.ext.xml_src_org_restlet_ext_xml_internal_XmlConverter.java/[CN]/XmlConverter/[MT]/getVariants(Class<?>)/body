{
  List<VariantInfo> result=null;
  if (Document.class.isAssignableFrom(objectClass) || DomRepresentation.class.isAssignableFrom(objectClass) || SaxRepresentation.class.isAssignableFrom(objectClass)) {
    result=addVariant(result,VARIANT_APPLICATION_ALL_XML);
    result=addVariant(result,VARIANT_APPLICATION_XML);
    result=addVariant(result,VARIANT_TEXT_XML);
  }
  return result;
}
