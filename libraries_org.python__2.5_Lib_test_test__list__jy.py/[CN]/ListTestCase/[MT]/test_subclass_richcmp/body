def test_subclass_richcmp(self):


    class Foo(list):

        def __init__(self, dotstring):
            list.__init__(self, map(int, dotstring.split('.')))
    bar1 = Foo('1.2.3')
    bar2 = Foo('1.2.4')
    self.assert_((bar1 < bar2))
    self.assert_((bar1 <= bar2))
    self.assert_((bar2 > bar1))
    self.assert_((bar2 >= bar1))
