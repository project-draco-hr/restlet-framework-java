def test_commonprefix(self):
    commonprefix = dospath.commonprefix
    self.assert_((commonprefix(['/home/swenson/spam', '/home/swen/spam']) == '/home/swen'))
    self.assert_((commonprefix(['\\home\\swen\\spam', '\\home\\swen\\eggs']) == '\\home\\swen\\'))
    self.assert_((commonprefix(['/home/swen/spam', '/home/swen/spam']) == '/home/swen/spam'))
