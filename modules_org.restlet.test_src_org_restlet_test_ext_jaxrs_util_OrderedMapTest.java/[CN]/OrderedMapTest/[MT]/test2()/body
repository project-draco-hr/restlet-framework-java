{
  OrderedMap<String,Integer> sob=new OrderedMap<String,Integer>();
  sob.add("a",1);
  assertEquals("[a -> 1]",sob.toString());
  sob.add("b",2);
  assertEquals("[a -> 1, b -> 2]",sob.toString());
  sob.add("d",1);
  assertEquals("[a -> 1, b -> 2, d -> 1]",sob.toString());
  sob.add("c",0);
  assertEquals("[a -> 1, b -> 2, d -> 1, c -> 0]",sob.toString());
}
