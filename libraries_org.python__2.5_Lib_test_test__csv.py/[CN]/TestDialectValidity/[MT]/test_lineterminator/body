def test_lineterminator(self):


    class mydialect(csv.Dialect):
        delimiter = ';'
        escapechar = '\\'
        doublequote = False
        skipinitialspace = True
        lineterminator = '\r\n'
        quoting = csv.QUOTE_NONE
    d = mydialect()
    mydialect.lineterminator = ':::'
    d = mydialect()
    mydialect.lineterminator = 4
    self.assertRaises(csv.Error, mydialect)
