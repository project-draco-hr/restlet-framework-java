def test_delimiter(self):


    class mydialect(csv.Dialect):
        delimiter = ';'
        escapechar = '\\'
        doublequote = False
        skipinitialspace = True
        lineterminator = '\r\n'
        quoting = csv.QUOTE_NONE
    d = mydialect()
    mydialect.delimiter = ':::'
    self.assertRaises(csv.Error, mydialect)
    mydialect.delimiter = 4
    self.assertRaises(csv.Error, mydialect)
