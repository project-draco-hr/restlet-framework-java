def test_read_with_blanks(self):
    reader = csv.DictReader(['1,2,abc,4,5,6\r\n', '\r\n', '1,2,abc,4,5,6\r\n'], fieldnames='1 2 3 4 5 6'.split())
    self.assertEqual(reader.next(), {'1': '1', '2': '2', '3': 'abc', '4': '4', '5': '5', '6': '6', })
    self.assertEqual(reader.next(), {'1': '1', '2': '2', '3': 'abc', '4': '4', '5': '5', '6': '6', })
