def test_read_short(self):
    (fd, name) = tempfile.mkstemp()
    fileobj = os.fdopen(fd, 'w+b')
    try:
        fileobj.write('1,2,abc,4,5,6\r\n1,2,abc\r\n')
        fileobj.seek(0)
        reader = csv.DictReader(fileobj, fieldnames='1 2 3 4 5 6'.split(), restval='DEFAULT')
        self.assertEqual(reader.next(), {'1': '1', '2': '2', '3': 'abc', '4': '4', '5': '5', '6': '6', })
        self.assertEqual(reader.next(), {'1': '1', '2': '2', '3': 'abc', '4': 'DEFAULT', '5': 'DEFAULT', '6': 'DEFAULT', })
    finally:
        fileobj.close()
        os.unlink(name)
