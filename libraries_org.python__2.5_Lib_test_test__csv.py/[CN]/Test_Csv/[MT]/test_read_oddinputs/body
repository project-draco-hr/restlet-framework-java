def test_read_oddinputs(self):
    self._read_test([], [])
    self._read_test([''], [[]])
    self.assertRaises(csv.Error, self._read_test, ['"ab"c'], None, strict=1)
    self.assertRaises(csv.Error, self._read_test, ['ab\x00c'], None, strict=1)
    self._read_test(['"ab"c'], [['abc']], doublequote=0)
