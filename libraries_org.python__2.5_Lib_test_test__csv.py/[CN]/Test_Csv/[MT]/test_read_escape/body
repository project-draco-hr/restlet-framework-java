def test_read_escape(self):
    self._read_test(['a,\\b,c'], [['a', 'b', 'c']], escapechar='\\')
    self._read_test(['a,b\\,c'], [['a', 'b,c']], escapechar='\\')
    self._read_test(['a,"b\\,c"'], [['a', 'b,c']], escapechar='\\')
    self._read_test(['a,"b,\\c"'], [['a', 'b,c']], escapechar='\\')
    self._read_test(['a,"b,c\\""'], [['a', 'b,c"']], escapechar='\\')
    self._read_test(['a,"b,c"\\'], [['a', 'b,c\\']], escapechar='\\')
