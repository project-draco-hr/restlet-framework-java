{
  try {
    return mbw.isWriteable(entityClass,genericType,annotations);
  }
 catch (  final NullPointerException e) {
    if ((genericType == null) || (annotations == null)) {
      return false;
    }
    throw e;
  }
catch (  final IllegalArgumentException e) {
    if ((genericType == null) || (annotations == null)) {
      return false;
    }
    throw e;
  }
}
