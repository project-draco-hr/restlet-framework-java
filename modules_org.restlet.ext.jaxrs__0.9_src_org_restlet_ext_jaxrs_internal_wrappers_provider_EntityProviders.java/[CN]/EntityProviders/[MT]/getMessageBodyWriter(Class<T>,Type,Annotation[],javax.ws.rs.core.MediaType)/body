{
  Collection<MediaType> restletMediaTypes=Collections.singleton(Converter.toRestletMediaType(mediaType));
  List<MessageBodyWriter> mbws=(List)this.messageBodyWriters;
  for (  MessageBodyWriter<T> mbw : mbws) {
    if (mbw.supportAtLeastOne(restletMediaTypes))     if (isWriteable(mbw,type,genericType,annotations))     return mbw.getJaxRsWriter();
  }
  return null;
}
