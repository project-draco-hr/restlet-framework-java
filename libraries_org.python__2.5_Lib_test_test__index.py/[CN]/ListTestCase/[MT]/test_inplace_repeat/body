def test_inplace_repeat(self):
    self.o.ind = 2
    self.n.ind = 3
    lst = [6, 4]
    lst *= self.o
    self.assertEqual(lst, [6, 4, 6, 4])
    lst *= self.n
    self.assertEqual(lst, ([6, 4, 6, 4] * 3))
    lst = [5, 6, 7, 8, 9, 11]
    l2 = lst.__imul__(self.n)
    self.assert_((l2 is lst))
    self.assertEqual(lst, ([5, 6, 7, 8, 9, 11] * 3))
