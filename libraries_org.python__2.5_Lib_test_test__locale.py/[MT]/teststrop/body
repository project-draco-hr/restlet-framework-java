def teststrop(s, method, output):
    if verbose:
        print ('%s.%s() =? %s ...' % (repr(s), method, repr(output))),
    result = getattr(s, method)()
    if (result != output):
        if verbose:
            print 'no'
        print ('%s.%s() == %s != %s' % (repr(s), method, repr(result), repr(output)))
    elif verbose:
        print 'yes'
