{
  Representation result=null;
  if (variant.getMediaType().equals(MediaType.TEXT_XML)) {
    result=new StringRepresentation("<root/>",MediaType.TEXT_XML);
  }
 else   if (variant.getMediaType().equals(MediaType.APPLICATION_JSON)) {
    result=new StringRepresentation("[\"root\"]",MediaType.APPLICATION_JSON);
  }
 else   if (variant.getMediaType().equals(MediaType.TEXT_HTML)) {
    result=new StringRepresentation("<html><body>root</body></html>",MediaType.TEXT_HTML);
  }
  return result;
}
