{
  int j=new Random().nextInt(size());
  if (get(j).score(request,response) >= requiredScore)   return get(j);
  for (int initialIndex=j++; initialIndex != j; j++) {
    if (j == size()) {
      j=0;
    }
    if (get(j).score(request,response) >= requiredScore)     return get(j);
  }
  return null;
}
