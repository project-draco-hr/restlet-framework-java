def istest(func, exp):
    obj = eval(exp)
    test(func(obj), ('%s(%s)' % (func.__name__, exp)))
    for other in [inspect.isbuiltin, inspect.isclass, inspect.iscode, inspect.isframe, inspect.isfunction, inspect.ismethod, inspect.ismodule, inspect.istraceback]:
        if (other is not func):
            test((not other(obj)), ('not %s(%s)' % (other.__name__, exp)))
