{
  StringBuilder stb=new StringBuilder(remainingPart);
  int mpEndPos=Integer.MAX_VALUE;
  for (int i=stb.length() - 1; i >= 0; i--) {
    char character=stb.charAt(i);
    if (character == '?') {
      stb.delete(i,Integer.MAX_VALUE);
    }
 else     if (character == ';') {
      stb.delete(i,mpEndPos);
      mpEndPos=i;
    }
 else     if (character == '/') {
      mpEndPos=i;
    }
  }
  return stb.toString();
}
