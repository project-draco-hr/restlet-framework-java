def test_scaled_msg(scale, name):
    iterations = ((106201 / scale) * 20)
    longStr = ('Z' * scale)
    localCF = creatorFunc
    start = time.time()
    for f in xrange(iterations):
        x = localCF(longStr).digest()
    end = time.time()
    print ('%2.2f' % (end - start)), 'seconds', iterations, 'x', len(longStr), 'bytes', name
