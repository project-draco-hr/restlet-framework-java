def test_title(self):
    self.checkequal(' Hello ', ' hello ', 'title')
    self.checkequal('Hello ', 'hello ', 'title')
    self.checkequal('Hello ', 'Hello ', 'title')
    self.checkequal('Format This As Title String', 'fOrMaT thIs aS titLe String', 'title')
    self.checkequal('Format,This-As*Title;String', 'fOrMaT,thIs-aS*titLe;String', 'title')
    self.checkequal('Getint', 'getInt', 'title')
    self.checkraises(TypeError, 'hello', 'title', 42)
