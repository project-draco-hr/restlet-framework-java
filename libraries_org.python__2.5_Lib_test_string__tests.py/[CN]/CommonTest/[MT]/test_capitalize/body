def test_capitalize(self):
    self.checkequal(' hello ', ' hello ', 'capitalize')
    self.checkequal('Hello ', 'Hello ', 'capitalize')
    self.checkequal('Hello ', 'hello ', 'capitalize')
    self.checkequal('Aaaa', 'aaaa', 'capitalize')
    self.checkequal('Aaaa', 'AaAa', 'capitalize')
    self.checkraises(TypeError, 'hello', 'capitalize', 42)
