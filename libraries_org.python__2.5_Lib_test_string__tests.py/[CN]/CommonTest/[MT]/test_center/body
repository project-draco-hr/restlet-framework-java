def test_center(self):
    self.checkequal('   abc    ', 'abc', 'center', 10)
    self.checkequal(' abc  ', 'abc', 'center', 6)
    self.checkequal('abc', 'abc', 'center', 3)
    self.checkequal('abc', 'abc', 'center', 2)
    self.checkequal('***abc****', 'abc', 'center', 10, '*')
    self.checkraises(TypeError, 'abc', 'center')
