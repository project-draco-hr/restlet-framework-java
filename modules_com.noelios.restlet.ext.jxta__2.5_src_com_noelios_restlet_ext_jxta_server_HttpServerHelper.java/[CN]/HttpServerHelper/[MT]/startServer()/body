{
  if (server != null) {
    throw new IllegalStateException("server already started");
  }
  if (logger.isLoggable(Level.FINE)) {
    logger.log(Level.FINE,"starting server");
  }
  server=new JxtaMulticastServer(Constants.PROTOTYPE_MULTICAST_PIPE_NAME,network.getNetwork().getNetPeerGroup(),PipeID.create(Constants.PROTOTYPE_MULTICAST_PIPE_ID));
  server.start();
  if (logger.isLoggable(Level.FINE)) {
    logger.log(Level.FINE,"started server");
  }
}
