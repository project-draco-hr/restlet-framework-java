{
  List l=new ArrayList(defaultList());
  l.add(new Integer(0));
  List a=newInstance(l);
  TestSupport.assertThat(a.lastIndexOf(new Integer(0)) == 4,"lastIndexOf() did not return 4");
  TestSupport.assertThat(a.lastIndexOf(new Integer(42)) == -1,"lastIndexOf() non-existing value did not return -1");
}
