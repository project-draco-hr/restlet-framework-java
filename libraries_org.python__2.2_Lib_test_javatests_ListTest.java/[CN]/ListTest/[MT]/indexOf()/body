{
  TestSupport.assertThat(defaultList().indexOf(new Integer(3)) == 3,"indexOf(3) did not return 3");
  TestSupport.assertThat(defaultList().indexOf(new Integer(42)) == -1,"indexOf() non-existing entry did not return -1");
  TestSupport.assertThat(defaultList().indexOf(null) == -1,"indexOf() non-existing null did not return -1");
}
