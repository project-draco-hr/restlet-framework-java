{
  List a=newInstance(null);
  TestSupport.assertThat(a.isEmpty(),"isEmpty() is false on an emtpy List");
  a.addAll(defaultList());
  TestSupport.assertThat(!a.isEmpty(),"isEmpty() is true on a non-empty List)");
  a.clear();
  TestSupport.assertThat(a.isEmpty(),"isEmpty() is false on an emtpy List");
}
