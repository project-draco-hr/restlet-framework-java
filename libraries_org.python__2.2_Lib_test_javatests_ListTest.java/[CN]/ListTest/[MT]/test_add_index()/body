{
  List a=newInstance(null);
  List b=defaultList();
  for (int i=0; i < b.size(); i++) {
    a.add(i,b.get(i));
  }
  try {
    a.add(a.size() + 1,new Integer(a.size() + 1));
    TestSupport.assertThat(false,"expected IndexOutOfBoundsException");
  }
 catch (  IndexOutOfBoundsException e) {
  }
  try {
    a.add(-1,new Integer(-1));
    TestSupport.assertThat(false,"expected IndexOutOfBoundsException");
  }
 catch (  IndexOutOfBoundsException e) {
  }
}
