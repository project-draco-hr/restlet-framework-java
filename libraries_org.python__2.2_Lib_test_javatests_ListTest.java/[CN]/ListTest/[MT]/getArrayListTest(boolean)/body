{
  return new ListTest(){
    public List newInstance(    Collection c){
      List l=null;
      if (c == null) {
        l=new ArrayList();
      }
 else {
        l=new ArrayList(c);
      }
      return (makeReadOnly) ? Collections.unmodifiableList(l) : l;
    }
    public boolean isReadOnly(){
      return makeReadOnly;
    }
  }
;
