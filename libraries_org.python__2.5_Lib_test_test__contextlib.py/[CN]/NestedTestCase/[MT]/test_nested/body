def test_nested(self):

    @contextmanager
    def a():
        yield 1

    @contextmanager
    def b():
        yield 2

    @contextmanager
    def c():
        yield 3
    with nested(a(), b(), c()) as (x, y, z):
        self.assertEqual(x, 1)
        self.assertEqual(y, 2)
        self.assertEqual(z, 3)
