def test_nested_b_swallows(self):

    @contextmanager
    def a():
        yield

    @contextmanager
    def b():
        try:
            yield
        except:
            pass
    try:
        with nested(a(), b()):
            (1 / 0)
    except ZeroDivisionError:
        self.fail("Didn't swallow ZeroDivisionError")
