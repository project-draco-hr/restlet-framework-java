{
  Injector inj=injector;
  if (inj == null) {
synchronized (RestletGuice.Module.this) {
      inj=injector;
      if (inj == null) {
        System.err.println("Automatically creating injector.");
        injector=inj=Guice.createInjector(RestletGuice.Module.this);
      }
    }
  }
  return inj;
}
