{
  int result=-1;
  try {
    if (formattedString != null) {
      Matcher matcher=getRegexPattern().matcher(formattedString);
      if ((getMatchingMode() == MODE_EQUALS) && matcher.matches()) {
        result=matcher.end();
      }
 else       if ((getMatchingMode() == MODE_STARTS_WITH) && matcher.lookingAt()) {
        result=matcher.end();
      }
    }
  }
 catch (  StackOverflowError soe) {
    System.err.println("StackOverflowError exception encountered while matching this string : " + formattedString);
  }
  return result;
}
