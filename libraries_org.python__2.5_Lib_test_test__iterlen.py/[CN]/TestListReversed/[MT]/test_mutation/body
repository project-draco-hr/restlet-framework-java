def test_mutation(self):
    d = range(n)
    it = reversed(d)
    it.next()
    it.next()
    self.assertEqual(len(it), (n - 2))
    d.append(n)
    self.assertEqual(len(it), (n - 2))
    d[1:] = []
    self.assertEqual(len(it), 0)
    self.assertEqual(list(it), [])
    d.extend(xrange(20))
    self.assertEqual(len(it), 0)
