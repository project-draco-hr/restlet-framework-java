{
  int cont=super.beforeHandle(request,response);
  if (cont == CONTINUE) {
    FBUser fbu=null;
    getLogger().info("FBProxy retrieving FACEBOOK ID");
    try {
      fbu=getMe(getToken(request));
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
    request.getClientInfo().getUser().setIdentifier(fbu.getId());
  }
  return cont;
}
