def test_copy_location(self):
    src = ast.parse('1 + 1', mode='eval')
    src.body.right = ast.copy_location(ast.Num(2), src.body.right)
    self.assertEqual(ast.dump(src, include_attributes=True), 'Expression(body=BinOp(left=Num(n=1, lineno=1, col_offset=0), op=Add(), right=Num(n=2, lineno=1, col_offset=4), lineno=1, col_offset=0))')
