def main():
    if (__name__ != '__main__'):
        return
    if (sys.argv[1:] == ['-g']):
        for (statements, kind) in ((exec_tests, 'exec'), (single_tests, 'single'), (eval_tests, 'eval')):
            print (kind + '_results = [')
            for s in statements:
                print (repr(to_tuple(compile(s, '?', kind, 1024))) + ',')
            print ']'
        print 'main()'
        raise SystemExit
    test_main()
