def testTimeoutThenBlocking(self):
    self.sock.settimeout(10)
    self.sock.setblocking(1)
    self.assertEqual(self.sock.gettimeout(), None)
    self.sock.setblocking(0)
    self.assertEqual(self.sock.gettimeout(), 0.0)
    self.sock.settimeout(10)
    self.sock.setblocking(0)
    self.assertEqual(self.sock.gettimeout(), 0.0)
    self.sock.setblocking(1)
    self.assertEqual(self.sock.gettimeout(), None)
