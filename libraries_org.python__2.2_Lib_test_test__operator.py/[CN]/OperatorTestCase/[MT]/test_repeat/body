def test_repeat(self):
    a = range(3)
    self.failUnless((operator.repeat(a, 2) == (a + a)))
    self.failUnless((operator.repeat(a, 1) == a))
    self.failUnless((operator.repeat(a, 0) == []))
    a = (1, 2, 3)
    self.failUnless((operator.repeat(a, 2) == (a + a)))
    self.failUnless((operator.repeat(a, 1) == a))
    self.failUnless((operator.repeat(a, 0) == ()))
    a = '123'
    self.failUnless((operator.repeat(a, 2) == (a + a)))
    self.failUnless((operator.repeat(a, 1) == a))
    self.failUnless((operator.repeat(a, 0) == ''))
