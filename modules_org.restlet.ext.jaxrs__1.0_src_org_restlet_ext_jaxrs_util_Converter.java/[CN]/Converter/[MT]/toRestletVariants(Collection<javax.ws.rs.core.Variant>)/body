{
  List<org.restlet.resource.Variant> restletVariants=new ArrayList<org.restlet.resource.Variant>(jaxRsVariants.size());
  for (  javax.ws.rs.core.Variant jaxRsVariant : jaxRsVariants) {
    org.restlet.resource.Variant restletVariant=new org.restlet.resource.Variant();
    restletVariant.setCharacterSet(getRestletCharacterSet(jaxRsVariant.getMediaType()));
    restletVariant.setEncodings(Util.createList(Encoding.valueOf(jaxRsVariant.getEncoding())));
    restletVariant.setLanguages(Util.createList(Language.valueOf(jaxRsVariant.getLanguage())));
    restletVariant.setMediaType(toRestletMediaType(jaxRsVariant.getMediaType()));
    restletVariants.add(restletVariant);
  }
  return restletVariants;
}
