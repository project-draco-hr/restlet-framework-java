import codecs
decoding_map = codecs.make_identity_dict(range(256))
decoding_map.update({120: u'abc', 'abc': 120, 1: None, 121: u'', })
encoding_map = {}
for (k, v) in decoding_map.items():
    encoding_map[v] = k
