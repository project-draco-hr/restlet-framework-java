{
  getHelper().handle(new SimpleCall(getHelper().getServer(),request,response,getHelper().isConfidential()));
  try {
    if (request.getInputStream() != null) {
      while (request.getInputStream().read() != -1) {
      }
    }
    response.getOutputStream().close();
  }
 catch (  IOException ioe) {
    getHelper().getLogger().log(Level.INFO,"Exception while closing the Simple response's output stream",ioe);
  }
}
