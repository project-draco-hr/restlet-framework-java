{
  getHelper().handle(new SimpleCall(getHelper().getHelped(),request,response,getHelper().isConfidential()));
  try {
    final InputStream in=request.getInputStream();
    if (in != null) {
      while (in.read() != -1) {
      }
    }
  }
 catch (  IOException e) {
    getHelper().getLogger().log(Level.FINE,"Exception while consuming the Simple request's input stream",e);
  }
  try {
    response.getOutputStream().close();
  }
 catch (  IOException e) {
    getHelper().getLogger().log(Level.FINE,"Exception while closing the Simple response's output stream",e);
  }
}
