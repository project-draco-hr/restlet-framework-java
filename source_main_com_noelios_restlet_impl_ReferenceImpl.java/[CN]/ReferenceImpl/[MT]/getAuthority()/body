{
  String ssp=getSchemeSpecificPart();
  if (ssp.startsWith("//")) {
    int index=ssp.indexOf('/',2);
    if (index != -1) {
      return ssp.substring(2,index);
    }
 else {
      index=ssp.indexOf('?');
      if (index != -1) {
        return ssp.substring(2,index);
      }
 else {
        return ssp.substring(2);
      }
    }
  }
 else {
    return null;
  }
}
