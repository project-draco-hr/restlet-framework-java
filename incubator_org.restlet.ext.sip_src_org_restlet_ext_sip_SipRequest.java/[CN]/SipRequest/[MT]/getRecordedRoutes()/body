{
  List<Address> rr=this.recordedRoutes;
  if (rr == null) {
synchronized (this) {
      rr=this.recordedRoutes;
      if (rr == null) {
        this.recordedRoutes=rr=new CopyOnWriteArrayList<Address>();
      }
    }
  }
  if (!recordedRoutesAdded) {
    String header=getHeaders().getValues(SipConstants.HEADER_RECORD_ROUTE);
    if (header != null) {
      try {
        this.recordedRoutes.addAll(new AddressReader(header).readValues());
      }
 catch (      Exception e) {
        Context.getCurrentLogger().info(e.getMessage());
      }
    }
    recordedRoutesAdded=true;
  }
  return rr;
}
