{
  List<String> irt=this.inReplyTo;
  if (irt == null) {
synchronized (this) {
      irt=this.inReplyTo;
      if (irt == null) {
        this.inReplyTo=irt=new CopyOnWriteArrayList<String>();
      }
    }
  }
  if (!inReplyToAdded) {
    String header=getHeaders().getValues(SipConstants.HEADER_ALLOW_EVENTS);
    try {
      this.inReplyTo.addAll(new HeaderReader<String>(header).readValues());
      inReplyToAdded=true;
    }
 catch (    Exception e) {
      Context.getCurrentLogger().info(e.getMessage());
    }
  }
  return irt;
}
