{
  List<ContactInfo> c=this.contact;
  if (c == null) {
synchronized (this) {
      c=this.contact;
      if (c == null) {
        this.contact=c=new CopyOnWriteArrayList<ContactInfo>();
      }
    }
  }
  if (!contactAdded) {
    String header=getHeaders().getValues(SipConstants.HEADER_CONTACT);
    try {
      this.contact.addAll(new ContactInfoReader(header).readValues());
      contactAdded=true;
    }
 catch (    Exception e) {
      Context.getCurrentLogger().info(e.getMessage());
    }
  }
  return c;
}
