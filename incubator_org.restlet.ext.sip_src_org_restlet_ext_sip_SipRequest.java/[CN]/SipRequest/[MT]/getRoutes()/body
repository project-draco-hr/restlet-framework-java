{
  List<Address> r=this.routes;
  if (r == null) {
synchronized (this) {
      r=this.routes;
      if (r == null) {
        this.routes=r=new CopyOnWriteArrayList<Address>();
      }
    }
  }
  if (!routesAdded) {
    String header=(getHeaders() == null) ? null : getHeaders().getValues(SipConstants.HEADER_ROUTE);
    if (header != null) {
      try {
        this.routes.addAll(new AddressReader(header).readValues());
      }
 catch (      Exception e) {
        Context.getCurrentLogger().info(e.getMessage());
      }
    }
    routesAdded=true;
  }
  return r;
}
