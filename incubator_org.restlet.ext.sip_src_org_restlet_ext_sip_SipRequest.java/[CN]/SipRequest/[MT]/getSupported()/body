{
  List<OptionTag> s=this.supported;
  if (s == null) {
synchronized (this) {
      s=this.supported;
      if (s == null) {
        this.supported=s=new CopyOnWriteArrayList<OptionTag>();
      }
    }
  }
  if (!supportedAdded) {
    String header=getHeaders().getValues(SipConstants.HEADER_SUPPORTED);
    if (header != null) {
      try {
        this.supported.addAll(new OptionTagReader(header).readValues());
      }
 catch (      Exception e) {
        Context.getCurrentLogger().info(e.getMessage());
      }
    }
    supportedAdded=true;
  }
  return s;
}
