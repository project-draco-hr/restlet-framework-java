{
  List<Address> ci=this.callerInfo;
  if (ci == null) {
synchronized (this) {
      ci=this.callerInfo;
      if (ci == null) {
        this.callerInfo=ci=new CopyOnWriteArrayList<Address>();
      }
    }
  }
  if (!callerInfoAdded) {
    String header=getHeaders().getValues(SipConstants.HEADER_CALL_INFO);
    try {
      this.callerInfo.addAll(new AddressReader(header).readValues());
      callerInfoAdded=true;
    }
 catch (    Exception e) {
      Context.getCurrentLogger().info(e.getMessage());
    }
  }
  return ci;
}
