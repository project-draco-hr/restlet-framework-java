{
  if (!referToAdded) {
    String header=getHeaders().getValues(SipConstants.HEADER_REFER_TO);
    if (header != null) {
      try {
        setReferTo(new AddressReader(header).readValue());
      }
 catch (      Exception e) {
        Context.getCurrentLogger().info(e.getMessage());
      }
    }
    referToAdded=true;
  }
  return referTo;
}
