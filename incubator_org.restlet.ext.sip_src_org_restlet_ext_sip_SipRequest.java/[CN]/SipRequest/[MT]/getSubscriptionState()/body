{
  if (!subscriptionStateAdded) {
    String header=getHeaders().getValues(SipConstants.HEADER_SUBSCRIPTION_STATE);
    try {
      setSubscriptionState(new SubscriptionStateReader(header).readValue());
    }
 catch (    Exception e) {
      Context.getCurrentLogger().info(e.getMessage());
    }
  }
  return subscriptionState;
}
