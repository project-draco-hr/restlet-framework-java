{
  try {
synchronized (byteBuffer) {
      byteBuffer.clear();
      int result=byteChannel.read(byteBuffer);
      if (result > 0) {
        byteBuffer.flip();
        ByteArrayInputStream bais=new ByteArrayInputStream(byteBuffer.array(),byteBuffer.arrayOffset(),byteBuffer.remaining());
        onContent(bais);
      }
 else       if (result == -1) {
        onEnd();
      }
 else {
        Context.getCurrentLogger().fine("Input stream selection found with no content available");
      }
    }
  }
 catch (  IOException ioe) {
    onError(ioe);
  }
}
