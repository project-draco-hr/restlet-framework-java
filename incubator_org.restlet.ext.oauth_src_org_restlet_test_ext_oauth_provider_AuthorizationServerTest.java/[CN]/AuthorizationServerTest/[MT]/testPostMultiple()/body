{
  ClientResource cr=new ClientResource(prot + "://localhost:" + serverPort+ "/client/unprotected");
  for (int i=0; i < 50; i++) {
    Form f=new Form();
    f.add("foo","bar");
    Representation r=cr.post(f.getWebRepresentation());
    assertNotNull(r);
    System.out.println("Body = " + r.getText());
  }
  cr.release();
}
