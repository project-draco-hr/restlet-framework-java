{
  org.restlet.resource.Resource result=null;
  if (getTargetClass() != null) {
    try {
      result=(org.restlet.resource.Resource)getTargetClass().newInstance();
    }
 catch (    Exception e) {
      getLogger().log(Level.WARNING,"Exception while instantiating the target resource.",e);
    }
  }
  return result;
}
