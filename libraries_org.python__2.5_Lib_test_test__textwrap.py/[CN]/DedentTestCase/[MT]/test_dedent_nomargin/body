def test_dedent_nomargin(self):
    text = "Hello there.\nHow are you?\nOh good, I'm glad."
    self.assertUnchanged(text)
    text = 'Hello there.\n\nBoo!'
    self.assertUnchanged(text)
    text = 'Hello there.\n  This is indented.'
    self.assertUnchanged(text)
    text = 'Hello there.\n\n  Boo!\n'
    self.assertUnchanged(text)
