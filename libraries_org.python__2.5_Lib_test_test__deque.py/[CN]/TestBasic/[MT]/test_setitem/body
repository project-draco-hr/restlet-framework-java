def test_setitem(self):
    n = 200
    d = deque(xrange(n))
    for i in xrange(n):
        d[i] = (10 * i)
    self.assertEqual(list(d), [(10 * i) for i in xrange(n)])
    l = list(d)
    for i in xrange((1 - n), 0, (-1)):
        d[i] = (7 * i)
        l[i] = (7 * i)
    self.assertEqual(list(d), l)
