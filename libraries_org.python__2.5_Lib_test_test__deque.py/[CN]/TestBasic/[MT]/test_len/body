def test_len(self):
    d = deque('ab')
    self.assertEqual(len(d), 2)
    d.popleft()
    self.assertEqual(len(d), 1)
    d.pop()
    self.assertEqual(len(d), 0)
    self.assertRaises(IndexError, d.pop)
    self.assertEqual(len(d), 0)
    d.append('c')
    self.assertEqual(len(d), 1)
    d.appendleft('d')
    self.assertEqual(len(d), 2)
    d.clear()
    self.assertEqual(len(d), 0)
