def test_copy_pickle(self):
    d = Deque('abc')
    e = d.__copy__()
    self.assertEqual(type(d), type(e))
    self.assertEqual(list(d), list(e))
    e = Deque(d)
    self.assertEqual(type(d), type(e))
    self.assertEqual(list(d), list(e))
    s = pickle.dumps(d)
    e = pickle.loads(s)
    self.assertNotEqual(id(d), id(e))
    self.assertEqual(type(d), type(e))
    self.assertEqual(list(d), list(e))
