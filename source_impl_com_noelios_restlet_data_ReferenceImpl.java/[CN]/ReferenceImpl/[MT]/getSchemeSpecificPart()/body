{
  if (schemeIndex != -1) {
    if (fragmentIndex != -1) {
      return this.uri.substring(schemeIndex + 1,fragmentIndex);
    }
 else {
      return this.uri.substring(schemeIndex + 1);
    }
  }
 else {
    if (fragmentIndex != -1) {
      return this.uri.substring(0,fragmentIndex);
    }
 else {
      return this.uri;
    }
  }
}
