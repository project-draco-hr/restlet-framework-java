def main():
    'Main program when invoked as a script.\n\n    One argument: tabulate a single row.\n    Two arguments: tabulate a range (inclusive).\n    Extra arguments are used to seed the random generator.\n\n    '
    k1 = 15
    k2 = 20
    if sys.argv[1:]:
        k1 = k2 = int(sys.argv[1])
        if sys.argv[2:]:
            k2 = int(sys.argv[2])
            if sys.argv[3:]:
                x = 1
                for a in sys.argv[3:]:
                    x = ((69069 * x) + hash(a))
                random.seed(x)
    r = range(k1, (k2 + 1))
    tabulate(r)
