{
  if ((entity != null) && !getMethod().equals(Method.HEAD.getName())) {
    if (getResponseStream() != null) {
      entity.write(getResponseStream());
    }
 else     if (getResponseChannel() != null) {
      entity.write(getResponseChannel());
    }
  }
  if (getResponseStream() != null) {
    getResponseStream().flush();
  }
}
