{
  final StringBuilder sb=new StringBuilder();
  Expectation expectation;
  for (int i=0; i < expectations.size(); i++) {
    if (i > 0) {
      sb.append(", ");
    }
    expectation=expectations.get(i);
    try {
      format(expectation,sb);
    }
 catch (    IOException e) {
    }
  }
  return sb.toString();
}
