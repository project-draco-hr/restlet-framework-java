def test_unixdll_getnode(self):
    import sys
    print  >> sys.__stdout__, '    WARNING: uuid._unixdll_getnode is unreliable on many platforms.\n        It is disabled until the code and/or test can be fixed properly.'
    return
    import os
    if (importable('ctypes') and (os.name == 'posix')):
        self.check_node(uuid._unixdll_getnode(), 'unixdll')
