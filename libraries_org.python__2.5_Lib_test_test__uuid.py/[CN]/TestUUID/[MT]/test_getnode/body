def test_getnode(self):
    import sys
    print  >> sys.__stdout__, '    WARNING: uuid.getnode is unreliable on many platforms.\n        It is disabled until the code and/or test can be fixed properly.'
    return
    node1 = uuid.getnode()
    self.check_node(node1, 'getnode1')
    node2 = uuid.getnode()
    self.check_node(node2, 'getnode2')
    self.assertEqual(node1, node2)
