def test_ifconfig_getnode(self):
    import sys
    print  >> sys.__stdout__, '    WARNING: uuid._ifconfig_getnode is unreliable on many platforms.\n        It is disabled until the code and/or test can be fixed properly.'
    return
    import os
    if (os.name == 'posix'):
        node = uuid._ifconfig_getnode()
        if (node is not None):
            self.check_node(node, 'ifconfig')
