def _test():
    threads = []
    print 'Creating'
    for i in range(NUM_THREADS):
        t = TempFileGreedy()
        threads.append(t)
        t.start()
    print 'Starting'
    startEvent.set()
    print 'Reaping'
    ok = errors = 0
    for t in threads:
        t.join()
        ok += t.ok_count
        errors += t.error_count
        if t.error_count:
            print ('%s errors:\n%s' % (t.getName(), t.errors.getvalue()))
    msg = ('Done: errors %d ok %d' % (errors, ok))
    print msg
    if errors:
        raise TestFailed(msg)
