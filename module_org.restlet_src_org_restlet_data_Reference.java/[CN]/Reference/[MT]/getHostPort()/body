{
  Logger.getLogger(Reference.class.getCanonicalName()).log(Level.WARNING,"[hostRef,requestUri]=[" + getBaseRef() + ","+ internalRef+ "]");
  int result=-1;
  String authority=getAuthority();
  if (authority != null) {
    int index=authority.indexOf(':');
    if (index != -1) {
      result=Integer.parseInt(authority.substring(index + 1));
    }
  }
  return result;
}
