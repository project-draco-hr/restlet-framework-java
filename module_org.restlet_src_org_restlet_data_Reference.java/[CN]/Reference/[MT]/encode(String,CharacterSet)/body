{
  String result=null;
  try {
    result=URLEncoder.encode(toEncode,characterSet.getName());
  }
 catch (  UnsupportedEncodingException uee) {
    Logger.getLogger(Reference.class.getCanonicalName()).log(Level.WARNING,"Unable to encode the string with the UTF-8 character set.",uee);
  }
  return result;
}
