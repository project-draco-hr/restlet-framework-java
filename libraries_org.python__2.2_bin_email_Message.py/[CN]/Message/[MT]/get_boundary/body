def get_boundary(self, failobj=None):
    "Return the boundary associated with the payload if present.\n\n        The boundary is extracted from the Content-Type header's `boundary'\n        parameter, and it is unquoted.\n        "
    missing = []
    boundary = self.get_param('boundary', missing)
    if (boundary is missing):
        return failobj
    if isinstance(boundary, TupleType):
        charset = (boundary[0] or 'us-ascii')
        return unicode(boundary[2], charset).encode('us-ascii')
    return _unquotevalue(boundary.strip())
