{
  String value=getValue();
  if (value != null) {
    OutputStreamWriter osw=null;
    if (getCharacterSet() != null) {
      osw=new OutputStreamWriter(outputStream,getCharacterSet().getName());
    }
 else {
      osw=new OutputStreamWriter(outputStream);
    }
    osw.write(value);
    osw.flush();
  }
}
