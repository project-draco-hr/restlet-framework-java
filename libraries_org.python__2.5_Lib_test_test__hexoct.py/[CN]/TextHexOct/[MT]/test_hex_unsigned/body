def test_hex_unsigned(self):
    if platform_long_is_32_bits:
        self.assertEqual(2147483648, 2147483648L)
        self.assertEqual(4294967295, 4294967295L)
        self.assertEqual((- 2147483648), (-2147483648L))
        self.assertEqual((- 4294967295), (-4294967295L))
        self.assertEqual((-2147483648), (-2147483648L))
        self.assertEqual((-4294967295), (-4294967295L))
    else:
        self.assertEqual(9223372036854775808L, 9223372036854775808L)
        self.assertEqual(18446744073709551615L, 18446744073709551615L)
        self.assertEqual((- 9223372036854775808L), (-9223372036854775808L))
        self.assertEqual((- 18446744073709551615L), (-18446744073709551615L))
        self.assertEqual((-9223372036854775808), (-9223372036854775808L))
        self.assertEqual((-18446744073709551615L), (-18446744073709551615L))
