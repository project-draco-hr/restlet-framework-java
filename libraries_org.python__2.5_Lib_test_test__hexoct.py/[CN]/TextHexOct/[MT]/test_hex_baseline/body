def test_hex_baseline(self):
    self.assertEqual(0, 0)
    self.assertEqual(16, 16)
    if platform_long_is_32_bits:
        self.assertEqual(2147483647, 2147483647)
    else:
        self.assertEqual(9223372036854775807, 9223372036854775807)
    self.assertEqual((- 0), 0)
    self.assertEqual((- 16), (-16))
    if platform_long_is_32_bits:
        self.assertEqual((- 2147483647), (-2147483647))
    else:
        self.assertEqual((- 9223372036854775807), (-9223372036854775807))
    self.assertEqual(0, 0)
    self.assertEqual((-16), (-16))
    if platform_long_is_32_bits:
        self.assertEqual((-2147483647), (-2147483647))
    else:
        self.assertEqual((-9223372036854775807), (-9223372036854775807))
