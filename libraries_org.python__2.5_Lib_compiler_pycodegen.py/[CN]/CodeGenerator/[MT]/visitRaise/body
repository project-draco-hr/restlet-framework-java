def visitRaise(self, node):
    self.set_lineno(node)
    n = 0
    if node.expr1:
        self.visit(node.expr1)
        n = (n + 1)
    if node.expr2:
        self.visit(node.expr2)
        n = (n + 1)
    if node.expr3:
        self.visit(node.expr3)
        n = (n + 1)
    self.emit('RAISE_VARARGS', n)
