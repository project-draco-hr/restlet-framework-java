{
  String msg="";
  if (securityContext == null)   msg+="\n* securityContext";
  if (uriInfo == null)   msg+="\n* uriInfo";
  if (request == null)   msg+="\n* request";
  if (httpHeaders == null)   msg+="\n* httpHeaders";
  if (msg.length() > 0)   return Response.serverError().entity("missing:" + msg).build();
  return Response.ok(String.valueOf("ok")).build();
}
