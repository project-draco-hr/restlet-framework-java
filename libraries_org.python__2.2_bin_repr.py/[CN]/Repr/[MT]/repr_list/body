def repr_list(self, x, level):
    n = len(x)
    if (n == 0):
        return '[]'
    if (level <= 0):
        return '[...]'
    s = ''
    for i in range(min(n, self.maxlist)):
        if s:
            s = (s + ', ')
        s = (s + self.repr1(x[i], (level - 1)))
    if (n > self.maxlist):
        s = (s + ', ...')
    return (('[' + s) + ']')
