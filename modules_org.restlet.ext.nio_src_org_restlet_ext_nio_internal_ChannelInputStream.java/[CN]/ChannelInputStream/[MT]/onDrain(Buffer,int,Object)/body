{
  int result=0;
  if (args.length == 1) {
    args[0]=getBuffer().drain();
    result=1;
  }
 else   if (args.length == 2) {
    byte[] targetArray=(byte[])args[0];
    int offset=((Integer)args[1]).intValue();
    result=Math.min(maxDrained,getBuffer().remaining());
    getBuffer().drain(targetArray,offset,result);
  }
  return result;
}
