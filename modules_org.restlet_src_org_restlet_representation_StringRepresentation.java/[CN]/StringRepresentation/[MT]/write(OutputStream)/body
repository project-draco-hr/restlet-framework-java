{
  if (getText() != null) {
    java.io.OutputStreamWriter osw=null;
    if (getCharacterSet() != null) {
      osw=new java.io.OutputStreamWriter(outputStream,getCharacterSet().getName());
    }
 else {
      osw=new java.io.OutputStreamWriter(outputStream);
    }
    osw.write(getText());
    osw.flush();
  }
}
