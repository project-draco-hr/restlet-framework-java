def test_float_repr(self):
    self.assertEqual(repr(12345678.000000006), '12345678.000000006')
    self.assertEqual(repr(12345678.0), '12345678.0')
    self.assertEqual(repr((math.pi ** (-100))), ((jython and '1.9275814160560203e-50') or '1.9275814160560206e-50'))
    self.assertEqual(repr((-1.0)), '-1.0')
    self.assertEqual(repr((-9876.54321)), ((jython and '-9876.54321') or '-9876.5432099999998'))
    self.assertEqual(repr(1.23456789e+34), '1.23456789e+34')
