def test_default_case_sensitivity(self):
    cf = self.newconfig({'foo': 'Bar', })
    self.assertEqual(cf.get('DEFAULT', 'Foo'), 'Bar', 'could not locate option, expecting case-insensitive option names')
    cf = self.newconfig({'Foo': 'Bar', })
    self.assertEqual(cf.get('DEFAULT', 'Foo'), 'Bar', 'could not locate option, expecting case-insensitive defaults')
