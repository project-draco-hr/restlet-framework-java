def test_write(self):
    cf = self.fromstring('[Long Line]\nfoo: this line is much, much longer than my editor\n   likes it.\n[DEFAULT]\nfoo: another very\n long line')
    output = StringIO.StringIO()
    cf.write(output)
    self.assertEqual(output.getvalue(), '[DEFAULT]\nfoo = another very\n\tlong line\n\n[Long Line]\nfoo = this line is much, much longer than my editor\n\tlikes it.\n\n')
