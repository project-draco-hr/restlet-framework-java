def read_some(self):
    "Read at least one byte of cooked data unless EOF is hit.\n\n        Return '' if EOF is hit.  Block if no data is immediately\n        available.\n\n        "
    self.process_rawq()
    while ((not self.cookedq) and (not self.eof)):
        self.fill_rawq()
        self.process_rawq()
    buf = self.cookedq
    self.cookedq = ''
    return buf
