{
  List<DocumentationEndPoint> result=new ArrayList<DocumentationEndPoint>();
  for (  Route route : router.getRoutes()) {
    DocumentationEndPoint endPoint=getEndPoint(documentation,route,"/",request,response);
    if (endPoint != null) {
      result.add(endPoint);
    }
  }
  if (router.getDefaultRoute() != null) {
    DocumentationEndPoint endPoint=getEndPoint(documentation,router.getDefaultRoute(),"/",request,response);
    if (endPoint != null) {
      result.add(endPoint);
    }
  }
  return result;
}
