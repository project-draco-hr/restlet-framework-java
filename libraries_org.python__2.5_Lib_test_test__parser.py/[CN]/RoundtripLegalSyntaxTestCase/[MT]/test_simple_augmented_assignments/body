def test_simple_augmented_assignments(self):
    self.check_suite('a += b')
    self.check_suite('a -= b')
    self.check_suite('a *= b')
    self.check_suite('a /= b')
    self.check_suite('a //= b')
    self.check_suite('a %= b')
    self.check_suite('a &= b')
    self.check_suite('a |= b')
    self.check_suite('a ^= b')
    self.check_suite('a <<= b')
    self.check_suite('a >>= b')
    self.check_suite('a **= b')
