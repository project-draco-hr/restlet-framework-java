{
  if (coll instanceof List)   return Collections.unmodifiableList((List<A>)coll);
  if (coll instanceof SortedSet)   return Collections.unmodifiableSortedSet((SortedSet<A>)coll);
  if (coll instanceof Set)   return Collections.unmodifiableSet((Set<A>)coll);
  return Collections.unmodifiableCollection(coll);
}
