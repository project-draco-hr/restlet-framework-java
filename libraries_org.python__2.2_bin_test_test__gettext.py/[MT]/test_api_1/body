def test_api_1(localedir, mofile):
    print 'test api 1'
    os.environ['LANGUAGE'] = 'xx'
    print 'installing gettext'
    gettext.install('gettext', localedir)
    print _('albatross')
    print _(u'mullusk')
    print _('Raymond Luxury Yach-t')
    print _(u'nudge nudge')
    print _('albatross')
    print _(u'mullusk')
    print _('Raymond Luxury Yach-t')
    print _(u'nudge nudge')
    print _('albatross')
    print _(u'mullusk')
    print _('Raymond Luxury Yach-t')
    print _(u'nudge nudge')
    print _('albatross')
    print _(u'mullusk')
    print _('Raymond Luxury Yach-t')
    print _(u'nudge nudge')
    print _('This module provides internationalization and localization\nsupport for your Python programs by providing an interface to the GNU\ngettext message catalog library.')
    fp = open(os.path.join(mofile), 'rb')
    t = gettext.GNUTranslations(fp)
    fp.close()
    t.install()
    print _('nudge nudge')
    t.install(unicode=1)
    print _('mullusk')
