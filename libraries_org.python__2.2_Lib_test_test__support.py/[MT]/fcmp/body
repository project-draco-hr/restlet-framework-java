def fcmp(x, y):
    if ((type(x) == type(0.0)) or (type(y) == type(0.0))):
        try:
            (x, y) = coerce(x, y)
            fuzz = ((abs(x) + abs(y)) * FUZZ)
            if (abs((x - y)) <= fuzz):
                return 0
        except:
            pass
    elif ((type(x) == type(y)) and (type(x) in (type(()), type([])))):
        for i in range(min(len(x), len(y))):
            outcome = fcmp(x[i], y[i])
            if (outcome != 0):
                return outcome
        return cmp(len(x), len(y))
    return cmp(x, y)
