def punycode_decode(text, errors):
    pos = text.rfind('-')
    if (pos == (-1)):
        base = ''
        extended = text
    else:
        base = text[:pos]
        extended = text[(pos + 1):]
    base = unicode(base, 'ascii', errors)
    extended = extended.upper()
    return insertion_sort(base, extended, errors)
