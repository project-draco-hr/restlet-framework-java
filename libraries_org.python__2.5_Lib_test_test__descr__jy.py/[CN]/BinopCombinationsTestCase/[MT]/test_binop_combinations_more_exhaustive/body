def test_binop_combinations_more_exhaustive(self):


    class X(Base):
        pass


    class B1(object):
        pass


    class B2(object):
        pass


    class X1(B1, X, B2):
        pass


    class C1(object):
        pass


    class C2(object):
        pass


    class Y(C1, X1, C2):
        pass
    fail = do_test(X, Y, 'sub', (lambda x, y: (x - y)))
    self.assert_((not fail))
