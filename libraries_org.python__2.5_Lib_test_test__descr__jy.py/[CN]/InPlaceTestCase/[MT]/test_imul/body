def test_imul(self):


    class FooInplace(list):

        def __imul__(self, other):
            return [1]


    class Bar(FooInplace):

        def __mul__(self, other):
            return [2]
    foo = FooInplace()
    foo *= 3
    self.assertEqual(foo, [1])
    foo = Bar([3])
    foo *= 3
    self.assertEqual(foo, [1])


    class Baz(FooInplace):

        def __mul__(self, other):
            return [3]
    baz = Baz()
    baz *= 3
    self.assertEqual(baz, [1])
