def test_other(self):
    for op in ('-', '+', '~'):
        try:
            eval(('%s(old)' % op))
        except AttributeError:
            pass
        else:
            self._assert(False, ('Expected an AttributeError, op: %s' % op))
        try:
            eval(('%s(new)' % op))
        except TypeError:
            pass
        else:
            self._assert(False, ('Expected a TypeError, op: %s' % op))
