def load_module(self, fullname):
    path = fullname.split('.')
    path[(-1)] += '.pyc'
    filename = os.path.join(self.__path, *path)
    f = open(filename, 'rb')
    (magic, mtime) = __readPycHeader(f)
    code = Unmarshaller(f).load()
    if __debugging__:
        print 'Successfully loaded:', fullname
    return __makeModule(fullname, code, filename)
