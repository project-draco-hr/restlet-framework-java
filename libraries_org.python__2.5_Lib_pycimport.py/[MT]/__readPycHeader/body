def __readPycHeader(file):

    def read():
        return ord(file.read(1))
    magic = (read() | (read() << 8))
    if (not ((file.read(1) == '\r') and (file.read(1) == '\n'))):
        raise TypeError('not valid pyc-file')
    mtime = (((read() | (read() << 8)) | (read() << 16)) | (read() << 24))
    return (magic, mtime)
