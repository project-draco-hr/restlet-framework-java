def tabulate(c1=Number, c2=Number):
    for op in operators:
        opfunc = opmap[op]
        print 
        print 'operator:', op
        print 
        print ('%9s' % ''),
        for b in range(3):
            b = c2(b)
            print ('| %9s' % b),
        print '|'
        print ('----------+-' * 4)
        for a in range(3):
            a = c1(a)
            print ('%9s' % a),
            for b in range(3):
                b = c2(b)
                print ('| %9s' % opfunc(a, b)),
            print '|'
        print ('----------+-' * 4)
    print 
    print ('*' * 50)
