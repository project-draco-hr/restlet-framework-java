def savememo(memo, good, bad, skipped):
    f = open(memo, 'w')
    try:
        for (n, l) in [('good', good), ('bad', bad), ('skipped', skipped)]:
            print  >> f, ('%s = [' % n)
            for x in l:
                print  >> f, ('    %r,' % x)
            print  >> f, ' ]'
    finally:
        f.close()
