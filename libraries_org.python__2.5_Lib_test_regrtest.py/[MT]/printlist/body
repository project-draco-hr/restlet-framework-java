def printlist(x, width=70, indent=4):
    'Print the elements of iterable x to stdout.\n\n    Optional arg width (default 70) is the maximum line length.\n    Optional arg indent (default 4) is the number of blanks with which to\n    begin each line.\n    '
    from textwrap import fill
    blanks = (' ' * indent)
    print fill(' '.join(map(str, sorted(x))), width, initial_indent=blanks, subsequent_indent=blanks)
