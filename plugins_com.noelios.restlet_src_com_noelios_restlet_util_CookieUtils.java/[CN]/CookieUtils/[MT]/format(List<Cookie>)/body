{
  StringBuilder sb=new StringBuilder();
  Cookie cookie;
  for (int i=0; i < cookies.size(); i++) {
    cookie=cookies.get(i);
    if (i == 0) {
      if (cookie.getVersion() > 0) {
        sb.append("$Version=\"").append(cookie.getVersion()).append("\"; ");
      }
    }
 else {
      sb.append("; ");
    }
    format(cookie,sb);
  }
  return sb.toString();
}
