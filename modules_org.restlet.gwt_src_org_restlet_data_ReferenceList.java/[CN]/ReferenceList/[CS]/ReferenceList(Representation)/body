{
  final BufferedReader lr=new BufferedReader(new StringReader(uriList.getText()));
  String line=lr.readLine();
  if ((line != null) && line.startsWith("#")) {
    setIdentifier(new Reference(line.substring(1).trim()));
    line=lr.readLine();
  }
  while (line != null) {
    if (!line.startsWith("#")) {
      add(new Reference(line.trim()));
    }
    line=lr.readLine();
  }
}
