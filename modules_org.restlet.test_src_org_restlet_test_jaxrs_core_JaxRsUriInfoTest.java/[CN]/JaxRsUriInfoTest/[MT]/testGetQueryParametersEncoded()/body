{
  assertEquals("123",URI_INFO_7.getQueryParameters(false).getFirst("def"));
  assertEquals("456",URI_INFO_7.getQueryParameters(false).getFirst("ghi"));
  assertEquals(2,URI_INFO_7.getQueryParameters(false).size());
  assertEquals("1+23",URI_INFO_8.getQueryParameters(false).getFirst("def"));
  assertEquals("45%206",URI_INFO_8.getQueryParameters(false).getFirst("gh%20i"));
  assertEquals(2,URI_INFO_8.getQueryParameters(false).size());
}
