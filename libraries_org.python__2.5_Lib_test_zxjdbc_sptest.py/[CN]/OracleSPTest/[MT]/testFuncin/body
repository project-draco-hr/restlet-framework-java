def testFuncin(self):
    c = self.cursor()
    try:
        params = ['testing']
        c.callproc('funcin', params)
        self.assertEquals(1, c.rowcount)
        data = c.fetchone()
        assert (data is not None), 'data was None'
        self.assertEquals(1, len(data))
        self.assertEquals('testingtesting', data[0])
    finally:
        c.close()
