def testFuncout(self):
    c = self.cursor()
    try:
        params = [None]
        c.callproc('funcout', params)
        data = c.fetchone()
        assert (data is not None), 'data was None'
        self.assertEquals(1, len(data))
        self.assertEquals('returned', data[0])
        self.assertEquals('tested', params[0].strip())
    finally:
        c.close()
