{
  if (getWrappedRequest() != null) {
    return getWrappedRequest().isEntityAvailable();
  }
 else {
    if (getMethod().equals(Method.GET) || getMethod().equals(Method.HEAD) || getMethod().equals(Method.DELETE)) {
      return false;
    }
 else {
      return super.isEntityAvailable();
    }
  }
}
