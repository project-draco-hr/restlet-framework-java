{
  MediaType restletMediaType=Converter.toRestletMediaType(mediaType);
  List<MessageBodyWriter> mbws=this.messageBodyWriters;
  for (  MessageBodyWriter mbw : mbws) {
    if (mbw.supportsWrite(restletMediaType))     if (mbw.isWriteable(type,genericType,annotations))     return (javax.ws.rs.ext.MessageBodyWriter<T>)mbw.getJaxRsWriter();
  }
  return null;
}
