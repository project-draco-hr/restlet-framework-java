{
  int result=-1;
  int wrappedRead=0;
  if (this.availableSize > 0) {
    if (this.availableSize < dst.remaining()) {
      dst.limit((int)(this.availableSize + dst.position()));
    }
    wrappedRead=getWrappedChannel().read(dst);
    result=wrappedRead;
  }
  if (result > 0) {
    this.availableSize-=result;
    if (Context.getCurrentLogger().isLoggable(Level.FINER)) {
      Context.getCurrentLogger().finer("Bytes (read | available) : " + result + " | "+ this.availableSize);
    }
    if (this.availableSize == 0) {
      if (Context.getCurrentLogger().isLoggable(Level.FINER)) {
        Context.getCurrentLogger().finer("Channel fully read.");
      }
    }
  }
  if ((result == -1) && (getWrappedChannel() instanceof CompletionListener)) {
    ((CompletionListener)getWrappedChannel()).onCompleted((wrappedRead == -1));
  }
  return result;
}
