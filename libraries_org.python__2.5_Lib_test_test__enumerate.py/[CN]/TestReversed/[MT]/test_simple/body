def test_simple(self):


    class A:

        def __getitem__(self, i):
            if (i < 5):
                return str(i)
            raise StopIteration

        def __len__(self):
            return 5
    for data in ('abc', range(5), tuple(enumerate('abc')), A(), xrange(1, 17, 5)):
        self.assertEqual(list(data)[::(-1)], list(reversed(data)))
    self.assertRaises(TypeError, reversed, {})
