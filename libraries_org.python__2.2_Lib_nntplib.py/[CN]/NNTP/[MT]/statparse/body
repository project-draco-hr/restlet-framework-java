def statparse(self, resp):
    'Internal: parse the response of a STAT, NEXT or LAST command.'
    if (resp[:2] != '22'):
        raise NNTPReplyError(resp)
    words = resp.split()
    nr = 0
    id = ''
    n = len(words)
    if (n > 1):
        nr = words[1]
        if (n > 2):
            id = words[2]
    return (resp, nr, id)
