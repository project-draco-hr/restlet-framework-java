def test_convert(self):
    self.assertRaises(TypeError, bool, 42, 42)
    self.assertIs(bool(10), True)
    self.assertIs(bool(1), True)
    self.assertIs(bool((-1)), True)
    self.assertIs(bool(0), False)
    self.assertIs(bool('hello'), True)
    self.assertIs(bool(''), False)
    self.assertIs(bool(), False)
