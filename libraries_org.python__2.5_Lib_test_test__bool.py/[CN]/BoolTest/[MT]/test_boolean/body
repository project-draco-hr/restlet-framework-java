def test_boolean(self):
    self.assertEqual((True & 1), 1)
    self.assert_((not isinstance((True & 1), bool)))
    self.assertIs((True & True), True)
    self.assertEqual((True | 1), 1)
    self.assert_((not isinstance((True | 1), bool)))
    self.assertIs((True | True), True)
    self.assertEqual((True ^ 1), 0)
    self.assert_((not isinstance((True ^ 1), bool)))
    self.assertIs((True ^ True), False)
