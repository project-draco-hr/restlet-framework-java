{
  String result=null;
  try {
    StringBuilder sb=new StringBuilder();
    InputStream is=new BufferedInputStream(inputStream);
    int nextByte=is.read();
    while (nextByte != -1) {
      sb.append((char)nextByte);
      nextByte=is.read();
    }
    is.close();
    result=sb.toString();
  }
 catch (  Exception e) {
  }
  return result;
}
