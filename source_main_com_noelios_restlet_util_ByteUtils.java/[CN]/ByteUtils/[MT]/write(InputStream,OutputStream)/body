{
  InputStream is=(inputStream instanceof BufferedInputStream) ? inputStream : new BufferedInputStream(inputStream);
  OutputStream os=(outputStream instanceof BufferedOutputStream) ? outputStream : new BufferedOutputStream(outputStream);
  int nextByte=is.read();
  while (nextByte != -1) {
    os.write(nextByte);
    nextByte=is.read();
  }
  is.close();
}
