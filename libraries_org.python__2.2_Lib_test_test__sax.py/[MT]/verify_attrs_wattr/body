def verify_attrs_wattr(attrs):
    return ((attrs.getLength() == 1) and (attrs.getNames() == ['attr']) and (attrs.getQNames() == ['attr']) and (len(attrs) == 1) and attrs.has_key('attr') and (attrs.keys() == ['attr']) and (attrs.get('attr') == 'val') and (attrs.get('attr', 25) == 'val') and (attrs.items() == [('attr', 'val')]) and (attrs.values() == ['val']) and (attrs.getValue('attr') == 'val') and (attrs.getValueByQName('attr') == 'val') and (attrs.getNameByQName('attr') == 'attr') and (attrs['attr'] == 'val') and (attrs.getQNameByName('attr') == 'attr'))
