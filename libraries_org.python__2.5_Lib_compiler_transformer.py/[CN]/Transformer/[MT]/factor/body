def factor(self, nodelist):
    elt = nodelist[0]
    t = elt[0]
    node = self.lookup_node(nodelist[(-1)])(nodelist[(-1)][1:])
    if (t == token.PLUS):
        return UnaryAdd(node, lineno=elt[2])
    elif (t == token.MINUS):
        return UnarySub(node, lineno=elt[2])
    elif (t == token.TILDE):
        node = Invert(node, lineno=elt[2])
    return node
