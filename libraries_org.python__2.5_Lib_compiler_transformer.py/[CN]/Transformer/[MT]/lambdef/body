def lambdef(self, nodelist):
    if (nodelist[2][0] == symbol.varargslist):
        (names, defaults, flags) = self.com_arglist(nodelist[2][1:])
    else:
        names = defaults = ()
        flags = 0
    code = self.com_node(nodelist[(-1)])
    return Lambda(names, defaults, flags, code, lineno=nodelist[1][2])
