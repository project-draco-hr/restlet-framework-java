def exec_stmt(self, nodelist):
    expr1 = self.com_node(nodelist[1])
    if (len(nodelist) >= 4):
        expr2 = self.com_node(nodelist[3])
        if (len(nodelist) >= 6):
            expr3 = self.com_node(nodelist[5])
        else:
            expr3 = None
    else:
        expr2 = expr3 = None
    return Exec(expr1, expr2, expr3, lineno=nodelist[0][2])
