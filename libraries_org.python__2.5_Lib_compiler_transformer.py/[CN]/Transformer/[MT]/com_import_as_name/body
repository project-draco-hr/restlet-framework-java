def com_import_as_name(self, node):
    assert (node[0] == symbol.import_as_name)
    node = node[1:]
    assert (node[0][0] == token.NAME)
    if (len(node) == 1):
        return (node[0][1], None)
    assert (node[1][1] == 'as'), node
    assert (node[2][0] == token.NAME)
    return (node[0][1], node[2][1])
