def raise_stmt(self, nodelist):
    if (len(nodelist) > 5):
        expr3 = self.com_node(nodelist[5])
    else:
        expr3 = None
    if (len(nodelist) > 3):
        expr2 = self.com_node(nodelist[3])
    else:
        expr2 = None
    if (len(nodelist) > 1):
        expr1 = self.com_node(nodelist[1])
    else:
        expr1 = None
    return Raise(expr1, expr2, expr3, lineno=nodelist[0][2])
