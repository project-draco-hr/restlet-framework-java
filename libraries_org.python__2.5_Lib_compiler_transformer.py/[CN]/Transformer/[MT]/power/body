def power(self, nodelist):
    node = self.com_node(nodelist[0])
    for i in range(1, len(nodelist)):
        elt = nodelist[i]
        if (elt[0] == token.DOUBLESTAR):
            return Power([node, self.com_node(nodelist[(i + 1)])], lineno=elt[2])
        node = self.com_apply_trailer(node, elt)
    return node
