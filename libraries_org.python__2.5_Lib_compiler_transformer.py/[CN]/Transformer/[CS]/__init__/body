def __init__(self):
    self._dispatch = {}
    for (value, name) in symbol.sym_name.items():
        if hasattr(self, name):
            self._dispatch[value] = getattr(self, name)
    self._dispatch[token.NEWLINE] = self.com_NEWLINE
    self._atom_dispatch = {token.LPAR: self.atom_lpar, token.LSQB: self.atom_lsqb, token.LBRACE: self.atom_lbrace, token.BACKQUOTE: self.atom_backquote, token.NUMBER: self.atom_number, token.STRING: self.atom_string, token.NAME: self.atom_name, }
    self.encoding = None
