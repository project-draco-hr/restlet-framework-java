def test_neg(self):
    self.failUnlessRaises(TypeError, operator.neg)
    self.failUnlessRaises(TypeError, operator.neg, None)
    self.failUnless((operator.neg(5) == (-5)))
    self.failUnless((operator.neg((-5)) == 5))
    self.failUnless((operator.neg(0) == 0))
    self.failUnless((operator.neg(0) == 0))
