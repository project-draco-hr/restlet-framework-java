def test_isCallable(self):
    self.failUnlessRaises(TypeError, operator.isCallable)


    class C:
        pass

    def check(self, o, v):
        self.assert_((operator.isCallable(o) == callable(o) == v))
    check(self, 4, 0)
    check(self, operator.isCallable, 1)
    check(self, C, 1)
    check(self, C(), 0)
