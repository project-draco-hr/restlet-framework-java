def test_pos(self):
    self.failUnlessRaises(TypeError, operator.pos)
    self.failUnlessRaises(TypeError, operator.pos, None)
    self.failUnless((operator.pos(5) == 5))
    self.failUnless((operator.pos((-5)) == (-5)))
    self.failUnless((operator.pos(0) == 0))
    self.failUnless((operator.pos(0) == 0))
