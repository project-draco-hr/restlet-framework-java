def __init__(self, index, parser, url, good, agent):
    unittest.TestCase.__init__(self)
    if good:
        self.str = ('RobotTest(%d, good, %s)' % (index, url))
    else:
        self.str = ('RobotTest(%d, bad, %s)' % (index, url))
    self.parser = parser
    self.url = url
    self.good = good
    self.agent = agent
