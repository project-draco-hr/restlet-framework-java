def test_buffer_artefacts(self):
    output = [('starttag', 'a', [('b', '<')])]
    self._run_check(["<a b='<'>"], output)
    self._run_check(['<a ', "b='<'>"], output)
    self._run_check(['<a b', "='<'>"], output)
    self._run_check(['<a b=', "'<'>"], output)
    self._run_check(["<a b='<", "'>"], output)
    self._run_check(["<a b='<'", '>'], output)
    output = [('starttag', 'a', [('b', '>')])]
    self._run_check(["<a b='>'>"], output)
    self._run_check(['<a ', "b='>'>"], output)
    self._run_check(['<a b', "='>'>"], output)
    self._run_check(['<a b=', "'>'>"], output)
    self._run_check(["<a b='>", "'>"], output)
    self._run_check(["<a b='>'", '>'], output)
