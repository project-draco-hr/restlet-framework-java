{
  this.networkHandler=new DefaultNetworkHandler();
  getNetworkHandler().start();
  this.peerGroup=getNetworkHandler().getNetwork().getNetPeerGroup();
  this.pipeAdvertisement=PipeUtility.createPipeAdvertisement(getName(),PipeService.UnicastType,this.peerGroup,PipeUtility.createPipeID(this.peerGroup));
  super.start();
}
