{
  Field hpAnnotated=UtilTests.class.getDeclaredField("hpAnnotated");
  assertTrue(Util.isAnnotationPresentExtended(hpAnnotated,HeaderParam.class));
  assertTrue(Util.isAnnotationPresentExtended(hpAnnotated,javax.ws.rs.HeaderParam.class));
  assertFalse(Util.isAnnotationPresentExtended(hpAnnotated,CookieParam.class));
  assertFalse(Util.isAnnotationPresentExtended(hpAnnotated,javax.ws.rs.CookieParam.class));
}
