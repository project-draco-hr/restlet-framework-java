{
  super(context,ChallengeScheme.HTTP_BASIC,"realm");
  authenticationSettings=new AuthenticationSettings();
  authenticateClientResource=AgentUtils.getClientResource(agentConfig,modulesSettings,AuthenticationAuthenticateResource.class,AUTHENTICATE_PATH);
  setOptional(authenticationSettings.isOptional());
  setVerifier(new AgentVerifier());
  initializeCache();
}
