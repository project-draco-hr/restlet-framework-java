def print_line(self, func):
    (cc, nc, tt, ct, callers) = self.stats[func]
    c = str(nc)
    if (nc != cc):
        c = ((c + '/') + str(cc))
    print c.rjust(9),
    print f8(tt),
    if (nc == 0):
        print (' ' * 8),
    else:
        print f8((tt / nc)),
    print f8(ct),
    if (cc == 0):
        print (' ' * 8),
    else:
        print f8((ct / cc)),
    print func_std_string(func)
