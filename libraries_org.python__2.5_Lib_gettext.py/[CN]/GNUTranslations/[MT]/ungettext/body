def ungettext(self, msgid1, msgid2, n):
    try:
        tmsg = self._catalog[(msgid1, self.plural(n))]
    except KeyError:
        if self._fallback:
            return self._fallback.ungettext(msgid1, msgid2, n)
        if (n == 1):
            tmsg = unicode(msgid1)
        else:
            tmsg = unicode(msgid2)
    return tmsg
