{
  Route[] routes=toArray();
  int j=new Random().nextInt(routes.length);
  if (routes[j].score(request,response) >= requiredScore)   return routes[j];
  for (int initialIndex=j++; initialIndex != j; j++) {
    if (j == routes.length) {
      j=0;
    }
    if (routes[j].score(request,response) >= requiredScore)     return routes[j];
  }
  return null;
}
