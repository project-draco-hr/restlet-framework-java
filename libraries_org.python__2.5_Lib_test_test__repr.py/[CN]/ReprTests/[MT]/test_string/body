def test_string(self):
    eq = self.assertEquals
    eq(r('abc'), "'abc'")
    eq(r('abcdefghijklmnop'), "'abcdefghijklmnop'")
    s = (('a' * 30) + ('b' * 30))
    expected = ((repr(s)[:13] + '...') + repr(s)[(-14):])
    eq(r(s), expected)
    eq(r('"\''), repr('"\''))
    s = (('"' * 30) + ("'" * 100))
    expected = ((repr(s)[:13] + '...') + repr(s)[(-14):])
    eq(r(s), expected)
