def _isnan(self):
    'Returns whether the number is not actually one.\n\n        0 if a number\n        1 if NaN\n        2 if sNaN\n        '
    if self._is_special:
        exp = self._exp
        if (exp == 'n'):
            return 1
        elif (exp == 'N'):
            return 2
    return 0
