def __neg__(self, context=None):
    'Returns a copy with the sign switched.\n\n        Rounds, if it has reason.\n        '
    if self._is_special:
        ans = self._check_nans(context=context)
        if ans:
            return ans
    if (not self):
        ans = self.copy_abs()
    else:
        ans = self.copy_negate()
    if (context is None):
        context = getcontext()
    return ans._fix(context)
