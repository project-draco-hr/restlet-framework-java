{
  super(context,request,response);
  this.currentUser=(User)getRequest().getAttributes().get(RmepGuard.CURRENT_USER);
  if (this.currentUser == null) {
    if (getRequest().getChallengeResponse() != null) {
      this.currentUser=getObjectsFacade().getUserByLoginPwd(getRequest().getChallengeResponse().getIdentifier(),getRequest().getChallengeResponse().getSecret());
    }
  }
}
