def enter(self):
    (checkin, checkout) = (self.checkin, self.checkout)
    checkin.acquire()
    self.waiting = (self.waiting + 1)
    if (self.waiting == self.n):
        self.waiting = (self.n - 1)
        checkout.release()
        return
    checkin.release()
    checkout.acquire()
    self.waiting = (self.waiting - 1)
    if (self.waiting == 0):
        checkin.release()
        return
    checkout.release()
