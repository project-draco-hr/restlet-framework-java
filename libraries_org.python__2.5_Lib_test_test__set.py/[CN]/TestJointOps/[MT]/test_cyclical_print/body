def test_cyclical_print(self):
    w = ReprWrapper()
    s = self.thetype([w])
    w.value = s
    try:
        fo = open(test_support.TESTFN, 'wb')
        print  >> fo, s,
        fo.close()
        fo = open(test_support.TESTFN, 'rb')
        self.assertEqual(fo.read(), repr(s))
    finally:
        fo.close()
        os.remove(test_support.TESTFN)
