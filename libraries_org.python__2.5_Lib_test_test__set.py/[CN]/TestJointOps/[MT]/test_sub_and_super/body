def test_sub_and_super(self):
    (p, q, r) = map(self.thetype, ['ab', 'abcde', 'def'])
    self.assert_((p < q))
    self.assert_((p <= q))
    self.assert_((q <= q))
    self.assert_((q > p))
    self.assert_((q >= p))
    self.failIf((q < r))
    self.failIf((q <= r))
    self.failIf((q > r))
    self.failIf((q >= r))
    self.assert_(set('a').issubset('abc'))
    self.assert_(set('abc').issuperset('a'))
    self.failIf(set('a').issubset('cbs'))
    self.failIf(set('cbs').issuperset('a'))
