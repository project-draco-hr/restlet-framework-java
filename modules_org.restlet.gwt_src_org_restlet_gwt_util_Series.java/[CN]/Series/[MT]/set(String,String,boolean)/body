{
  E result=null;
  E param=null;
  boolean found=false;
  for (Iterator<E> iter=iterator(); iter.hasNext(); ) {
    param=iter.next();
    if (equals(param.getName(),name,ignoreCase)) {
      if (found) {
        iter.remove();
      }
 else {
        found=true;
        param.setValue(value);
        result=param;
      }
    }
  }
  if (!found) {
    add(name,value);
  }
  return result;
}
