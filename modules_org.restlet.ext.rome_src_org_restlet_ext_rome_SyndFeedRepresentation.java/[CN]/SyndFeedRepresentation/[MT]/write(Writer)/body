{
  try {
    SyndFeedOutput output=new SyndFeedOutput();
    if (this.feed.getFeedType() == null) {
      this.feed.setFeedType("atom_1.0");
    }
    output.output(this.feed,writer);
  }
 catch (  FeedException e) {
    IOException ioe=new IOException("Feed exception");
    ioe.initCause(e);
    throw ioe;
  }
}
