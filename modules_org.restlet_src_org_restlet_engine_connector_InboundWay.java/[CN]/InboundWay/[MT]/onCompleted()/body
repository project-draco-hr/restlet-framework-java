{
  if (getLogger().isLoggable(Level.FINER)) {
    getLogger().finer("Inbound message fully received");
  }
  if (HeaderUtils.isConnectionClose(getHeaders())) {
    getConnection().setState(ConnectionState.CLOSING);
  }
  if (getHeaders() != null) {
    setHeaders(null);
  }
  super.onCompleted();
}
