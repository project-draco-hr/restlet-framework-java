{
  int result=0;
  if ((getMessageState() == MessageState.BODY) && (getEntityRegistration() != null) && (getEntityRegistration().getListener() != null)) {
    result=getEntityRegistration().getInterestOperations();
  }
 else   if (getIoState() == IoState.INTEREST) {
    result=SelectionKey.OP_READ;
  }
  return result;
}
