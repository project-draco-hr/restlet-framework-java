{
  ReadableByteChannel result=null;
  if (getByteBuffer().hasRemaining()) {
    if (chunked) {
      getLogger().warning("Chunked encoding not supported (yet) in the NIO connector.");
    }
 else {
      result=new ReadableEntityChannel(this,getByteBuffer(),getConnection().getReadableSelectionChannel(),size);
    }
  }
 else {
    result=getConnection().getReadableSelectionChannel();
  }
  return result;
}
