{
  HttpServerHelper http=new HttpServerHelper(null);
  try {
    http.start();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  System.out.println("sleeping");
  try {
    Thread.sleep(8000);
  }
 catch (  Exception e) {
  }
  System.out.println("waking");
  try {
    http.stop();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  System.out.println("exiting");
}
