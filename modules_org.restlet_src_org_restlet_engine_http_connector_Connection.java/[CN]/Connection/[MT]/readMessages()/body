{
  try {
    boolean doRead=false;
synchronized (this) {
      if (canRead()) {
        doRead=true;
        setInboundBusy(true);
      }
    }
    if (doRead) {
      readMessage();
    }
  }
 catch (  Throwable e) {
    getLogger().log(Level.WARNING,"Error while reading a message. Closing the connection.",e.getMessage());
    getLogger().log(Level.FINE,"Error while reading a message. Closing the connection.",e);
    close(false);
  }
  getHelper().handleNextInbound();
}
