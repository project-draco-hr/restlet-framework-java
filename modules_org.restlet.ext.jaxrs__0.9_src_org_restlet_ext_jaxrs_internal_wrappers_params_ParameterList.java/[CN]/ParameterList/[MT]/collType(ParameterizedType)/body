{
  Type rawType=type.getRawType();
  if (rawType.equals(List.class))   return (Class)ArrayList.class;
 else   if (rawType.equals(Set.class))   return (Class)HashSet.class;
 else   if (rawType.equals(SortedSet.class))   return (Class)TreeSet.class;
 else   if (rawType.equals(Collection.class)) {
    localLogger.config(ParameterList.COLL_PARAM_NOT_DEFAULT);
    return (Class)ArrayList.class;
  }
  return null;
}
