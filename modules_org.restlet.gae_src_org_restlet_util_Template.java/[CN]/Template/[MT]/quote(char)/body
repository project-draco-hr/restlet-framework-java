{
switch (character) {
case '[':
    return "\\[";
case ']':
  return "\\]";
case '.':
return "\\.";
case '\\':
return "\\\\";
case '$':
return "\\$";
case '^':
return "\\^";
case '?':
return "\\?";
case '*':
return "\\*";
case '|':
return "\\|";
case '(':
return "\\(";
case ')':
return "\\)";
case ':':
return "\\:";
case '-':
return "\\-";
case '!':
return "\\!";
case '<':
return "\\<";
case '>':
return "\\>";
default :
return Character.toString(character);
}
}
