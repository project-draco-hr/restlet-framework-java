{
  Property result=null;
  if (entity != null) {
    EntityType type=getEntityType(entity.getClass());
    if (type != null) {
      for (      Property property : type.getProperties()) {
        if (property.getName().equals(propertyName) || property.getNormalizedName().equals(propertyName)) {
          result=property;
          break;
        }
      }
    }
  }
  return result;
}
