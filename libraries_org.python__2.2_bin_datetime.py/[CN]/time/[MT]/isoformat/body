def isoformat(self):
    "Return the time formatted according to ISO.\n\n        This is 'HH:MM:SS.mmmmmm+zz:zz', or 'HH:MM:SS+zz:zz' if\n        self.microsecond == 0.\n        "
    s = _format_time(self.__hour, self.__minute, self.__second, self.__microsecond)
    tz = self._tzstr()
    if tz:
        s += tz
    return s
