{
  if (csvRepresentation != null) {
    csvRepresentation.write(writer);
  }
 else   if (object != null) {
    for (    Method method : object.getClass().getMethods()) {
      if (method.getName().startsWith("get")) {
      }
    }
    new CsvBeanWriter(writer,getCsvPreference()).write(object);
  }
}
