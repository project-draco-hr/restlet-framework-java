{
  if (message instanceof SipRequest) {
    SipRequest sipRequest=(SipRequest)message;
    if (!sipRequest.getSipRecipientsInfo().isEmpty()) {
      for (      SipRecipientInfo recipient : sipRequest.getSipRecipientsInfo()) {
        headers.add(HeaderConstants.HEADER_VIA,SipRecipientInfoWriter.write(recipient));
      }
    }
  }
  super.addGeneralHeaders(message,headers);
}
