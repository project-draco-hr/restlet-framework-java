def setUp(self):
    self.tempdir = (TESTFN + '_dir')
    self.mktemp('a', 'D')
    self.mktemp('aab', 'F')
    self.mktemp('aaa', 'zzzF')
    self.mktemp('ZZZ')
    self.mktemp('a', 'bcd', 'EF')
    self.mktemp('a', 'bcd', 'efg', 'ha')
    if hasattr(os, 'symlink'):
        os.symlink(self.norm('broken'), self.norm('sym1'))
        os.symlink(self.norm('broken'), self.norm('sym2'))
