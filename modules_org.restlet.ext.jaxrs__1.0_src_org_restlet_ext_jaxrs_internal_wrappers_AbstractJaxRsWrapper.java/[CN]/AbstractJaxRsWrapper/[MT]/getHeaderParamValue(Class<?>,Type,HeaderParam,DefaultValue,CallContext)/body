{
  Form httpHeaders=Util.getHttpHeaders(callContext.getRequest());
  String headerName=annotation.value();
  try {
    return convertParamValuesFromParam(paramClass,paramGenericType,new ParamValueIter(httpHeaders.subList(headerName,true)),getValue(httpHeaders.getFirst(headerName,true)),defaultValue,true);
  }
 catch (  ConvertParameterException e) {
    throw new ConvertHeaderParamException(e);
  }
}
