{
  String pathParamValue=callContext.getLastPathParamEnc(pathParam);
  Iterator<String> pathParamValueIter=callContext.pathParamEncIter(pathParam);
  try {
    return convertParamValuesFromParam(paramClass,paramGenericType,pathParamValueIter,pathParamValue,defaultValue,leaveEncoded);
  }
 catch (  ConvertParameterException e) {
    throw new ConvertPathParamException(e);
  }
}
