{
  final String authenticate1="Basic realm=\"Restlet tutorial\"";
  final String authorization1="Basic c2NvdHQ6dGlnZXI=";
  assertEquals(authorization1,AuthenticatorUtils.formatResponse(AuthenticatorUtils.parseResponse(null,authorization1),null,null));
  assertEquals(authenticate1,AuthenticatorUtils.formatRequest(AuthenticatorUtils.parseRequest(authenticate1),null,null));
}
