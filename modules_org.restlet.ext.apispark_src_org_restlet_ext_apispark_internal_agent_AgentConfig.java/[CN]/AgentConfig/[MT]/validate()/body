{
  if (cell == null) {
    throw new IllegalArgumentException("The cell identifier is mandatory");
  }
  if (cellVersion == null) {
    throw new IllegalArgumentException("The cell version identifier is mandatory");
  }
  if (StringUtils.isNullOrEmpty(agentServiceUrl)) {
    throw new IllegalArgumentException("The agent service url is mandatory");
  }
  if (StringUtils.isNullOrEmpty(agentUsername)) {
    throw new IllegalArgumentException("The agent username is mandatory");
  }
  if (agentSecret == null || agentSecret.length == 0) {
    throw new IllegalArgumentException("The agent secret key is mandatory");
  }
  if (redirectionEnabled && StringUtils.isNullOrEmpty(redirectionUrl)) {
    throw new IllegalArgumentException("The redirection url is mandatory when redirection is enabled");
  }
}
