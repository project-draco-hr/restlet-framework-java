{
  CookieReader cr=new CookieReader(Logger.getLogger(CookiesTestCase.class.getCanonicalName()),headerValue);
  List<Cookie> cookies=new ArrayList<Cookie>();
  Cookie cookie=cr.readCookie();
  while (cookie != null) {
    cookies.add(cookie);
    cookie=cr.readCookie();
  }
  String newHeaderValue=CookieUtils.format(cookies);
  assertEquals(headerValue,newHeaderValue);
}
