def test_alias_nofallback(self):
    if _have_soundcard():
        try:
            winsound.PlaySound('!"$%&/(#+*', (winsound.SND_ALIAS | winsound.SND_NODEFAULT))
        except RuntimeError:
            pass
    else:
        self.assertRaises(RuntimeError, winsound.PlaySound, '!"$%&/(#+*', (winsound.SND_ALIAS | winsound.SND_NODEFAULT))
