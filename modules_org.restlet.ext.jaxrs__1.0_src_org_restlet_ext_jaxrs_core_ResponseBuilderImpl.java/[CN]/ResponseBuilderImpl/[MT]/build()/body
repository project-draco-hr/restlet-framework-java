{
  if (this.response == null)   return new ResponseImpl();
  Response r=this.response;
  if (this.newCookies != null) {
    MultivaluedMap<String,Object> metadata=getMetadata();
    for (    NewCookie cookie : this.newCookies.values())     metadata.putSingle(HttpHeaders.SET_COOKIE,cookie);
    this.newCookies=null;
  }
  this.response=null;
  this.metadata=null;
  return r;
}
