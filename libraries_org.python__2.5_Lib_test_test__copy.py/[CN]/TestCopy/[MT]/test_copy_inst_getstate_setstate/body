def test_copy_inst_getstate_setstate(self):


    class C:

        def __init__(self, foo):
            self.foo = foo

        def __getstate__(self):
            return self.foo

        def __setstate__(self, state):
            self.foo = state

        def __cmp__(self, other):
            return cmp(self.foo, other.foo)
    x = C(42)
    self.assertEqual(copy.copy(x), x)
