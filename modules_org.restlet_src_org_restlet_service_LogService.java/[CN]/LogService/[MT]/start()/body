{
  super.start();
  this.logTemplate=(getLogFormat() == null) ? null : new Template(getLogFormat());
  if (getLogPropertiesRef() != null) {
    Representation logProperties=new ClientResource(getContext(),getLogPropertiesRef()).get();
    if (logProperties != null) {
      LogManager.getLogManager().readConfiguration(logProperties.getStream());
    }
  }
}
