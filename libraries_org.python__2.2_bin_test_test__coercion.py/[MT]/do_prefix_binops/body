def do_prefix_binops():
    for a in candidates:
        for b in candidates:
            for op in prefix_binops:
                print ('%s(%s, %s)' % (op, a, b)),
                try:
                    x = eval(('%s(a, b)' % op))
                except:
                    error = sys.exc_info()[:2]
                    print ('... %s' % error[0])
                else:
                    print '=', x
