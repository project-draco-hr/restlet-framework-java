def do_alias(self, arg):
    args = arg.split()
    if (len(args) == 0):
        keys = self.aliases.keys()
        keys.sort()
        for alias in keys:
            print ('%s = %s' % (alias, self.aliases[alias]))
        return
    if (self.aliases.has_key(args[0]) and (len(args) == 1)):
        print ('%s = %s' % (args[0], self.aliases[args[0]]))
    else:
        self.aliases[args[0]] = ' '.join(args[1:])
