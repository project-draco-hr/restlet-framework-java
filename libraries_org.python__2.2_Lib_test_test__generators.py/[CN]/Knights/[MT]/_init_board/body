def _init_board(self):
    succs = self.succs
    del succs[:]
    (m, n) = (self.m, self.n)
    c2i = self.coords2index
    offsets = [(1, 2), (2, 1), (2, (-1)), (1, (-2)), ((-1), (-2)), ((-2), (-1)), ((-2), 1), ((-1), 2)]
    rangen = range(n)
    for i in range(m):
        for j in rangen:
            s = [c2i((i + io), (j + jo)) for (io, jo) in offsets if ((0 <= (i + io) < m) and (0 <= (j + jo) < n))]
            succs.append(s)
