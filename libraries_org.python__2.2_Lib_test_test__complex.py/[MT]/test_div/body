def test_div(x, y):
    'Compute complex z=x*y, and check that z/x==y and z/y==x.'
    global nerrors
    z = (x * y)
    if (x != 0):
        q = (z / x)
        if (not check_close(q, y)):
            nerrors += 1
            print ('%r / %r == %r but expected %r' % (z, x, q, y))
    if (y != 0):
        q = (z / y)
        if (not check_close(q, x)):
            nerrors += 1
            print ('%r / %r == %r but expected %r' % (z, y, q, x))
