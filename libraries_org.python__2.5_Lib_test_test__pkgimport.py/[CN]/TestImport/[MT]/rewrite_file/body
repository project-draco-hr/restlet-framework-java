def rewrite_file(self, contents):
    if is_jython:
        compiled_path = (self.module_path.replace('.', '$') + '.class')
        if os.path.exists(compiled_path):
            os.remove(compiled_path)
    else:
        for extension in 'co':
            compiled_path = (self.module_path + extension)
            if os.path.exists(compiled_path):
                os.remove(compiled_path)
    f = open(self.module_path, 'w')
    f.write(contents)
    f.close()
