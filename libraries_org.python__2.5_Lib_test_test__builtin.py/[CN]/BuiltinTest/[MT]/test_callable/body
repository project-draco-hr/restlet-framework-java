def test_callable(self):
    self.assert_(callable(len))

    def f():
        pass
    self.assert_(callable(f))


    class C:

        def meth(self):
            pass
    self.assert_(callable(C))
    x = C()
    self.assert_(callable(x.meth))
    self.assert_((not callable(x)))


    class D(C):

        def __call__(self):
            pass
    y = D()
    self.assert_(callable(y))
    y()
