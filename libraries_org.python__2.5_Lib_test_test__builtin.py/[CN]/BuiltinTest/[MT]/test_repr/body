def test_repr(self):
    self.assertEqual(repr(''), "''")
    self.assertEqual(repr(0), '0')
    self.assertEqual(repr(0L), '0L')
    self.assertEqual(repr(()), '()')
    self.assertEqual(repr([]), '[]')
    self.assertEqual(repr({}), '{}')
    a = []
    a.append(a)
    self.assertEqual(repr(a), '[[...]]')
    a = {}
    a[0] = a
    self.assertEqual(repr(a), '{0: {...}}')
