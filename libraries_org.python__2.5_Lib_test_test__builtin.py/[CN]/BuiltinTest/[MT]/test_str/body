def test_str(self):
    self.assertEqual(str(''), '')
    self.assertEqual(str(0), '0')
    self.assertEqual(str(0L), '0')
    self.assertEqual(str(()), '()')
    self.assertEqual(str([]), '[]')
    self.assertEqual(str({}), '{}')
    a = []
    a.append(a)
    self.assertEqual(str(a), '[[...]]')
    a = {}
    a[0] = a
    self.assertEqual(str(a), '{0: {...}}')
