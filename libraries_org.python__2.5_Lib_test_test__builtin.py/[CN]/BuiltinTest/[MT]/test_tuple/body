def test_tuple(self):
    self.assertEqual(tuple(()), ())
    t0_3 = (0, 1, 2, 3)
    t0_3_bis = tuple(t0_3)
    self.assert_((t0_3 is t0_3_bis))
    self.assertEqual(tuple([]), ())
    self.assertEqual(tuple([0, 1, 2, 3]), (0, 1, 2, 3))
    self.assertEqual(tuple(''), ())
    self.assertEqual(tuple('spam'), ('s', 'p', 'a', 'm'))
