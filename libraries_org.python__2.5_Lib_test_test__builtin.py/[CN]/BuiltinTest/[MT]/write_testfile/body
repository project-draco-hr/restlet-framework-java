def write_testfile(self):
    fp = open(TESTFN, 'w')
    try:
        fp.write('1+1\n')
        fp.write('1+1\n')
        fp.write('The quick brown fox jumps over the lazy dog')
        fp.write('.\n')
        fp.write('Dear John\n')
        fp.write(('XXX' * 100))
        fp.write(('YYY' * 100))
    finally:
        fp.close()
