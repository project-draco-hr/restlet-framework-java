{
  final boolean bRep0Null=(rep0.getIdentifier() == null);
  final boolean bRep1Null=(rep1.getIdentifier() == null);
  if (bRep0Null && bRep1Null) {
    return 0;
  }
  if (bRep0Null) {
    return -1;
  }
  if (bRep1Null) {
    return 1;
  }
  return compare(rep0.toString(false,false),rep1.toString(false,false));
}
