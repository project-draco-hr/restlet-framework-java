{
  for (  Annotation annotation : javaMethod.getAnnotations()) {
    Class<? extends Annotation> annoType=annotation.annotationType();
    if (annoType.getName().startsWith(WrapperUtil.JAX_RS_PACKAGE_PREFIX))     return true;
    if (annoType.isAnnotationPresent(HttpMethod.class))     return true;
  }
  return false;
}
