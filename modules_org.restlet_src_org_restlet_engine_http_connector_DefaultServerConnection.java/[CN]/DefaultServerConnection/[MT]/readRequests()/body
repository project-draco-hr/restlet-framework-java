{
  try {
    if (isPipelining()) {
    }
 else     if (!isInboundBusy()) {
      if (getInboundRequests().size() == 0) {
        ConnectedRequest request=readRequest();
        if (request != null) {
          getInboundRequests().add(request);
          getHelper().getPendingRequests().add(request);
        }
      }
    }
    getHelper().control();
  }
 catch (  Exception e) {
    getLogger().log(Level.WARNING,"Error while reading an HTTP request: ",e.getMessage());
    getLogger().log(Level.INFO,"Error while reading an HTTP request",e);
  }
}
