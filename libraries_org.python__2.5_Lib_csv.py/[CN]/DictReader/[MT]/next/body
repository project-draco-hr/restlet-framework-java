def next(self):
    row = self.reader.next()
    if (self.fieldnames is None):
        self.fieldnames = row
        row = self.reader.next()
    self.line_num = self.reader.line_num
    while (row == []):
        row = self.reader.next()
    d = dict(zip(self.fieldnames, row))
    lf = len(self.fieldnames)
    lr = len(row)
    if (lf < lr):
        d[self.restkey] = row[lf:]
    elif (lf > lr):
        for key in self.fieldnames[lr:]:
            d[key] = self.restval
    return d
