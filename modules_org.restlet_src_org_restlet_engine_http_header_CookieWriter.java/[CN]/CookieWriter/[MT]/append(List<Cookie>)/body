{
  Cookie cookie;
  for (int i=0; i < cookies.size(); i++) {
    cookie=cookies.get(i);
    if (i == 0) {
      if (cookie.getVersion() > 0) {
        append("$Version=\"").append(cookie.getVersion()).append("\"; ");
      }
    }
 else {
      append("; ");
    }
    append(cookie);
  }
  return this;
}
