{
  this.logger=(logger == null) ? Logger.getLogger(getClass().getCanonicalName()) : logger;
  this.pattern=pattern;
  this.defaultVariable=new Variable(defaultType,defaultDefaultValue,defaultRequired,defaultFixed);
  this.matchingMode=matchingMode;
  this.variables=new ConcurrentHashMap<String,Variable>();
  this.regexPattern=null;
  this.regexVariables=new CopyOnWriteArrayList<String>();
  this.encodeVariables=encodeVariables;
}
