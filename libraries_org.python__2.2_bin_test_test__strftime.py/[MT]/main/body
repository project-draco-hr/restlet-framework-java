def main():
    global verbose
    try:
        import java
        java.util.Locale.setDefault(java.util.Locale.US)
    except ImportError:
        import locale
        locale.setlocale(locale.LC_TIME, 'C')
    now = time.time()
    strftest(now)
    verbose = 0
    for j in range((-5), 5):
        for i in range(25):
            strftest((now + (((i + (j * 100)) * 23) * 3603)))
