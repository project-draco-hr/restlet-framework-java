def test_mutable_bases_catch_mro_conflict():
    if verbose:
        print 'Testing mutable bases catch mro conflict...'


    class A(object):
        pass


    class B(object):
        pass


    class C(A, B):
        pass


    class D(A, B):
        pass


    class E(C, D):
        pass
    try:
        C.__bases__ = (B, A)
    except TypeError:
        pass
    else:
        raise TestFailed, "didn't catch MRO conflict"
