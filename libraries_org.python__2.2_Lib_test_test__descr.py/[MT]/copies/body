def copies():
    if verbose:
        print 'Testing copy.copy() and copy.deepcopy()...'
    import copy


    class C(object):
        pass
    a = C()
    a.foo = 12
    b = copy.copy(a)
    vereq(b.__dict__, a.__dict__)
    a.bar = [1, 2, 3]
    c = copy.copy(a)
    vereq(c.bar, a.bar)
    verify((c.bar is a.bar))
    d = copy.deepcopy(a)
    vereq(d.__dict__, a.__dict__)
    a.bar.append(4)
    vereq(d.bar, [1, 2, 3])
