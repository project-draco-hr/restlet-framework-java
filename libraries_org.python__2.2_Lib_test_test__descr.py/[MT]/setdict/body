def setdict():
    if verbose:
        print 'Testing __dict__ assignment...'


    class C(object):
        pass
    a = C()
    a.__dict__ = {'b': 1, }
    vereq(a.b, 1)

    def cant(x, dict):
        try:
            x.__dict__ = dict
        except TypeError:
            pass
        else:
            raise TestFailed, ("shouldn't allow %r.__dict__ = %r" % (x, dict))
    cant(a, None)
    cant(a, [])
    cant(a, 1)
    del a.__dict__
    cant(C, {})
