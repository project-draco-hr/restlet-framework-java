def numops(a, b, skip=[]):
    dict = {'a': a, 'b': b, }
    for (name, expr) in binops.items():
        if (name not in skip):
            name = ('__%s__' % name)
            if hasattr(a, name):
                res = eval(expr, dict)
                testbinop(a, b, res, expr, name)
    for (name, expr) in unops.items():
        if (name not in skip):
            name = ('__%s__' % name)
            if hasattr(a, name):
                res = eval(expr, dict)
                testunop(a, res, expr, name)
