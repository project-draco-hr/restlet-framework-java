def test_join(self):
    self.assert_((strop.join(['a', 'b', 'c', 'd']) == 'a b c d'))
    self.assert_((strop.join(('a', 'b', 'c', 'd'), '') == 'abcd'))
    self.assert_((strop.join(Sequence()) == 'w x y z'))
    self.assert_((strop.join(([('x' * 100)] * 100), ':') == (((('x' * 100) + ':') * 99) + ('x' * 100))))
    self.assert_((strop.join(((('x' * 100),) * 100), ':') == (((('x' * 100) + ':') * 99) + ('x' * 100))))
