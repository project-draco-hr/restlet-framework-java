def test_explicit_from_Decimal(self):
    d = Decimal(45)
    e = Decimal(d)
    self.assertEqual(str(e), '45')
    self.assertNotEqual(id(d), id(e))
    d = Decimal(500000123)
    e = Decimal(d)
    self.assertEqual(str(e), '500000123')
    self.assertNotEqual(id(d), id(e))
    d = Decimal((-45))
    e = Decimal(d)
    self.assertEqual(str(e), '-45')
    self.assertNotEqual(id(d), id(e))
    d = Decimal(0)
    e = Decimal(d)
    self.assertEqual(str(e), '0')
    self.assertNotEqual(id(d), id(e))
