def eval_directive(self, s):
    (funct, value) = map((lambda x: x.strip().lower()), s.split(':'))
    if (funct == 'rounding'):
        value = RoundingDict[value]
    else:
        try:
            value = int(value)
        except ValueError:
            pass
    funct = self.ChangeDict.get(funct, Nonfunction)
    funct(value)
