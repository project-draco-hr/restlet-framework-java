def test_tostring_methods(self):
    d = Decimal('15.32')
    self.assertEqual(str(d), '15.32')
    self.assertEqual(repr(d), 'Decimal("15.32")')
    unicode_inputs = [u'123.4', u'0.5E2', u'Infinity', u'sNaN', u'-0.0E100', u'-NaN001', u'-Inf']
    for u in unicode_inputs:
        d = Decimal(u)
        self.assertEqual(type(str(d)), str)
        self.assertEqual(type(repr(d)), str)
        self.assertEqual(type(d.to_eng_string()), str)
