def test_eval_round_trip(self):
    d = Decimal((0, (0,), 0))
    self.assertEqual(d, eval(repr(d)))
    d = Decimal((1, (4, 5), 0))
    self.assertEqual(d, eval(repr(d)))
    d = Decimal((0, (4, 5, 3, 4), (-2)))
    self.assertEqual(d, eval(repr(d)))
    d = Decimal((1, (4, 3, 4, 9, 1, 3, 5, 3, 4), (-25)))
    self.assertEqual(d, eval(repr(d)))
