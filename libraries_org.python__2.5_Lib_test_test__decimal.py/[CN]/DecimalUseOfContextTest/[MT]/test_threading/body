def test_threading(self):
    self.synchro = threading.Event()
    self.finish1 = threading.Event()
    self.finish2 = threading.Event()
    th1 = threading.Thread(target=thfunc1, args=(self,))
    th2 = threading.Thread(target=thfunc2, args=(self,))
    th1.start()
    th2.start()
    self.finish1.wait()
    self.finish2.wait()
    return
