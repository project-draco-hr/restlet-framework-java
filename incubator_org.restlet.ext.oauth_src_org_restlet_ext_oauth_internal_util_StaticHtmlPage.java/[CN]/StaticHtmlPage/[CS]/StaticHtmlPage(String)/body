{
  Reference ref=new Reference(uri);
  ClientResource local=new ClientResource(ref);
  Representation tmpPage=local.get();
  try {
    page=tmpPage.getText();
  }
 catch (  IOException e) {
    page=e.getLocalizedMessage();
  }
  type=tmpPage.getMediaType();
  tmpPage.release();
  local.release();
}
