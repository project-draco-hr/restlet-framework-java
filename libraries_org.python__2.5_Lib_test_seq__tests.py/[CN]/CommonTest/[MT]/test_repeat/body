def test_repeat(self):
    for m in xrange(4):
        s = tuple(range(m))
        for n in xrange((-3), 5):
            self.assertEqual(self.type2test((s * n)), (self.type2test(s) * n))
        self.assertEqual((self.type2test(s) * (-4)), self.type2test([]))
        self.assertEqual(id(s), id((s * 1)))
