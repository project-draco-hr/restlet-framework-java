{
  if (responseEntityStream == null)   try {
    if (isResponseChunked()) {
      responseEntityStream=new ChunkedOutputStream(getResponse().getOutputStream());
    }
 else {
      responseEntityStream=new KeepAliveOutputStream(getResponse().getOutputStream());
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  return responseEntityStream;
}
