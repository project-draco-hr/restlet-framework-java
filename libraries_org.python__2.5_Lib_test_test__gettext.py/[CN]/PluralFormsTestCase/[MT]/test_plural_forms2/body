def test_plural_forms2(self):
    eq = self.assertEqual
    fp = open(self.mofile, 'rb')
    t = gettext.GNUTranslations(fp)
    fp.close()
    x = t.ngettext('There is %s file', 'There are %s files', 1)
    eq(x, 'Hay %s fichero')
    x = t.ngettext('There is %s file', 'There are %s files', 2)
    eq(x, 'Hay %s ficheros')
