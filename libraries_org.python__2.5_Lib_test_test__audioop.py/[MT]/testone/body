def testone(name, data):
    try:
        func = eval(('test' + name))
    except NameError:
        print (('No test found for audioop.' + name) + '()')
        return
    try:
        rv = func(data)
    except 'xx':
        print (('Test FAILED for audioop.' + name) + '() (with an exception)')
        return
    if (not rv):
        print (('Test FAILED for audioop.' + name) + '()')
