def testmul(data):
    if verbose:
        print 'mul'
    data2 = []
    for d in data:
        str = ''
        for s in d:
            str = (str + chr((ord(s) * 2)))
        data2.append(str)
    if ((audioop.mul(data[0], 1, 2) != data2[0]) or (audioop.mul(data[1], 2, 2) != data2[1]) or (audioop.mul(data[2], 4, 2) != data2[2])):
        return 0
    return 1
