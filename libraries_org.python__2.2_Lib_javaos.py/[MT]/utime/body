def utime(path, times):
    'utime(path, (atime, mtime))\n    utime(path, None)\n\n    Set the access and modified time of the file to the given values.\n    If the second form is used, set the access and modified times to the\n    current time.\n\n    Due to java limitations only the modification time is changed.\n    '
    if (times is not None):
        mtime = times[1]
    else:
        mtime = time.time()
    if hasattr(File, 'setLastModified'):
        File(path).setLastModified(long((mtime * 1000.0)))
