{
  if (attribute == AttributeExchange.FULL_NAME)   return this.getName();
 else   if (attribute == AttributeExchange.EMAIL)   return this.getEmail();
 else   if (attribute == AttributeExchange.FIRST_NAME)   return this.getFirstName();
 else   if (attribute == AttributeExchange.LAST_NAME)   return this.getLastName();
  return null;
}
