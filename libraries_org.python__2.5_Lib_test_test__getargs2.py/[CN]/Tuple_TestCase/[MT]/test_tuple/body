def test_tuple(self):
    from _testcapi import getargs_tuple
    ret = getargs_tuple(1, (2, 3))
    self.assertEquals(ret, (1, 2, 3))


    class seq:

        def __len__(self):
            return 2

        def __getitem__(self, n):
            raise ValueError
    self.assertRaises(TypeError, getargs_tuple, 1, seq())
