{
  List<MethodAnnotationInfo> infos=AnnotationUtils.getInstance().getAnnotations(IChild.class);
  Assert.assertEquals("Wrong count: " + infos,4,infos.size());
  boolean found=false;
  for (  MethodAnnotationInfo ai : infos) {
    if (ai.getJavaClass().equals(IChild.class) && ai.getRestletMethod().equals(Method.GET)) {
      found=true;
      Assert.assertEquals(String.class,ai.getJavaOutputType());
    }
  }
  Assert.assertEquals("Didn't find a method with IChild as the declaring class.",true,found);
}
