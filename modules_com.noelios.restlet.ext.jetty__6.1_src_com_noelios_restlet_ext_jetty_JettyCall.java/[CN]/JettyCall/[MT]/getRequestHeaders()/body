{
  Series<Parameter> result=super.getRequestHeaders();
  if (!requestHeadersAdded) {
    String headerName;
    String headerValue;
    for (Enumeration<String> names=getConnection().getRequest().getHeaderNames(); names.hasMoreElements(); ) {
      headerName=names.nextElement();
      for (Enumeration<String> values=getConnection().getRequest().getHeaders(headerName); values.hasMoreElements(); ) {
        headerValue=values.nextElement();
        result.add(new Parameter(headerName,headerValue));
      }
    }
    requestHeadersAdded=true;
  }
  return result;
}
