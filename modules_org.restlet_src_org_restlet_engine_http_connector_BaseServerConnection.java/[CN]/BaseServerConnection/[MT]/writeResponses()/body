{
  try {
    if (isPipelining()) {
    }
 else {
      if (getOutboundResponses().size() > 0) {
        Response nextResponse=getOutboundResponses().poll();
        writeResponse(nextResponse);
      }
    }
    readRequests();
  }
 catch (  Exception e) {
    getLogger().log(Level.WARNING,"Error while writing an HTTP response: ",e.getMessage());
    getLogger().log(Level.INFO,"Error while writing an HTTP response",e);
  }
}
