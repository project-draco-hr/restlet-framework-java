{
  try {
    if (isPipelining()) {
    }
 else {
      Response response=null;
synchronized (getOutboundResponses()) {
        if (canWrite()) {
          response=getOutboundResponses().poll();
          setOutboundBusy((response != null));
        }
      }
      writeResponse(response);
    }
  }
 catch (  Exception e) {
    getLogger().log(Level.WARNING,"Error while writing an HTTP response: ",e.getMessage());
    getLogger().log(Level.INFO,"Error while writing an HTTP response",e);
  }
}
