{
  try {
    if (isPipelining()) {
    }
 else {
      boolean doRead=false;
synchronized (getInboundRequests()) {
        if (canRead()) {
          doRead=(getInboundStream().available() > 0);
          setInboundBusy(doRead);
        }
      }
      if (doRead) {
        readRequest();
      }
    }
  }
 catch (  Exception e) {
    getLogger().log(Level.WARNING,"Error while reading an HTTP request: ",e.getMessage());
    getLogger().log(Level.INFO,"Error while reading an HTTP request",e);
  }
  getHelper().handleNextRequest();
}
