{
  try {
    if (isPipelining()) {
    }
 else {
      boolean doRead=false;
synchronized (getInboundRequests()) {
        if (canRead()) {
          doRead=true;
          setInboundBusy(doRead);
        }
      }
      if (doRead) {
        readRequest();
      }
    }
  }
 catch (  Exception e) {
    getLogger().log(Level.INFO,"Error while reading an HTTP request. Closing the connection: ",e.getMessage());
    getLogger().log(Level.INFO,"Error while reading an HTTP request. Closing the connection.",e);
    close(false);
  }
  getHelper().handleNextRequest();
}
