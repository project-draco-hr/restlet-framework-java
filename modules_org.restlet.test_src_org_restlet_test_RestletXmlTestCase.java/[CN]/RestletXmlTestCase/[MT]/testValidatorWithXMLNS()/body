{
  System.out.println("-- testValidatorWithXMLNS");
  try {
    validator.validate(getAsSource(XML_WITH_XMLNS));
    assertTrue(true);
  }
 catch (  SAXException x) {
    fail("MUST be able to validate restlet.xml with xmlns attribute");
  }
catch (  IOException x) {
    fail("MUST be able to validate restlet.xml with xmlns attribute");
  }
}
