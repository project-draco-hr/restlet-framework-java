{
  System.out.println("-- testParserWithoutXMLNS");
  try {
    builder.parse(getAsStream(XML_WITHOUT_XMLNS));
    assertTrue(true);
  }
 catch (  SAXException x) {
    fail("MUST be able to parse a good restlet.xml without xmlns attribute");
  }
catch (  IOException x) {
    fail("MUST be able to parse a good restlet.xml without xmlns attribute");
  }
}
