def test_scal_int_zero(self):
    self.check('scal', xrange(1, 15), [0], '\n(long)\n(int)\n(short)\n(byte)\n(byte)\n(double)\n(float)\n(boolean)\n(java.io.Serializable)\n(java.io.Serializable)\n(java.io.Serializable)\n(java.io.Serializable)\n(java.io.Serializable)\n(java.lang.Object)                   \n                   ')
