def test_invalid(self):
    ai = self.assertInvalid
    ai('a b')
    ai('a @')
    ai('a b @')
    ai('a ** @')
    ai('a = ')
    ai('a = 9 +')
    ai('def x():\n\npass\n')
    ai('\n\n if 1: pass\n\npass')
    ai('a = 9+ \\\n')
    ai("a = 'a\\ ")
    ai("a = 'a\\\n")
    ai('a = 1', 'eval')
    ai('a = (', 'eval')
    ai(']', 'eval')
    ai('())', 'eval')
    ai('[}', 'eval')
    ai('9+', 'eval')
    ai('lambda z:', 'eval')
    ai('a b', 'eval')
