{
  List<MessageBodyReader<String>> mbrs=messageBodyWorkers.getMessageBodyReaders(headers.getMediaType(),String.class,String.class,new Annotation[0]);
  String toCreate=mbrs.get(0).readFrom(String.class,String.class,headers.getMediaType(),new Annotation[0],headers.getRequestHeaders(),inputStream);
  System.out.println(toCreate);
  URI location=uriInfo.getAbsolutePathBuilder().path("5").build();
  return Response.created(location).build();
}
