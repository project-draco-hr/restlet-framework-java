{
  if (((toEncode >= 'A') && (toEncode <= 'Z')) || ((toEncode >= 'a') && (toEncode <= 'z')) || ((toEncode >= '0') && (toEncode <= '9'))|| (toEncode == '-')|| (toEncode == '.')|| (toEncode == '_')|| (toEncode == '~')) {
    stb.append(toEncode);
    return 1;
  }
 else {
    toHex(toEncode,stb);
    return 3;
  }
}
