def dump(self, io=None):
    if io:
        save = sys.stdout
        sys.stdout = io
    pc = 0
    for t in self.insts:
        opname = t[0]
        if (opname == 'SET_LINENO'):
            print 
        if (len(t) == 1):
            print '\t', ('%3d' % pc), opname
            pc = (pc + 1)
        else:
            print '\t', ('%3d' % pc), opname, t[1]
            pc = (pc + 3)
    if io:
        sys.stdout = save
