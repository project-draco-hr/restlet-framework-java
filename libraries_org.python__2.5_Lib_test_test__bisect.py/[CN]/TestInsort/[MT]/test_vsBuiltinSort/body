def test_vsBuiltinSort(self, n=500):
    from random import choice
    for insorted in (list(), UserList()):
        for i in xrange(n):
            digit = choice('0123456789')
            if (digit in '02468'):
                f = insort_left
            else:
                f = insort_right
            f(insorted, digit)
    self.assertEqual(sorted(insorted), insorted)
