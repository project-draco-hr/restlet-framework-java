{
  if (target != null) {
    if (target.isStopped()) {
      try {
        target.start();
      }
 catch (      Exception e) {
        logger.log(Level.WARNING,UNABLE_TO_START,e);
        call.setStatus(Statuses.SERVER_ERROR_INTERNAL);
      }
    }
    if (target.isStarted()) {
      target.handle(call);
    }
 else {
      logger.log(Level.WARNING,UNABLE_TO_START);
      call.setStatus(Statuses.SERVER_ERROR_INTERNAL);
    }
  }
 else {
  }
}
