{
  if ((this.object == null) && (getText() != null)) {
    try {
      SerializationStreamFactory serializationFactory=(SerializationStreamFactory)GWT.create(targetClass);
      SerializationStreamReader streamReader=serializationFactory.createStreamReader(getText());
      this.object=(T)streamReader.readObject();
    }
 catch (    Exception e) {
      this.object=null;
      e.printStackTrace();
    }
  }
  return object;
}
