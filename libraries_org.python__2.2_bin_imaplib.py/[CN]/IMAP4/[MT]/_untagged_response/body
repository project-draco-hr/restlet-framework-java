def _untagged_response(self, typ, dat, name):
    if (typ == 'NO'):
        return (typ, dat)
    if (not self.untagged_responses.has_key(name)):
        return (typ, [None])
    data = self.untagged_responses[name]
    if __debug__:
        if (self.debug >= 5):
            _mesg(('untagged_responses[%s] => %s' % (name, data)))
    del self.untagged_responses[name]
    return (typ, data)
