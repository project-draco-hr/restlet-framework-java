{
  try {
    JaxbRepresentation<Mail> mailRep=new JaxbRepresentation<Mail>(representation,Mail.class);
    Mail mail=mailRep.getObject();
    System.out.println("Status: " + mail.getStatus());
    System.out.println("Subject: " + mail.getSubject());
    System.out.println("Content: " + mail.getContent());
    System.out.println("Account URI: " + mail.getAccountRef());
  }
 catch (  IOException e) {
    throw new ResourceException(e);
  }
  return null;
}
