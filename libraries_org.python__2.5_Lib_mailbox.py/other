'Read/write support for Maildir, mbox, MH, Babyl, and MMDF mailboxes.'
import sys
import os
import time
import calendar
import socket
import errno
import copy
import email
import email.Message
import email.Generator
import rfc822
import StringIO
try:
    if (sys.platform == 'os2emx'):
        raise ImportError
    import fcntl
except ImportError:
    fcntl = None
