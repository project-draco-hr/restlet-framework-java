def visitIf(self, node, scope):
    for (test, body) in node.tests:
        if isinstance(test, ast.Const):
            if (type(test.value) in self._const_types):
                if (not test.value):
                    continue
        self.visit(test, scope)
        self.visit(body, scope)
    if node.else_:
        self.visit(node.else_, scope)
