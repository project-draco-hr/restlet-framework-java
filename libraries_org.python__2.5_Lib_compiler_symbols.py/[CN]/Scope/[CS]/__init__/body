def __init__(self, name, module, klass=None):
    self.name = name
    self.module = module
    self.defs = {}
    self.uses = {}
    self.globals = {}
    self.params = {}
    self.frees = {}
    self.cells = {}
    self.children = []
    self.nested = None
    self.generator = None
    self.klass = None
    if (klass is not None):
        for i in range(len(klass)):
            if (klass[i] != '_'):
                self.klass = klass[i:]
                break
