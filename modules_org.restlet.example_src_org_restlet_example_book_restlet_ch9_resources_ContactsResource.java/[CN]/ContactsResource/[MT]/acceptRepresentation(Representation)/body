{
  Form form=new Form(entity);
  Contact contact=new Contact();
  contact.setMailAddress(form.getFirstValue("mailAddress"));
  contact.setName(form.getFirstValue("name"));
  contact=getDAOFactory().getMailboxDAO().createContact(mailbox,contact);
  getResponse().redirectSeeOther(getChildReference(getRequest().getResourceRef(),contact.getId()));
}
