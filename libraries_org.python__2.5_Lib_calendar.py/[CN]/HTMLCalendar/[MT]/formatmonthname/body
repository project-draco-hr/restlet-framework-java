def formatmonthname(self, theyear, themonth, withyear=True):
    '\n        Return a month name as a table row.\n        '
    if withyear:
        s = ('%s %s' % (month_name[themonth], theyear))
    else:
        s = ('%s' % month_name[themonth])
    return ('<tr><th colspan="7" class="month">%s</th></tr>' % s)
