{
  try {
    File testDir=new File(System.getProperty("java.io.tmpdir"),"DirectoryTestCase");
    testDir.mkdir();
    File testFile=File.createTempFile("test",".txt",testDir);
    testFileUrl=webSiteURL.concat(testFile.getName());
    Container clientContainer=new Container();
    clientContainer.getClients().add(Protocol.FILE);
    clientContainer.start();
    Directory directory=new Directory(clientContainer.getContext(),testDir.toURI().toString(),"");
    testDirectory(directory);
    clientContainer.stop();
    testFile.delete();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
