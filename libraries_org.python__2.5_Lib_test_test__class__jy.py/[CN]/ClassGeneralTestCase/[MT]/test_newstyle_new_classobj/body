def test_newstyle_new_classobj(self):


    class Foo(object):
        pass

    def hello(self):
        return 'hello'
    Bar = types.ClassType('Bar', (Foo,), dict(hello=hello))
    self.assert_(type(Bar), type)
    self.assert_(issubclass(Bar, Foo))
    self.assert_(hasattr(Bar, 'hello'))
    self.assertEquals(Bar().hello(), 'hello')
