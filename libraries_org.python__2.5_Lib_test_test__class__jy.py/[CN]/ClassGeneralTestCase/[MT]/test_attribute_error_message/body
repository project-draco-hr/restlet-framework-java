def test_attribute_error_message(self):


    class Bar:
        pass
    try:
        Bar.bar
        self._assert(False)
    except AttributeError as e:
        self.assertEqual("class Bar has no attribute 'bar'", str(e))


    class Foo(object):
        pass
    try:
        Foo.bar
        self._assert(False)
    except AttributeError as e:
        self.assertEqual("type object 'Foo' has no attribute 'bar'", str(e))
