{
  WadlComponent comp=new WadlComponent();
  Representation wadl=comp.getContext().getDispatcher().get("clap://class/org/restlet/test/YahooSearch.wadl").getEntity();
  comp.attach(wadl);
  Application app=(Application)comp.getHosts().get(0).getRoutes().get(0).getNext();
  assertNotNull(app);
  Router root=(Router)app.getRoot();
  assertNotNull(root);
}
