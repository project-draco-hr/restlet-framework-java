def setUp(self):
    self.data = [x.split('|')[:(-1)] for x in data.splitlines()]
    self.posix_data = [x.split('|')[:(-1)] for x in posix_data.splitlines()]
    for item in self.data:
        item[0] = item[0].replace('\\n', '\n')
    for item in self.posix_data:
        item[0] = item[0].replace('\\n', '\n')
