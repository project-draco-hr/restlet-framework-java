def startElement(self, name, amap):
    self.writer.write(('<' + name))
    a_names = amap.keys()
    a_names.sort()
    for a_name in a_names:
        self.writer.write(((' ' + a_name) + '="'))
        self.write_data(amap[a_name])
        self.writer.write('"')
    self.writer.write('>')
    self.elem_level = (self.elem_level + 1)
