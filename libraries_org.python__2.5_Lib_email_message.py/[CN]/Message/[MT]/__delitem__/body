def __delitem__(self, name):
    'Delete all occurrences of a header, if present.\n\n        Does not raise an exception if the header is missing.\n        '
    name = name.lower()
    newheaders = []
    for (k, v) in self._headers:
        if (k.lower() != name):
            newheaders.append((k, v))
    self._headers = newheaders
