{
  float result=-1.0F;
  if (source instanceof JSONArray) {
    if (MediaType.APPLICATION_JSON.isCompatible(target.getMediaType())) {
      result=1.0F;
    }
 else {
      result=0.5F;
    }
  }
 else   if (source instanceof JSONObject) {
    if (MediaType.APPLICATION_JSON.isCompatible(target.getMediaType())) {
      result=1.0F;
    }
 else {
      result=0.5F;
    }
  }
 else   if (source instanceof JSONTokener) {
    if (MediaType.APPLICATION_JSON.isCompatible(target.getMediaType())) {
      result=1.0F;
    }
 else {
      result=0.5F;
    }
  }
  return result;
}
