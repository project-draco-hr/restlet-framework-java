{
  WrapperFactory wrapperFactory=new WrapperFactory(new ThreadLocalizedContext(),null,null,Logger.getAnonymousLogger());
  try {
    wrapperFactory.getRootResourceClass(IllegalPathService1.class);
    fail("must not pass");
  }
 catch (  AssertionFailedError e) {
  }
  RootResourceClass rrc=wrapperFactory.getRootResourceClass(IllegalPathService2.class);
  assertEquals("/afsdf%3Ause",rrc.getPathRegExp().getPathPattern());
}
