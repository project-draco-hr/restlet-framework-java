{
  final WrapperFactory wrapperFactory=new WrapperFactory(new ThreadLocalizedContext(),null,null);
  try {
    wrapperFactory.getRootResourceClass(IllegalPathService1.class);
    fail("must not pass");
  }
 catch (  final AssertionFailedError e) {
  }
  final RootResourceClass rrc=wrapperFactory.getRootResourceClass(IllegalPathService2.class);
  assertEquals("/afsdf%3Ause",rrc.getPathRegExp().getPathPattern());
}
