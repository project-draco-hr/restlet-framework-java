{
  try {
    if (!isStarted())     start();
    if (getRoot() != null) {
      getRoot().handle(request,response);
    }
 else {
      response.setStatus(Status.SERVER_ERROR_INTERNAL);
      logger.log(Level.SEVERE,"No root handler defined.");
    }
  }
 catch (  Exception e) {
    response.setStatus(Status.SERVER_ERROR_INTERNAL);
    logger.log(Level.SEVERE,"Unexpected exception while handling a call",e);
  }
}
