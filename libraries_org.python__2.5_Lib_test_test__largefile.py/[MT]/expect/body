def expect(got_this, expect_this):
    if test_support.verbose:
        print ('%r =?= %r ...' % (got_this, expect_this)),
    if (got_this != expect_this):
        if test_support.verbose:
            print 'no'
        raise test_support.TestFailed, ('got %r, but expected %r' % (got_this, expect_this))
    elif test_support.verbose:
        print 'yes'
