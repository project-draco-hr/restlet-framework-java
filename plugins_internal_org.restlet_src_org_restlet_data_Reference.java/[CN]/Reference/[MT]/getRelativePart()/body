{
  String result=null;
  if (schemeIndex == -1) {
    if (queryIndex != -1) {
      result=this.uri.substring(0,queryIndex);
    }
 else {
      if (fragmentIndex != -1) {
        result=this.uri.substring(0,fragmentIndex);
      }
 else {
        result=this.uri;
      }
    }
  }
  return result;
}
