{
  if (this.source != null) {
    this.source.release();
    this.source=null;
  }
  if (this.transformer != null) {
    this.transformer=null;
  }
  if (this.transformSheet != null) {
    this.transformSheet.release();
    this.transformSheet=null;
  }
  if (this.uriResolver != null) {
    this.uriResolver=null;
  }
  super.release();
}
