def replace(self, year=None, month=None, day=None):
    'Return a new date with new values for the specified fields.'
    if (year is None):
        year = self.__year
    if (month is None):
        month = self.__month
    if (day is None):
        day = self.__day
    _check_date_fields(year, month, day)
    return date(year, month, day)
