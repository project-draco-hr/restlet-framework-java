def __setstate(self, state):
    assert isinstance(state, tuple)
    assert (1 <= len(state) <= 2)
    string = state[0]
    assert (len(string) == 6)
    (self.__hour, self.__minute, self.__second, us1, us2, us3) = map(ord, string)
    self.__microsecond = ((((us1 << 8) | us2) << 8) | us3)
    if (len(state) == 1):
        self._tzinfo = None
    else:
        self._tzinfo = state[1]
