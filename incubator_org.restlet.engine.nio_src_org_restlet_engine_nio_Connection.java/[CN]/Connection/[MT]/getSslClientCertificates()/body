{
  if (getSocket() instanceof SSLSocket) {
    SSLSocket sslSocket=(SSLSocket)getSocket();
    SSLSession sslSession=sslSocket.getSession();
    if (sslSession != null) {
      try {
        List<Certificate> clientCertificates=Arrays.asList(sslSession.getPeerCertificates());
        return clientCertificates;
      }
 catch (      SSLPeerUnverifiedException e) {
        getHelper().getLogger().log(Level.FINE,"Can't get the client certificates.",e);
      }
    }
  }
  return null;
}
