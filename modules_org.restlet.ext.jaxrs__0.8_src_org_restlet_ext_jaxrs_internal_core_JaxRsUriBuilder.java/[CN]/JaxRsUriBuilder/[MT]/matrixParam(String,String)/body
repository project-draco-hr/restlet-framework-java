{
  CharSequence ncs;
  ncs=EncodeOrCheck.nameOrValue(name,encode,"matrix parameter name");
  CharSequence vcs=EncodeOrCheck.nameOrValue(value,encode,"matrix parameter value");
  StringBuilder path=getPath();
  path.append(';');
  path.append(ncs);
  path.append('=');
  path.append(vcs);
  return this;
}
