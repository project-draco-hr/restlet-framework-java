{
  List<Class<?>> result=null;
  if (variant != null) {
    if (VARIANT_JSON.isCompatible(variant)) {
      if (result == null) {
        result=new ArrayList<Class<?>>();
      }
      result.add(JSONArray.class);
      result.add(JSONObject.class);
      result.add(JSONTokener.class);
    }
  }
  return result;
}
