def close(self):
    if (self.mode == WRITE):
        self.fileobj.write(self.compress.flush())
        write32(self.fileobj, self.crc)
        write32(self.fileobj, self.size)
        self.fileobj = None
    elif (self.mode == READ):
        self.fileobj = None
    if self.myfileobj:
        self.myfileobj.close()
        self.myfileobj = None
