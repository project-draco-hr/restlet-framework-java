{
  if (acceptCharsetHeader != null) {
    if (acceptCharsetHeader.length() == 0) {
      client.getAcceptedCharacterSets().add(new Preference<CharacterSet>(CharacterSet.ISO_8859_1));
    }
 else {
      AcceptReader pr=new AcceptReader(AcceptReader.TYPE_CHARACTER_SET,acceptCharsetHeader);
      pr.addValues(client.getAcceptedCharacterSets());
    }
  }
 else {
    client.getAcceptedCharacterSets().add(new Preference(CharacterSet.ALL));
  }
}
