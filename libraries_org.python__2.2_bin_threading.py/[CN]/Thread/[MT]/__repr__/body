def __repr__(self):
    assert self.__initialized, 'Thread.__init__() was not called'
    status = 'initial'
    if self.__started:
        status = 'started'
    if self.__stopped:
        status = 'stopped'
    if self.__daemonic:
        status = (status + ' daemon')
    return ('<%s(%s, %s)>' % (self.__class__.__name__, self.__name, status))
