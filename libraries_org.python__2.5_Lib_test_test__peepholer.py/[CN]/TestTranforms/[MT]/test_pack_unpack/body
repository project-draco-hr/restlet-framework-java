def test_pack_unpack(self):
    for (line, elem) in (('a, = a,', 'LOAD_CONST'), ('a, b = a, b', 'ROT_TWO'), ('a, b, c = a, b, c', 'ROT_THREE')):
        asm = dis_single(line)
        self.assert_((elem in asm))
        self.assert_(('BUILD_TUPLE' not in asm))
        self.assert_(('UNPACK_TUPLE' not in asm))
