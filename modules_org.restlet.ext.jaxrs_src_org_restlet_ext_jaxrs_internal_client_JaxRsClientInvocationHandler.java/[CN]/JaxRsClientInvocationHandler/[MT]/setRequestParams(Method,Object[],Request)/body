{
  int argIndex=0;
  Annotation[][] parameterAnnotations=javaMethod.getParameterAnnotations();
  for (  Annotation[] annotations : parameterAnnotations) {
    String representationAsText=getRepresentationAsText(argIndex,args);
    for (    Annotation annotation : annotations) {
      if (annotation instanceof HeaderParam && representationAsText != null) {
        Util.getHttpHeaders(request).add(((HeaderParam)annotation).value(),representationAsText);
        argIndex++;
      }
 else       if (annotation instanceof QueryParam) {
        request.getResourceRef().addQueryParameter(new Parameter(((QueryParam)annotation).value(),representationAsText));
        argIndex++;
      }
 else       if (annotation instanceof MatrixParam && representationAsText != null) {
        argIndex++;
      }
 else       if (annotation instanceof CookieParam && representationAsText != null) {
        argIndex++;
      }
 else       if (annotation instanceof FormParam && representationAsText != null) {
        argIndex++;
      }
 else       if (annotation instanceof QueryParam && representationAsText != null) {
        argIndex++;
      }
 else       if (annotation instanceof PathParam && representationAsText != null) {
        argIndex++;
      }
    }
  }
}
