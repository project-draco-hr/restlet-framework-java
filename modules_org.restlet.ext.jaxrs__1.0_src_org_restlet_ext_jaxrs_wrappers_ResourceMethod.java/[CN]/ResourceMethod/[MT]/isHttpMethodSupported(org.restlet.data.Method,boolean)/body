{
  if (requestedMethod == null)   throw new IllegalArgumentException("null is not a valid HTTP method");
  if (this.httpMethod == null)   this.httpMethod=getHttpMethod(this.javaMethod);
  if (alsoGet && this.httpMethod.equals(org.restlet.data.Method.GET))   return true;
  return this.httpMethod.equals(requestedMethod);
}
