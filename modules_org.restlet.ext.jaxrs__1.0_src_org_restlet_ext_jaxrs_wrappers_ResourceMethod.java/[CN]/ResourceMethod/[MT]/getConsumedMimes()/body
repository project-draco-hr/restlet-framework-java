{
  if (this.consumedMimes == null) {
    ConsumeMime consumeMime=this.javaMethod.getAnnotation(ConsumeMime.class);
    if (consumeMime == null)     consumeMime=this.javaMethod.getAnnotation(ConsumeMime.class);
    if (consumeMime == null)     return Collections.singletonList(MediaType.ALL);
    this.consumedMimes=convertToMediaTypes(consumeMime.value());
  }
  return consumedMimes;
}
