{
  if (producedMimes == null) {
    ProduceMime produceMime=this.javaMethod.getAnnotation(ProduceMime.class);
    if (produceMime == null)     produceMime=this.javaMethod.getAnnotation(ProduceMime.class);
    if (produceMime == null)     this.producedMimes=Collections.emptyList();
 else     this.producedMimes=convertToMediaTypes(produceMime.value());
  }
  return producedMimes;
}
