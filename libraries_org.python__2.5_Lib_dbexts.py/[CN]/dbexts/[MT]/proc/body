def proc(self, proc=None, owner=None, schema=None):
    'If no proc argument, displays a list of all procedures.  If a proc argument,\n        displays the parameters of the given procedure.'
    cur = self.begin()
    if proc:
        cur.procedurecolumns(schema, owner, proc, None)
    else:
        cur.procedures(schema, owner, None)
    self.commit(cur)
    self.display()
