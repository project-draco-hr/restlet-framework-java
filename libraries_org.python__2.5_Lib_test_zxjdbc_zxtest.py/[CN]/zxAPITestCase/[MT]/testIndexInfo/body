def testIndexInfo(self):
    'testing index information'
    c = self.cursor()
    try:
        c.statistics(None, None, 'zxtesting', 0, 0)
        f = c.fetchall()
        assert (f is not None), 'expected some values'
        f = filter((lambda x: x[5]), f)
        assert (len(f) == 1), ('expected [1], got [%d]' % len(f))
    finally:
        c.close()
