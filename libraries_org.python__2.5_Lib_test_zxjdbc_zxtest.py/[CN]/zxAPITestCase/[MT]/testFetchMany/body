def testFetchMany(self):
    'testing cursor.fetchmany()'
    c = self.cursor()
    try:
        c.execute('select * from zxtesting')
        data = c.fetchmany(6)
        assert (len(data) == 6), ('expected [6] rows, got [%d]' % len(data))
        c.execute('select * from zxtesting')
        data = c.fetchmany(16)
        assert (len(data) == 7), ('expected [7] rows, got [%d]' % len(data))
    finally:
        c.close()
