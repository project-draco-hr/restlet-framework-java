def testOrderOfArgsMaxRowsOnly(self):
    'testing execute with max rows only'
    c = self.cursor()
    try:
        c.execute('select * from zxtesting', maxrows=3)
        f = c.fetchall()
        assert (len(f) == 3), ('expected length [3], got [%d]' % len(f))
    finally:
        c.close()
        self.db.commit()
