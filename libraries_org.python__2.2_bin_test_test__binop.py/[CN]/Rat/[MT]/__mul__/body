def __mul__(self, other):
    'Multiply two Rats, or a Rat and a number.'
    if isRat(other):
        return Rat((self.__num * other.__num), (self.__den * other.__den))
    if isint(other):
        return Rat((self.__num * other), self.__den)
    if isnum(other):
        return (float(self) * other)
    return NotImplemented
