{
  if (MediaType.TEXT_RDF_N3.equals(rdfRepresentation.getMediaType(),true)) {
    new RdfN3Representation(rdfRepresentation,this);
  }
 else   if (MediaType.APPLICATION_RDF_XML.equals(rdfRepresentation.getMediaType(),true)) {
    new RdfXmlRepresentation(rdfRepresentation,this);
  }
 else   if (MediaType.APPLICATION_RDF_TURTLE.equals(rdfRepresentation.getMediaType(),true)) {
    new RdfTurtleRepresentation(rdfRepresentation,this);
  }
 else   if (MediaType.TEXT_PLAIN.equals(rdfRepresentation.getMediaType(),true)) {
    new RdfNTriplesRepresentation(rdfRepresentation,this);
  }
}
