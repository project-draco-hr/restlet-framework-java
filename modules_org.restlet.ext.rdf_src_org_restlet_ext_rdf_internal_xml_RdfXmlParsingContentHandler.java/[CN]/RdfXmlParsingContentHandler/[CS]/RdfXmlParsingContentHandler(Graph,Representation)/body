{
  super();
  this.linkSet=linkSet;
  if (rdfXmlRepresentation instanceof SaxRepresentation) {
    this.rdfXmlRepresentation=(SaxRepresentation)rdfXmlRepresentation;
  }
 else {
    this.rdfXmlRepresentation=new SaxRepresentation(rdfXmlRepresentation);
    this.rdfXmlRepresentation.setIdentifier(rdfXmlRepresentation.getIdentifier());
  }
}
