def check_hello(self, out, has_length=True):
    self.assertEqual(out, (((('HTTP/1.0 200 OK\r\nServer: WSGIServer/0.1 Python/' + sys.version.split()[0]) + '\r\nContent-Type: text/plain\r\nDate: Mon, 05 Jun 2006 18:49:54 GMT\r\n') + ((has_length and 'Content-Length: 13\r\n') or '')) + '\r\nHello, world!'))
