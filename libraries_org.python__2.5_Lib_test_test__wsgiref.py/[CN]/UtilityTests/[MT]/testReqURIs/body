def testReqURIs(self):
    self.checkReqURI('http://127.0.0.1/')
    self.checkReqURI('http://127.0.0.1/spam', SCRIPT_NAME='/spam')
    self.checkReqURI('http://127.0.0.1/spammity/spam', SCRIPT_NAME='/spammity', PATH_INFO='/spam')
    self.checkReqURI('http://127.0.0.1/spammity/spam?say=ni', SCRIPT_NAME='/spammity', PATH_INFO='/spam', QUERY_STRING='say=ni')
    self.checkReqURI('http://127.0.0.1/spammity/spam', 0, SCRIPT_NAME='/spammity', PATH_INFO='/spam', QUERY_STRING='say=ni')
