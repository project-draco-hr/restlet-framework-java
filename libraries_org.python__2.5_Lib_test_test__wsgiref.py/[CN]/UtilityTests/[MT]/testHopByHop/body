def testHopByHop(self):
    for hop in 'Connection Keep-Alive Proxy-Authenticate Proxy-Authorization TE Trailers Transfer-Encoding Upgrade'.split():
        for alt in (hop, hop.title(), hop.upper(), hop.lower()):
            self.failUnless(util.is_hop_by_hop(alt))
    for hop in 'Accept Cache-Control Date Pragma Trailer Via Warning'.split():
        for alt in (hop, hop.title(), hop.upper(), hop.lower()):
            self.failIf(util.is_hop_by_hop(alt))
