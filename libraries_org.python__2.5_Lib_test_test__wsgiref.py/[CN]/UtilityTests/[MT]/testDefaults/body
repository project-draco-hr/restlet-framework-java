def testDefaults(self):
    for (key, value) in [('SERVER_NAME', '127.0.0.1'), ('SERVER_PORT', '80'), ('SERVER_PROTOCOL', 'HTTP/1.0'), ('HTTP_HOST', '127.0.0.1'), ('REQUEST_METHOD', 'GET'), ('SCRIPT_NAME', ''), ('PATH_INFO', '/'), ('wsgi.version', (1, 0)), ('wsgi.run_once', 0), ('wsgi.multithread', 0), ('wsgi.multiprocess', 0), ('wsgi.input', StringIO('')), ('wsgi.errors', StringIO()), ('wsgi.url_scheme', 'http')]:
        self.checkDefault(key, value)
