{
  if (secure) {
    try {
      return createSecureSocketFactory();
    }
 catch (    IOException ex) {
      getLogger().log(Level.SEVERE,"Could not create secure socket factory: " + ex.getMessage(),ex);
    }
catch (    GeneralSecurityException ex) {
      getLogger().log(Level.SEVERE,"Could not create secure socket factory: " + ex.getMessage(),ex);
    }
  }
  return SocketFactory.getDefault();
}
