def readline(self):
    if (self.peek is not None):
        return ''
    line = self.file.readline()
    if (not line):
        return line
    if self.boundary:
        if (line == (self.boundary + '\n')):
            self.peek = line
            return ''
        if (line == (self.boundary + '--\n')):
            self.peek = line
            return ''
    return line
