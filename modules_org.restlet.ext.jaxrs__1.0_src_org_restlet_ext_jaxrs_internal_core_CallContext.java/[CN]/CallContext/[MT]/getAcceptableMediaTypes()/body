{
  if (this.acceptedMediaTypes == null) {
    List<MediaType> accMediaTypes=new ArrayList<MediaType>();
    for (    org.restlet.data.MediaType mediaType : this.accMediaTypes)     accMediaTypes.add(Converter.toJaxRsMediaType(mediaType));
    this.acceptedMediaTypes=Collections.unmodifiableList(accMediaTypes);
  }
  return this.acceptedMediaTypes;
}
