def test_rfc2822_header_syntax(self):
    eq = self.assertEqual
    m = '>From: foo\nFrom: bar\n!"#QUX;~: zoo\n\nbody'
    msg = email.message_from_string(m)
    eq(len(msg.keys()), 3)
    keys = msg.keys()
    keys.sort()
    eq(keys, ['!"#QUX;~', '>From', 'From'])
    eq(msg.get_payload(), 'body')
