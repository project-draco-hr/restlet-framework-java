def test__contains__(self):
    msg = Message()
    msg['From'] = 'Me'
    msg['to'] = 'You'
    self.failUnless(('from' in msg))
    self.failUnless(('From' in msg))
    self.failUnless(('FROM' in msg))
    self.failUnless(('to' in msg))
    self.failUnless(('To' in msg))
    self.failUnless(('TO' in msg))
