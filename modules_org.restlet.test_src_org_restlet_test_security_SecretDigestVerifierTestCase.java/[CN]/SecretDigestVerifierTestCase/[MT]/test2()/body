{
  MapVerifier mv=new MapVerifier();
  mv.getSecrets().put("scott","RuPXcqGIjq3/JsetpH/XUC15bgc=".toCharArray());
  SecretDigestVerifier sdv=new SecretDigestVerifier(Digest.ALGORITHM_SHA_1,Digest.ALGORITHM_SHA_1,mv);
  assertTrue(sdv.verify("scott","RuPXcqGIjq3/JsetpH/XUC15bgc=".toCharArray()));
}
