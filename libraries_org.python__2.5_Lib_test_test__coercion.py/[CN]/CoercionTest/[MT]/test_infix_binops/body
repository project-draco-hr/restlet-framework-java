def test_infix_binops(self):
    for (ia, a) in enumerate(candidates):
        for (ib, b) in enumerate(candidates):
            results = infix_results[(ia, ib)]
            for (op, res, ires) in zip(infix_binops, results[0], results[1]):
                if (res is TE):
                    self.assertRaises(TypeError, eval, ('a %s b' % op), {'a': a, 'b': b, })
                else:
                    self.assertEquals(format_result(res), format_result(eval(('a %s b' % op))), ('%s %s %s == %s failed' % (a, op, b, res)))
                try:
                    z = copy.copy(a)
                except copy.Error:
                    z = a
                if (ires is TE):
                    try:
                        exec ('z %s= b' % op)
                    except TypeError:
                        pass
                    else:
                        self.fail('TypeError not raised')
                else:
                    exec ('z %s= b' % op)
                    self.assertEquals(ires, z)
