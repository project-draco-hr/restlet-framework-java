def test_infinite_rec_classic_classes(self):


    class Tester:

        def __coerce__(self, other):
            return (other, self)
    exc = TestFailed('__coerce__() returning its arguments reverse should raise RuntimeError')
    try:
        (Tester() + 1)
    except (RuntimeError, TypeError):
        return
    except:
        raise exc
    else:
        raise exc
