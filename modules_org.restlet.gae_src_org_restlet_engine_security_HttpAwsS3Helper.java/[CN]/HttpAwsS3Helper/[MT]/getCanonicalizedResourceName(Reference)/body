{
  final StringBuilder sb=new StringBuilder();
  sb.append(resourceRef.getPath());
  final Form query=resourceRef.getQueryAsForm();
  if (query.getFirst("acl",true) != null) {
    sb.append("?acl");
  }
 else   if (query.getFirst("torrent",true) != null) {
    sb.append("?torrent");
  }
  return sb.toString();
}
