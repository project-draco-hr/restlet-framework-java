def run(self):
    self.errors = StringIO.StringIO()
    startEvent.wait()
    for i in range(FILES_PER_THREAD):
        try:
            f = tempfile.TemporaryFile('w+b')
            f.close()
        except:
            self.error_count += 1
            print_exc(file=self.errors)
        else:
            self.ok_count += 1
