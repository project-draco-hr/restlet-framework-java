{
  try {
    UniformInterface handler=new AbstractHandler(){
      public void handle(      UniformCall call){
        call.setOutput(new StringRepresentation("Hello World!",MediaTypes.TEXT_PLAIN));
      }
    }
;
    Manager.createHttpServer("My server",handler,HttpServer.PROTOCOL_HTTP,null,8182).start();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
