{
  if (getHelper().getLogger().isLoggable(Level.FINE)) {
    getHelper().getLogger().fine("Closing connection with \"" + getSocketAddress() + "\" due to lack of activity during "+ getHelper().getMaxIoIdleTimeMs()+ " ms");
  }
  getInboundWay().onTimeOut();
  getOutboundWay().onTimeOut();
  close(false);
}
