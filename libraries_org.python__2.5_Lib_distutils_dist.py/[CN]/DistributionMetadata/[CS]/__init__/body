def __init__(self):
    self.name = None
    self.version = None
    self.author = None
    self.author_email = None
    self.maintainer = None
    self.maintainer_email = None
    self.url = None
    self.license = None
    self.description = None
    self.long_description = None
    self.keywords = None
    self.platforms = None
    self.classifiers = None
    self.download_url = None
    self.provides = None
    self.requires = None
    self.obsoletes = None
