{
  if (decode) {
    return getQueryParameters();
  }
  if (this.queryParametersEncoded == null) {
    final Form queryForm=Converter.toFormEncoded(this.referenceOriginal.getQuery());
    this.queryParametersEncoded=UnmodifiableMultivaluedMap.getFromForm(queryForm,false);
  }
  return this.queryParametersEncoded;
}
