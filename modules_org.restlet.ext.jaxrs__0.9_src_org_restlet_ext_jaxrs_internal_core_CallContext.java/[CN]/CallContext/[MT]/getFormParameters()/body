{
  if (true)   throw new NotYetImplementedException();
  if (!request.getEntity().getMediaType().equals(org.restlet.data.MediaType.APPLICATION_WWW_FORM))   throw new IllegalStateException("The form parameters are only available, if you used the media type \"application/x-www-form-urlencoded\"");
  Form form=request.getEntityAsForm();
  return UnmodifiableMultivaluedMap.getFromForm(form,false);
}
