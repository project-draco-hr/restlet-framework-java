{
  boolean result=false;
  int size;
  size=getInboundMessages().size();
  for (int i=0; i < size; i++) {
    handleInbound(getInboundMessages().poll());
  }
  size=getOutboundMessages().size();
  for (int i=0; i < size; i++) {
    handleOutbound(getOutboundMessages().poll());
  }
  return result;
}
