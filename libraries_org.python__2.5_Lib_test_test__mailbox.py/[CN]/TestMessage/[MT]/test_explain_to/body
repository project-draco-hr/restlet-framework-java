def test_explain_to(self):
    msg = self._factory()
    for class_ in (mailbox.Message, mailbox.MaildirMessage, mailbox.mboxMessage, mailbox.MHMessage, mailbox.BabylMessage, mailbox.MMDFMessage):
        other_msg = class_()
        msg._explain_to(other_msg)
    other_msg = email.Message.Message()
    self.assertRaises(TypeError, (lambda : msg._explain_to(other_msg)))
