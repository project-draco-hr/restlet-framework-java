def test_babyl_to_mh(self):
    pairs = (('unseen', ['unseen']), ('deleted', []), ('filed', []), ('answered', ['replied']), ('forwarded', []), ('edited', []), ('resent', []))
    for (setting, result) in pairs:
        msg = mailbox.BabylMessage(_sample_message)
        msg.add_label(setting)
        self.assert_((mailbox.MHMessage(msg).get_sequences() == result))
    msg = mailbox.BabylMessage(_sample_message)
    for label in ('unseen', 'deleted', 'filed', 'answered', 'forwarded', 'edited', 'resent'):
        msg.add_label(label)
    self.assert_((mailbox.MHMessage(msg).get_sequences() == ['unseen', 'replied']))
