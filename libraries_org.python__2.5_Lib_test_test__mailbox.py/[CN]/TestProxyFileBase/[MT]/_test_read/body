def _test_read(self, proxy):
    proxy.seek(0)
    self.assert_((proxy.read() == 'bar'))
    proxy.seek(1)
    self.assert_((proxy.read() == 'ar'))
    proxy.seek(0)
    self.assert_((proxy.read(2) == 'ba'))
    proxy.seek(1)
    self.assert_((proxy.read((-1)) == 'ar'))
    proxy.seek(2)
    self.assert_((proxy.read(1000) == 'r'))
