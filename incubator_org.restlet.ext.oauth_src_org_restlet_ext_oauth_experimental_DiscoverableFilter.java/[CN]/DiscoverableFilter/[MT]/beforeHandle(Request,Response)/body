{
  if (request.getResourceRef().hasExtensions() && DISCOVERY_RESOURCE.equals(request.getResourceRef().getExtensions())) {
    createInfo(request,response);
    return Filter.STOP;
  }
 else   if (request.getResourceRef().hasExtensions() && WADL_RESOURCE.equals(request.getResourceRef().getExtensions())) {
    createWadl(request,response);
    return Filter.STOP;
  }
  return super.beforeHandle(request,response);
}
