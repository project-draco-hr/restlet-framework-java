{
  if ((getByteBuffer().position() > 0) && (getIoState() == IoState.PROCESSING)) {
    getByteBuffer().flip();
    int bytesWritten=getConnection().getWritableSelectionChannel().write(getByteBuffer());
    System.out.println("Bytes written: " + bytesWritten);
    if (bytesWritten == 0) {
      setIoState(IoState.INTEREST);
    }
 else     if (getByteBuffer().hasRemaining()) {
      getByteBuffer().compact();
    }
 else     if (getMessageState() == MessageState.IDLE) {
      onCompleted();
    }
 else {
      getByteBuffer().clear();
    }
  }
}
