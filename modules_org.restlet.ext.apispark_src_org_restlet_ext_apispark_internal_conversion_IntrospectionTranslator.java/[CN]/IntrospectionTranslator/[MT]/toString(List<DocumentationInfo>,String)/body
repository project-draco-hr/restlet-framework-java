{
  if (dis != null && !dis.isEmpty()) {
    StringBuilder d=new StringBuilder();
    for (    DocumentationInfo doc : dis) {
      if (doc.getTextContent() != null) {
        d.append(doc.getTextContent());
      }
    }
    if (d.length() > 0) {
      return d.toString();
    }
  }
  return defaultValue;
}
