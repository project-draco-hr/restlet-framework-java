def test_sorted_dict(self):
    d = {'a': 1, 'b': 1, 'c': 1, }
    self.assertEqual(pprint.pformat(d), "{'a': 1, 'b': 1, 'c': 1}")
    self.assertEqual(pprint.pformat([d, d]), "[{'a': 1, 'b': 1, 'c': 1}, {'a': 1, 'b': 1, 'c': 1}]")
    self.assertEqual(pprint.pformat({'xy\tab\n': (3,), 5: [[]], (): {}, }), "{5: [[]], 'xy\\tab\\n': (3,), (): {}}")
