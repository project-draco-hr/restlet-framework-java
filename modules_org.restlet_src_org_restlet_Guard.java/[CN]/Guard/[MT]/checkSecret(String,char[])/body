{
  boolean result=false;
  char[] secret2=findSecret(identifier);
  if (secret == null || secret2 == null) {
    result=(secret == secret2);
  }
 else {
    if (secret.length == secret2.length) {
      boolean equals=true;
      for (int i=0; i < secret.length && equals; i++) {
        equals=(secret[i] == secret2[i]);
      }
      result=equals;
    }
  }
  return result;
}
