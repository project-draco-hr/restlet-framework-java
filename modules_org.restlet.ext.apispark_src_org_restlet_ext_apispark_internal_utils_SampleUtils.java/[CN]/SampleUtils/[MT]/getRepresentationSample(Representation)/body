{
  List<Property> properties=representation.getProperties();
  Map<String,Object> content=new HashMap<>();
  for (  Property property : properties) {
    String fieldName=property.getName();
    Object fieldValue=property.getExample() != null ? property.getExample() : getSampleValue(property.getType(),property.getName(),property.getMaxOccurs() != 1);
    content.put(fieldName,fieldValue);
  }
  return content;
}
