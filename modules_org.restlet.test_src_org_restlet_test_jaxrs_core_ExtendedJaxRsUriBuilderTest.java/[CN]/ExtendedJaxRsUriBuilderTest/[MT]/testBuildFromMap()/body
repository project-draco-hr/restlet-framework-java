{
  final Map<String,Object> vars=new HashMap<String,Object>();
  try {
    this.uriBuilderWithVars.buildFromMap(vars);
    fail("must fail, because missing UriTemplate variables");
  }
 catch (  IllegalArgumentException e) {
  }
  vars.put("var1","123");
  try {
    this.uriBuilderWithVars.buildFromMap(vars);
    fail("must fail, because missing UriTemplate variable");
  }
 catch (  IllegalArgumentException e) {
  }
  vars.put("var2","456");
  vars.put("extension","odt");
  assertEqualsURI("http://localhost/abc/123/def/456.odt",this.uriBuilderWithVars.buildFromMap(vars));
  vars.put("var3","789");
  assertEqualsURI("http://localhost/abc/123/def/456.odt",this.uriBuilderWithVars.buildFromMap(vars));
  vars.put("var2"," ");
  assertEqualsURI("http://localhost/abc/123/def/%20.odt",this.uriBuilderWithVars.buildFromMap(vars));
}
