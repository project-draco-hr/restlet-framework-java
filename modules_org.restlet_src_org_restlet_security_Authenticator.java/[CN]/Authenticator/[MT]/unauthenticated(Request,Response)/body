{
  if (isOptional()) {
    return CONTINUE;
  }
 else {
    if (request.getChallengeResponse() != null) {
      request.getChallengeResponse().setAuthenticated(false);
    }
    if (request.getClientInfo() != null) {
      request.getClientInfo().setAuthenticated(false);
    }
    return STOP;
  }
}
