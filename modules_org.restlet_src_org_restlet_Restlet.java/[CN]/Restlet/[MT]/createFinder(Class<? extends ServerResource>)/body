{
  Finder result=null;
  if (getFinderClass() != null) {
    result=Finder.createFinder(resourceClass,getFinderClass(),getContext(),getLogger());
  }
 else   if ((getApplication() != null) && (getApplication() != this)) {
    result=getApplication().createFinder(resourceClass);
  }
 else {
    result=Finder.createFinder(resourceClass,Finder.class,getContext(),getLogger());
  }
  return result;
}
