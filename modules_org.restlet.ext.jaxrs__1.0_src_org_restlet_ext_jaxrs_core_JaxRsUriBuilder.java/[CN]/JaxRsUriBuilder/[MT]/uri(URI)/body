{
  if (uri.getScheme() != null)   this.scheme=uri.getScheme();
  if (uri.getHost() != null)   this.host=uri.getHost();
  this.port=uri.getPort();
  if (uri.getUserInfo() != null)   this.userInfo=uri.getUserInfo();
  if (uri.getPath() != null)   this.replacePath(uri.getPath());
  if (uri.getQuery() != null)   this.query=uri.getQuery();
  if (uri.getFragment() != null)   this.fragment=uri.getFragment();
  return this;
}
