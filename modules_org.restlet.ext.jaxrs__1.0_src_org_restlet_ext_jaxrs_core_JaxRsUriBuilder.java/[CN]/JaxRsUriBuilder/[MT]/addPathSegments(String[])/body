{
  List<JaxRsPathSegment> pathSegments=new ArrayList<JaxRsPathSegment>(segments.length);
  int l=segments.length;
  for (int i=0; i < l; i++)   pathSegments.add(new JaxRsPathSegment(segments[i],false,false,encode,true,i));
  for (  JaxRsPathSegment pathSegment : pathSegments)   this.pathSegments.add(pathSegment);
}
