def interact(self):
    self.output.write('\n')
    while True:
        try:
            request = self.getline('help> ')
            if (not request):
                break
        except (KeyboardInterrupt, EOFError):
            break
        request = strip(replace(request, '"', '', "'", ''))
        if (lower(request) in ('q', 'quit')):
            break
        self.help(request)
