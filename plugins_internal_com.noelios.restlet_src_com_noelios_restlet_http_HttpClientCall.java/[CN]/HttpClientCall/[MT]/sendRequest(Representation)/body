{
  OutputStream rs=getRequestStream();
  WritableByteChannel wbc=getRequestChannel();
  if (rs != null) {
    if (entity != null) {
      entity.write(rs);
    }
    rs.flush();
  }
 else   if (wbc != null) {
    if (entity != null) {
      entity.write(wbc);
    }
  }
  if (rs != null) {
    rs.close();
  }
 else   if (wbc != null) {
    wbc.close();
  }
  Status result=new Status(getStatusCode(),null,getReasonPhrase(),null);
  return result;
}
