{
  return new NodeIterator((DetailAST)aObject){
    /** 
 * @see NodeIterator 
 */
    protected DetailAST getFirstNode(    DetailAST aAST){
      return getNextNode(aAST);
    }
    /** 
 * @see NodeIterator 
 */
    protected DetailAST getNextNode(    DetailAST aAST){
      return getNextSibling(aAST);
    }
  }
;
}
