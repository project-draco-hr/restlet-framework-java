def handle_entityref(self, name):
    'Handle entity references.\n\n        There should be no need to override this method; it can be\n        tailored by setting up the self.entitydefs mapping appropriately.\n        '
    table = self.entitydefs
    if table.has_key(name):
        self.handle_data(table[name])
    else:
        self.unknown_entityref(name)
        return
