{
  Status result=null;
  Representation entity=request.isEntityAvailable() ? request.getEntity() : null;
  try {
    if (entity != null) {
    }
    result=new Status(getStatusCode(),null,getReasonPhrase(),null);
  }
 catch (  Exception ioe) {
    result=new Status(Status.CONNECTOR_ERROR_COMMUNICATION,ioe);
  }
 finally {
    if (entity != null) {
      entity.release();
    }
  }
  return result;
}
