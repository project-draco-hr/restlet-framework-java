{
  String TSPECIALS;
  int length;
  TSPECIALS="()<>@,;:/[]?=\\\"";
  length=TSPECIALS.length();
  for (int i=0; i < length; i++) {
    Exception error;
    error=null;
    try {
      new MediaType("application/" + TSPECIALS.charAt(i));
    }
 catch (    IllegalArgumentException e) {
      error=e;
    }
    assertNotNull(error);
    error=null;
    try {
      new MediaType(TSPECIALS.charAt(i) + "/xml");
    }
 catch (    IllegalArgumentException e) {
      error=e;
    }
    assertNotNull(error);
  }
}
