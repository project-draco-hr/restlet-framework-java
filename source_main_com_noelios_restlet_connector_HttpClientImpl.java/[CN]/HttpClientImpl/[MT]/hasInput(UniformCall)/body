{
  boolean result=true;
  if (call.getMethod().equals(Methods.GET) || call.getMethod().equals(Methods.HEAD) || call.getMethod().equals(Methods.DELETE)) {
    result=false;
  }
 else {
    try {
      result=(call.getInput().getStream() != null) || (call.getInput().getChannel() != null);
    }
 catch (    IOException e) {
      result=false;
    }
  }
  return result;
}
