def test_getaddresses_nasty(self):
    eq = self.assertEqual
    eq(Utils.getaddresses(['foo: ;']), [('', '')])
    eq(Utils.getaddresses(['[]*-- =~$']), [('', ''), ('', ''), ('', '*--')])
    eq(Utils.getaddresses(['foo: ;', '"Jason R. Mastaler" <jason@dom.ain>']), [('', ''), ('Jason R. Mastaler', 'jason@dom.ain')])
