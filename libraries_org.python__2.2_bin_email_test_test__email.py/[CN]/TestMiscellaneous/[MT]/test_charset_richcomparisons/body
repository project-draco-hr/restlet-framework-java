def test_charset_richcomparisons(self):
    eq = self.assertEqual
    ne = self.failIfEqual
    cset1 = Charset()
    cset2 = Charset()
    eq(cset1, 'us-ascii')
    eq(cset1, 'US-ASCII')
    eq(cset1, 'Us-AsCiI')
    eq('us-ascii', cset1)
    eq('US-ASCII', cset1)
    eq('Us-AsCiI', cset1)
    ne(cset1, 'usascii')
    ne(cset1, 'USASCII')
    ne(cset1, 'UsAsCiI')
    ne('usascii', cset1)
    ne('USASCII', cset1)
    ne('UsAsCiI', cset1)
    eq(cset1, cset2)
    eq(cset2, cset1)
