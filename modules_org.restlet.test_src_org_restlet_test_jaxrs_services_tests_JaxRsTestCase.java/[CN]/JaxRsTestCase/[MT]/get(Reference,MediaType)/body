{
  if (reference.getBaseRef() == null)   reference.setBaseRef(reference.getHostIdentifier());
  Collection<MediaType> mediaTypes=null;
  if (mediaType != null) {
    mediaTypes=new ArrayList<MediaType>();
    mediaTypes.add(mediaType);
  }
  return accessServer(Method.GET,reference,mediaTypes,null,null,null,null);
}
