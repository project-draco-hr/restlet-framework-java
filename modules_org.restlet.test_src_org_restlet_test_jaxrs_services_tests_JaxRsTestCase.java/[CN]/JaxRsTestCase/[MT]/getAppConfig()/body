{
  ApplicationConfig appConfig=new ApplicationConfig(){
    @Override @SuppressWarnings("unchecked") public Set<Class<?>> getResourceClasses(){
      return (Set)Collections.singleton(getRootResourceClass());
    }
    @Override public Set<Class<?>> getProviderClasses(){
      return (Set)getProvClasses();
    }
  }
;
  return appConfig;
}
