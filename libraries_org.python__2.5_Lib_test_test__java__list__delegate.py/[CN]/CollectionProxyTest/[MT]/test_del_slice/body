def test_del_slice(self):
    initial_value = range(0, 10)

    def make_op_func(i, j, k):

        def _f(xs):
            del xs[i:j:k]
        return _f
    for i in xrange((-12), 12):
        for j in xrange((-12), 12):
            for k in xrange((-12), 12):
                self._list_op_test(initial_value, make_op_func(i, j, k), True)
