def nextfile(self):
    savestdout = self._savestdout
    self._savestdout = 0
    if savestdout:
        sys.stdout = savestdout
    output = self._output
    self._output = 0
    if output:
        output.close()
    file = self._file
    self._file = 0
    if (file and (not self._isstdin)):
        file.close()
    backupfilename = self._backupfilename
    self._backupfilename = 0
    if (backupfilename and (not self._backup)):
        try:
            os.unlink(backupfilename)
        except OSError:
            pass
    self._isstdin = False
    self._buffer = []
    self._bufindex = 0
