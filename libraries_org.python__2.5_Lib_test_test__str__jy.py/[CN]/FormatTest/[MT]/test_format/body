def test_format(self):
    '%#.0f, %e and %+f w/ negative numbers print correctly.'
    self.assertEquals(('%.1f' % 5), '5.0')
    self.assertEquals(('%e' % (-1e-06)), '-1.000000e-06')
    self.assertEquals(('%e' % 0), '0.000000e+00')
    self.assertEquals(('%e' % 1e-06), '1.000000e-06')
    self.assertEquals(('%+f' % (-5)), '-5.000000')
    self.assertEquals(('%+f' % 5), '+5.000000')
