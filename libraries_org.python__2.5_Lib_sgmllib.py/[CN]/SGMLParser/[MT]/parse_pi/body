def parse_pi(self, i):
    rawdata = self.rawdata
    if (rawdata[i:(i + 2)] != '<?'):
        self.error('unexpected call to parse_pi()')
    match = piclose.search(rawdata, (i + 2))
    if (not match):
        return (-1)
    j = match.start(0)
    self.handle_pi(rawdata[(i + 2):j])
    j = match.end(0)
    return (j - i)
