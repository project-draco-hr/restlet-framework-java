def test_kw_combinations(self):
    p = self.thetype(capture)
    self.assertEqual(p(), ((), {}))
    self.assertEqual(p(a=1), ((), {'a': 1, }))
    p = self.thetype(capture, a=1)
    self.assertEqual(p(), ((), {'a': 1, }))
    self.assertEqual(p(b=2), ((), {'a': 1, 'b': 2, }))
    self.assertEqual(p(a=3, b=2), ((), {'a': 3, 'b': 2, }))
