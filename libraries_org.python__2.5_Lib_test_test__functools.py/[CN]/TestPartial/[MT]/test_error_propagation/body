def test_error_propagation(self):

    def f(x, y):
        (x / y)
    self.assertRaises(ZeroDivisionError, self.thetype(f, 1, 0))
    self.assertRaises(ZeroDivisionError, self.thetype(f, 1), 0)
    self.assertRaises(ZeroDivisionError, self.thetype(f), 1, 0)
    self.assertRaises(ZeroDivisionError, self.thetype(f, y=0), 1)
