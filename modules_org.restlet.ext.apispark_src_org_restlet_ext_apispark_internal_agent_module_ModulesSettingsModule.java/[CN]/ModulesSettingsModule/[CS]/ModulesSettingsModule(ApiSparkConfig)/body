{
  ModulesSettingsResource modulesSettingsResource=AgentUtils.getClientResource(apiSparkConfig,null,ModulesSettingsResource.class,MODULE_PATH);
  try {
    modulesSettings=modulesSettingsResource.getSettings();
  }
 catch (  Exception e) {
    throw new AgentConfigurationException("Unable to retrieve agent settings from apispark",e);
  }
}
