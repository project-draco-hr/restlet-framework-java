{
  super(sdcClientHelper,method,requestUri);
  this.connection=connection;
  if (requestUri.startsWith("http")) {
    try {
      setFetchRequest(FetchRequest.newBuilder().setId(UUID.randomUUID().toString()).setResource(requestUri).setStrategy("URLConnection").build());
      getConnection().getFrameSender().sendFrame(FrameInfo.Type.FETCH_REQUEST,getFetchRequest().toByteString());
      FrameInfo frame=getConnection().getFrameReceiver().readOneFrame();
      System.out.println(frame);
    }
 catch (    FramingException e) {
      e.printStackTrace();
    }
  }
 else {
    throw new IllegalArgumentException("Only HTTP or HTTPS resource URIs are allowed here");
  }
}
