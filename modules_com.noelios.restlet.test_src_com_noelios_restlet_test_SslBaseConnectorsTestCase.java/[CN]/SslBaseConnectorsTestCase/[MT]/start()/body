{
  System.setProperty("javax.net.ssl.keyStorePassword",System.getProperty("javax.net.ssl.keyStorePassword","testtest"));
  System.setProperty("javax.net.ssl.trustStorePassword",System.getProperty("javax.net.ssl.trustStorePassword","testtest"));
  System.setProperty("javax.net.ssl.keyStore",System.getProperty("javax.net.ssl.keyStore","dummy.jks"));
  System.setProperty("javax.net.ssl.trustStore",System.getProperty("javax.net.ssl.trustStore","dummy.jks"));
  this.component=new Component();
  configureSslParameters(this.component.getContext());
  final Server server=this.component.getServers().add(Protocol.HTTPS,0);
  final Application application=createApplication(this.component);
  this.component.getDefaultHost().attach(application);
  this.component.start();
  return "https://localhost:" + server.getEphemeralPort() + "/test";
}
