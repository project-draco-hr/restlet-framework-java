def walk(node):
    "\n    Recursively yield all child nodes of *node*, in no specified order.  This is\n    useful if you only want to modify nodes in place and don't care about the\n    context.\n    "
    from collections import deque
    todo = deque([node])
    while todo:
        node = todo.popleft()
        todo.extend(iter_child_nodes(node))
        yield node
