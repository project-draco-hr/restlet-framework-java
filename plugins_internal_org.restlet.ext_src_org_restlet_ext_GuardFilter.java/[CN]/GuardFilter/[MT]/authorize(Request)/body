{
  int result=0;
  String identifier=request.getChallengeResponse().getIdentifier();
  String secret=request.getChallengeResponse().getSecret();
  if ((identifier != null) && (secret != null)) {
    if (secret.equals(getAuthorizations().get(identifier))) {
      result=1;
    }
 else {
      result=-1;
    }
  }
  return result;
}
