def test_getformat(self):
    self.assert_((float.__getformat__('double') in ['unknown', 'IEEE, big-endian', 'IEEE, little-endian']))
    self.assert_((float.__getformat__('float') in ['unknown', 'IEEE, big-endian', 'IEEE, little-endian']))
    self.assertRaises(ValueError, float.__getformat__, 'chicken')
    self.assertRaises(TypeError, float.__getformat__, 1)
