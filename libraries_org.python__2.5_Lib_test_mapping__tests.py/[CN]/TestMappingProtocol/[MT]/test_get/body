def test_get(self):
    BasicTestMappingProtocol.test_get(self)
    d = self._empty_mapping()
    self.assert_((d.get('c') is None))
    self.assertEqual(d.get('c', 3), 3)
    d = self._full_mapping({'a': 1, 'b': 2, })
    self.assert_((d.get('c') is None))
    self.assertEqual(d.get('c', 3), 3)
    self.assertEqual(d.get('a'), 1)
    self.assertEqual(d.get('a', 3), 1)
