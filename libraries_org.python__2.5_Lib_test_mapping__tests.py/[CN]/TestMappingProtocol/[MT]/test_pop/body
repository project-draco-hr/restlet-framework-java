def test_pop(self):
    BasicTestMappingProtocol.test_pop(self)
    d = self._empty_mapping()
    (k, v) = ('abc', 'def')
    x = 4503599627370496L
    y = 4503599627370496
    h = self._full_mapping({x: 'anything', y: 'something else', })
    self.assertEqual(h[x], h[y])
    self.assertEqual(d.pop(k, v), v)
    d[k] = v
    self.assertEqual(d.pop(k, 1), v)
