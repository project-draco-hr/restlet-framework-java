def __init__(self, *args, **kw):
    unittest.TestCase.__init__(self, *args, **kw)
    self.reference = self._reference().copy()
    (key, value) = self.reference.popitem()
    self.other = {key: value, }
    (key, value) = self.reference.popitem()
    self.inmapping = {key: value, }
    self.reference[key] = value
