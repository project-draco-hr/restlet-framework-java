{
  Representation result=null;
  if (this.directoryRedirection) {
    if (this.originalRef != null) {
      redirectPermanent(this.originalRef.getIdentifier() + "/");
    }
 else {
      redirectPermanent(getReference().getIdentifier() + "/");
    }
  }
 else {
    result=super.handle();
  }
  return result;
}
