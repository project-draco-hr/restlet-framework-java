{
  final Map<String,Object> attrsOfRequ=request.getAttributes();
  @SuppressWarnings("unchecked") MultivaluedMap<String,String> headers=(MultivaluedMap)attrsOfRequ.get(ORG_RESTLET_EXT_JAXRS_HTTP_HEADERS);
  if (headers == null) {
    headers=UnmodifiableMultivaluedMap.getFromForm(getHttpHeaders(request),false);
    attrsOfRequ.put(ORG_RESTLET_EXT_JAXRS_HTTP_HEADERS,headers);
  }
  return headers;
}
