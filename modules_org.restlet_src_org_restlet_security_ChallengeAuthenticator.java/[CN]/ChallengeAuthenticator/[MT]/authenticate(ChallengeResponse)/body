{
  int result=Guard.AUTHENTICATION_MISSING;
  if (getScheme() != null) {
    if (cr != null) {
      if (getScheme().equals(cr.getScheme())) {
        final AuthenticationHelper helper=Engine.getInstance().findHelper(cr.getScheme(),false,true);
        if (helper != null) {
        }
 else {
          throw new IllegalArgumentException("Challenge scheme " + getScheme() + " not supported by the Restlet engine.");
        }
      }
 else {
      }
    }
 else {
    }
  }
  if (cr != null) {
    cr.setAuthenticated(result == Guard.AUTHENTICATION_VALID);
  }
  return result;
}
