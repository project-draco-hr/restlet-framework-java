def write(self, data):
    if (self.fileobj is None):
        raise ValueError, 'write() on closed GzipFile object'
    if (len(data) > 0):
        self.size = (self.size + len(data))
        self.crc = zlib.crc32(data, self.crc)
        self.fileobj.write(self.compress.compress(data))
        self.offset += len(data)
