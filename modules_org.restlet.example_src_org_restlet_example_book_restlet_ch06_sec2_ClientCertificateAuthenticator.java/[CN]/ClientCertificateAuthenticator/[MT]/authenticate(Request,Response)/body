{
  @SuppressWarnings("unchecked") List<X509Certificate> certchain=(List<X509Certificate>)request.getAttributes().get("org.restlet.https.clientCertificates");
  List<Principal> principals=getPrincipals(certchain);
  if ((principals != null) && (principals.size() > 0)) {
    request.getClientInfo().getPrincipals().addAll(principals);
    User user=getUser(principals.get(0));
    if (user != null) {
      request.getClientInfo().setUser(user);
    }
    return true;
  }
 else {
    return false;
  }
}
