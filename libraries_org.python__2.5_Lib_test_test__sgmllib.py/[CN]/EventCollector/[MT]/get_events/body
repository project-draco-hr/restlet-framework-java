def get_events(self):
    L = []
    prevtype = None
    for event in self.events:
        type = event[0]
        if (type == prevtype == 'data'):
            L[(-1)] = ('data', (L[(-1)][1] + event[1]))
        else:
            L.append(event)
        prevtype = type
    self.events = L
    return L
