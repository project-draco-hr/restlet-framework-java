def test_callable_proxy(self):
    o = Callable()
    ref1 = weakref.proxy(o)
    self.check_proxy(o, ref1)
    self.assert_((type(ref1) is weakref.CallableProxyType), 'proxy is not of callable type')
    ref1('twinkies!')
    self.assert_((o.bar == 'twinkies!'), 'call through proxy not passed through to original')
    ref1(x='Splat.')
    self.assert_((o.bar == 'Splat.'), 'call through proxy not passed through to original')
    self.assertRaises(TypeError, ref1)
    self.assertRaises(TypeError, ref1, 1, 2, 3)
