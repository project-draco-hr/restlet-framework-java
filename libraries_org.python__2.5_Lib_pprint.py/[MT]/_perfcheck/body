def _perfcheck(object=None):
    import time
    if (object is None):
        object = ([('string', (1, 2), [3, 4], {5: 6, 7: 8, })] * 100000)
    p = PrettyPrinter()
    t1 = time.time()
    _safe_repr(object, {}, None, 0)
    t2 = time.time()
    p.pformat(object)
    t3 = time.time()
    print '_safe_repr:', (t2 - t1)
    print 'pformat:', (t3 - t2)
