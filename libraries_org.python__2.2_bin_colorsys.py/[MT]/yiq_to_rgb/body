def yiq_to_rgb(y, i, q):
    r = ((y + (0.948262 * i)) + (0.624013 * q))
    g = ((y - (0.276066 * i)) - (0.63981 * q))
    b = ((y - (1.10545 * i)) + (1.72986 * q))
    if (r < 0.0):
        r = 0.0
    if (g < 0.0):
        g = 0.0
    if (b < 0.0):
        b = 0.0
    if (r > 1.0):
        r = 1.0
    if (g > 1.0):
        g = 1.0
    if (b > 1.0):
        b = 1.0
    return (r, g, b)
