def load_build(self):
    stack = self.stack
    state = stack.pop()
    inst = stack[(-1)]
    setstate = getattr(inst, '__setstate__', None)
    if setstate:
        setstate(state)
        return
    slotstate = None
    if (isinstance(state, tuple) and (len(state) == 2)):
        (state, slotstate) = state
    if state:
        try:
            inst.__dict__.update(state)
        except RuntimeError:
            for (k, v) in state.items():
                setattr(inst, k, v)
    if slotstate:
        for (k, v) in slotstate.items():
            setattr(inst, k, v)
