def load_string(self):
    rep = self.readline()[:(-1)]
    for q in '"\'':
        if rep.startswith(q):
            if (not rep.endswith(q)):
                raise ValueError, 'insecure string pickle'
            rep = rep[len(q):(- len(q))]
            break
    else:
        raise ValueError, 'insecure string pickle'
    self.append(rep.decode('string-escape'))
