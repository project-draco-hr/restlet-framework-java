def test_matching(self):
    self.failUnless(filecmp.cmp(self.name, self.name_same), 'Comparing file to itself fails')
    self.failUnless(filecmp.cmp(self.name, self.name_same, shallow=False), 'Comparing file to itself fails')
    self.failUnless(filecmp.cmp(self.name, self.name, shallow=False), 'Comparing file to identical file fails')
    self.failUnless(filecmp.cmp(self.name, self.name), 'Comparing file to identical file fails')
