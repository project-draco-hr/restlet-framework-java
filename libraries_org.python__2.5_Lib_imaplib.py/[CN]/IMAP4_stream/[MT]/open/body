def open(self, host=None, port=None):
    'Setup a stream connection.\n        This connection will be used by the routines:\n            read, readline, send, shutdown.\n        '
    self.host = None
    self.port = None
    self.sock = None
    self.file = None
    (self.writefile, self.readfile) = os.popen2(self.command)
