def _append_untagged(self, typ, dat):
    if (dat is None):
        dat = ''
    ur = self.untagged_responses
    if __debug__:
        if (self.debug >= 5):
            self._mesg(('untagged_responses[%s] %s += ["%s"]' % (typ, len(ur.get(typ, '')), dat)))
    if (typ in ur):
        ur[typ].append(dat)
    else:
        ur[typ] = [dat]
