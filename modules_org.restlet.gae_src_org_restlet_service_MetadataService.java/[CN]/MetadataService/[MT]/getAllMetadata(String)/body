{
  List<Metadata> result=null;
  if (extension != null) {
    for (    final MetadataExtension metadataExtension : this.mappings) {
      if (extension.equals(metadataExtension.getName())) {
        if (result == null) {
          result=new ArrayList<Metadata>();
        }
        result.add(metadataExtension.getMetadata());
      }
    }
  }
  return result;
}
