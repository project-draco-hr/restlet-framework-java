def test_constructors(self):
    l0 = []
    l1 = [0]
    l2 = [0, 1]
    u = UserList()
    u0 = UserList(l0)
    u1 = UserList(l1)
    u2 = UserList(l2)
    uu = UserList(u)
    uu0 = UserList(u0)
    uu1 = UserList(u1)
    uu2 = UserList(u2)
    v = UserList(tuple(u))


    class OtherList:

        def __init__(self, initlist):
            self.__data = initlist

        def __len__(self):
            return len(self.__data)

        def __getitem__(self, i):
            return self.__data[i]
    v0 = UserList(OtherList(u0))
    vv = UserList('this is also a sequence')
