def test_setslice(self):
    l = [0, 1]
    u = UserList(l)
    for i in range((-3), 4):
        u[:i] = l[:i]
        self.assertEqual(u, l)
        u2 = u[:]
        u2[:i] = u[:i]
        self.assertEqual(u2, u)
        u[i:] = l[i:]
        self.assertEqual(u, l)
        u2 = u[:]
        u2[i:] = u[i:]
        self.assertEqual(u2, u)
        for j in range((-3), 4):
            u[i:j] = l[i:j]
            self.assertEqual(u, l)
            u2 = u[:]
            u2[i:j] = u[i:j]
            self.assertEqual(u2, u)
    uu2 = u2[:]
    uu2[:0] = [(-2), (-1)]
    self.assertEqual(uu2, [(-2), (-1), 0, 1])
    uu2[0:] = []
    self.assertEqual(uu2, [])
