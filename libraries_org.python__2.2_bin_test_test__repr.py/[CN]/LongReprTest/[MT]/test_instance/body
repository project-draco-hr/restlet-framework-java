def test_instance(self):
    touch(os.path.join(self.subpkgname, (('baz' + os.extsep) + 'py')), 'class baz:\n    pass\n')
    from areallylongpackageandmodulenametotestreprtruncation.areallylongpackageandmodulenametotestreprtruncation import baz
    ibaz = baz.baz()
    self.failUnless(repr(ibaz).startswith('<areallylongpackageandmodulenametotestreprtruncation.areallylongpackageandmodulenametotestreprtruncation.baz.baz instance'))
