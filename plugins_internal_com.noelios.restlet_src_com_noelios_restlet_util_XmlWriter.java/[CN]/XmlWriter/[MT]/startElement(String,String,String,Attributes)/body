{
  if (isDataFormat()) {
    stateStack.push(SEEN_ELEMENT);
    state=SEEN_NOTHING;
    if (depth > 0) {
      characters("\n");
    }
    doIndent();
  }
  elementLevel++;
  nsSupport.pushContext();
  write('<');
  writeName(uri,localName,qName,true);
  writeAttributes(atts);
  if (elementLevel == 1) {
    forceNSDecls();
  }
  writeNSDecls();
  write('>');
  super.startElement(uri,localName,qName,atts);
  if (isDataFormat()) {
    depth++;
  }
}
