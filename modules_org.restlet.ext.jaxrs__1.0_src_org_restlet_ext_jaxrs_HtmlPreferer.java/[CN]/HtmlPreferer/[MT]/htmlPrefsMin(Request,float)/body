{
  for (  Preference<MediaType> accPref : request.getClientInfo().getAcceptedMediaTypes()) {
    MediaType accMediaType=accPref.getMetadata();
    if (accMediaType.equals(MediaType.APPLICATION_XHTML_XML,true) || accMediaType.equals(MediaType.TEXT_HTML,true)) {
      if (accPref.getQuality() < htmlQuality)       accPref.setQuality(htmlQuality);
    }
  }
}
