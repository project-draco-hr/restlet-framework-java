def testMultipleFetch(self):
    'testing the second fetch call to a callproc() is None'
    c = self.cursor()
    try:
        c.callproc('funcnone')
        data = c.fetchone()
        assert (data is not None), 'data was None'
        data = c.fetchone()
        assert (data is None), 'data was not None'
    finally:
        c.close()
