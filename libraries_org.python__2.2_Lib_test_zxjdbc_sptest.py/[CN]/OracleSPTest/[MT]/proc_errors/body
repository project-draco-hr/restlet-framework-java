def proc_errors(self, name):
    c = self.cursor()
    try:
        c.execute(("select * from user_errors where name like '%s%%'" % name.upper()))
        errors = c.fetchall()
        try:
            assert (not errors), 'found errors'
        except AssertionError as e:
            print 'printing errors:'
            for a in errors:
                print a
            raise e
    finally:
        c.close()
