{
  Context ctx=getContext();
  Router router=new Router(ctx);
  OAuthAuthorizer auth2=new OAuthAuthorizer(AuthorizationServerTest.prot + "://localhost:" + MultipleUserAuthorizationServerTest.oauthServerPort+ "/oauth/validate",AuthorizationServerTest.prot + "://localhost:" + MultipleUserAuthorizationServerTest.oauthServerPort+ "/oauth/authorize");
  System.out.println("attaching resource");
  auth2.setNext(Scoped3.class);
  router.attach("/scoped/{oauth-user}",auth2);
  return router;
}
