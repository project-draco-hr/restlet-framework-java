{
  T result=null;
  if (this.object != null) {
    result=this.object;
  }
 else   if (this.representation != null) {
    try {
      result=(T)this.xstream.fromXML(this.representation.getStream());
    }
 catch (    IOException e) {
      Context.getCurrentLogger().log(Level.WARNING,"Unable to parse the object with XStream.",e);
    }
  }
  return result;
}
