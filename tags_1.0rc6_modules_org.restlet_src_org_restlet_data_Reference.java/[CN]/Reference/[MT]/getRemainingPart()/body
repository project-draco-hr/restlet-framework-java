{
  if (getBaseRef() != null) {
    try {
      return toString(true,false).substring(getBaseRef().toString(true,false).length());
    }
 catch (    Exception e) {
      Logger.getLogger(Reference.class.getCanonicalName()).log(Level.WARNING,"Cannot get the remaining part. [baseRef,internalRef]=[" + this.baseRef + ", "+ this.internalRef+ "]",e);
      return null;
    }
  }
 else {
    return toString(true,false);
  }
}
