def test_unicodedecodeerror(self):
    self.check_exceptionobjectargs(UnicodeDecodeError, ['ascii', 'g\xfcrk', 1, 2, 'ouch'], "'ascii' codec can't decode byte 0xfc in position 1: ouch")
    self.check_exceptionobjectargs(UnicodeDecodeError, ['ascii', 'g\xfcrk', 1, 3, 'ouch'], "'ascii' codec can't decode bytes in position 1-2: ouch")
