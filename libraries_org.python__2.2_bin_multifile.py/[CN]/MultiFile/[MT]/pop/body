def pop(self):
    if (self.stack == []):
        raise Error, 'bad MultiFile.pop() call'
    if (self.level <= 1):
        self.last = 0
    else:
        abslastpos = (self.lastpos + self.start)
    self.level = max(0, (self.level - 1))
    del self.stack[0]
    if self.seekable:
        self.start = self.posstack[0]
        del self.posstack[0]
        if (self.level > 0):
            self.lastpos = (abslastpos - self.start)
