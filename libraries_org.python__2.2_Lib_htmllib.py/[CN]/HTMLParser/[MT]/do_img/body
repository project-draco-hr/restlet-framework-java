def do_img(self, attrs):
    align = ''
    alt = '(image)'
    ismap = ''
    src = ''
    width = 0
    height = 0
    for (attrname, value) in attrs:
        if (attrname == 'align'):
            align = value
        if (attrname == 'alt'):
            alt = value
        if (attrname == 'ismap'):
            ismap = value
        if (attrname == 'src'):
            src = value
        if (attrname == 'width'):
            try:
                width = int(value)
            except ValueError:
                pass
        if (attrname == 'height'):
            try:
                height = int(value)
            except ValueError:
                pass
    self.handle_image(src, alt, ismap, align, width, height)
