def start_ol(self, attrs):
    self.formatter.end_paragraph((not self.list_stack))
    self.formatter.push_margin('ol')
    label = '1.'
    for (a, v) in attrs:
        if (a == 'type'):
            if (len(v) == 1):
                v = (v + '.')
            label = v
    self.list_stack.append(['ol', label, 0])
