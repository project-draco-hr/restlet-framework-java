def start_a(self, attrs):
    href = ''
    name = ''
    type = ''
    for (attrname, value) in attrs:
        value = value.strip()
        if (attrname == 'href'):
            href = value
        if (attrname == 'name'):
            name = value
        if (attrname == 'type'):
            type = value.lower()
    self.anchor_bgn(href, name, type)
