def test_gcd(self):
    self.assertEqual(gcd(10, 12), 2)
    self.assertEqual(gcd(10, 15), 5)
    self.assertEqual(gcd(10, 11), 1)
    self.assertEqual(gcd(100, 15), 5)
    self.assertEqual(gcd((-10), 2), (-2))
    self.assertEqual(gcd(10, (-2)), 2)
    self.assertEqual(gcd((-10), (-2)), (-2))
    for i in range(1, 20):
        for j in range(1, 20):
            self.assert_((gcd(i, j) > 0))
            self.assert_((gcd((- i), j) < 0))
            self.assert_((gcd(i, (- j)) > 0))
            self.assert_((gcd((- i), (- j)) < 0))
