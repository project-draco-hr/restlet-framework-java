def rewind(self):
    'Return the uncompressed stream file position indicator to the\n        beginning of the file'
    if (self.mode != READ):
        raise IOError("Can't rewind in write mode")
    self.fileobj.seek(0)
    self._new_member = True
    self.extrabuf = ''
    self.extrasize = 0
    self.offset = 0
