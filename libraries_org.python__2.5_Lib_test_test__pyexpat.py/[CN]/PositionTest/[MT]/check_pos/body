def check_pos(self, event):
    pos = (event, self.parser.CurrentByteIndex, self.parser.CurrentLineNumber, self.parser.CurrentColumnNumber)
    require((self.upto < len(self.expected_list)), 'too many parser events')
    expected = self.expected_list[self.upto]
    require((pos == expected), ('expected position %s, got %s' % (expected, pos)))
    self.upto += 1
