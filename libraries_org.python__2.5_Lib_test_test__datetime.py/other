pickle_choices = [(pickler, unpickler, proto) for pickler in (pickle, cPickle) for unpickler in (pickle, cPickle) for proto in range(3)]
assert (len(pickle_choices) == ((2 * 2) * 3))
OTHERSTUFF = (10, 10L, 34.5, 'abc', {}, [], ())
