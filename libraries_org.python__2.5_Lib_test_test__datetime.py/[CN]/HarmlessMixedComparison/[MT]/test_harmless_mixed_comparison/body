def test_harmless_mixed_comparison(self):
    me = self.theclass(1, 1, 1)
    self.failIf((me == ()))
    self.failUnless((me != ()))
    self.failIf((() == me))
    self.failUnless((() != me))
    self.failUnless((me in [1, 20L, [], me]))
    self.failIf((me not in [1, 20L, [], me]))
    self.failUnless(([] in [me, 1, 20L, []]))
    self.failIf(([] not in [me, 1, 20L, []]))
