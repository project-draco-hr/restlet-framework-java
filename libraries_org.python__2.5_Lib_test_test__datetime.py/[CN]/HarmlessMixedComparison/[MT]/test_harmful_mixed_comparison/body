def test_harmful_mixed_comparison(self):
    me = self.theclass(1, 1, 1)
    self.assertRaises(TypeError, (lambda : (me < ())))
    self.assertRaises(TypeError, (lambda : (me <= ())))
    self.assertRaises(TypeError, (lambda : (me > ())))
    self.assertRaises(TypeError, (lambda : (me >= ())))
    self.assertRaises(TypeError, (lambda : (() < me)))
    self.assertRaises(TypeError, (lambda : (() <= me)))
    self.assertRaises(TypeError, (lambda : (() > me)))
    self.assertRaises(TypeError, (lambda : (() >= me)))
    self.assertRaises(TypeError, cmp, (), me)
    self.assertRaises(TypeError, cmp, me, ())
