def checkJoin(relurl, expected):
    global errors
    result = urlparse.urljoin(RFC1808_BASE, relurl)
    print ('%-13s = %r' % (relurl, result))
    if (result != expected):
        errors += 1
        print ('urljoin(%r, %r)' % (RFC1808_BASE, relurl))
        print ('expected %r,\n     got %r' % (expected, result))
