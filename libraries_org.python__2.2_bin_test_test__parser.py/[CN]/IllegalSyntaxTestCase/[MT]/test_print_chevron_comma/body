def test_print_chevron_comma(self):
    'Illegal input: print >>fp,'
    tree = (257, (264, (265, (266, (268, (1, 'print'), (35, '>>'), (290, (291, (292, (293, (295, (296, (297, (298, (299, (300, (301, (302, (303, (1, 'fp')))))))))))))), (12, ','))), (4, ''))), (0, ''))
    self.check_bad_tree(tree, 'print >>fp,')
