{
  Enumeration<URL> result=null;
  ClassLoader cl=Engine.getUserClassLoader();
  if (cl != null) {
    result=cl.getResources(name);
  }
  if (result == null) {
    cl=Thread.currentThread().getContextClassLoader();
    if (cl != null) {
      result=cl.getResources(name);
    }
  }
  return result;
}
