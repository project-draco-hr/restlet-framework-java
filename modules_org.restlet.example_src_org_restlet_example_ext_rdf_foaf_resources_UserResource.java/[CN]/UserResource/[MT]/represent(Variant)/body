{
  final Map<String,Object> dataModel=new TreeMap<String,Object>();
  dataModel.put("user",this.user);
  dataModel.put("contacts",this.contacts);
  dataModel.put("resourceRef",getRequest().getResourceRef());
  dataModel.put("rootRef",getRequest().getRootRef());
  if (MediaType.APPLICATION_RDF_XML.equals(variant.getMediaType())) {
    return getTemplateRepresentation("user.foaf",dataModel,variant.getMediaType());
  }
 else {
    return getTemplateRepresentation("user.html",dataModel,variant.getMediaType());
  }
}
