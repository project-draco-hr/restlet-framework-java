def test_anon():
    print '  anonymous mmap.mmap(-1, PAGESIZE)...'
    m = mmap.mmap((-1), PAGESIZE)
    for x in xrange(PAGESIZE):
        verify((m[x] == '\x00'), "anonymously mmap'ed contents should be zero")
    for x in xrange(PAGESIZE):
        m[x] = ch = chr((x & 255))
        vereq(m[x], ch)
