{
  if (!isStarted()) {
    AbstractConnector connector=null;
switch (getType()) {
case 1:
      connector=new SelectChannelConnector();
    break;
case 2:
  connector=new BlockingChannelConnector();
break;
case 3:
connector=new SocketConnector();
break;
}
configure(connector);
getWrappedServer().addConnector(connector);
super.start();
}
}
