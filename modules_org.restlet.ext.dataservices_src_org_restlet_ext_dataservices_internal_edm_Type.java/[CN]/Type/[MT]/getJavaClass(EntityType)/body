{
  Class<?> result=null;
  String fullClassName=getPackageName(type.getSchema()) + "." + type.getClassName();
  try {
    result=Class.forName(fullClassName);
  }
 catch (  ClassNotFoundException e) {
    Logger.getLogger(Type.class.getCanonicalName()).warning("Can't find the following class in the class loader: " + fullClassName);
  }
  return result;
}
