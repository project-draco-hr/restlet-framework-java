def test():
    (data, rate, ssize, nchannels) = read_sound_file(findfile('audiotest.au'))
    play_sound_file(data, rate, ssize, nchannels)
    dsp = ossaudiodev.open('w')
    try:
        test_setparameters(dsp)
    finally:
        dsp.close()
        _assert((dsp.closed is True), 'dsp.closed is not True')
