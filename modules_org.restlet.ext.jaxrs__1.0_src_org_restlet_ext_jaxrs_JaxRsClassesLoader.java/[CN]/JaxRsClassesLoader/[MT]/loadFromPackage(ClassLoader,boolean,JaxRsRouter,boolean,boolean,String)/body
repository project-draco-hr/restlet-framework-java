{
  if (!rootResourceClasses && !providers)   return;
  for (  String packageName : packageNames) {
    Iterator<Class<?>> classIter;
    try {
      classIter=new PackageIterator(classLoader,packageName,throwOnExc,jaxRsRouter.getLogger(),Level.WARNING);
    }
 catch (    IOException e) {
      throw new WrappedLoadException("Could not load the package data",e);
    }
    while (classIter.hasNext()) {
      Class<?> clazz=classIter.next();
      addClassToRouter(clazz,jaxRsRouter,rootResourceClasses,providers);
    }
  }
}
