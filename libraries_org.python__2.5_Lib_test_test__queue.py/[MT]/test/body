def test():
    q = Queue.Queue()
    QueueTaskDoneTest(q)
    QueueJoinTest(q)
    QueueJoinTest(q)
    QueueTaskDoneTest(q)
    q = Queue.Queue(QUEUE_SIZE)
    SimpleQueueTest(q)
    SimpleQueueTest(q)
    if verbose:
        print 'Simple Queue tests seemed to work'
    q = FailingQueue(QUEUE_SIZE)
    FailingQueueTest(q)
    FailingQueueTest(q)
    if verbose:
        print 'Failing Queue tests seemed to work'
