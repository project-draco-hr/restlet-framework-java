{
  @SuppressWarnings("unchecked") MultivaluedMap<String,String> headers=(MultivaluedMap)request.getAttributes().get(ORG_RESTLET_EXT_JAXRS_HTTP_HEADERS);
  if (headers == null) {
    headers=UnmodifiableMultivaluedMap.getFromForm(getHttpHeaders(request),false);
    request.getAttributes().put(ORG_RESTLET_EXT_JAXRS_HTTP_HEADERS,headers);
  }
  return headers;
}
