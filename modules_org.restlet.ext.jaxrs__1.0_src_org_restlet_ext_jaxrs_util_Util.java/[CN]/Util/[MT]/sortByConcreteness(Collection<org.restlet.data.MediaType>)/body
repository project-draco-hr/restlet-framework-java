{
  List<org.restlet.data.MediaType> newList=new ArrayList<org.restlet.data.MediaType>(mediaTypes.size());
  for (  org.restlet.data.MediaType mediaType : mediaTypes)   if (specificness(mediaType) > 0)   newList.add(mediaType);
  for (  org.restlet.data.MediaType mediaType : mediaTypes)   if (specificness(mediaType) == 0)   newList.add(mediaType);
  for (  org.restlet.data.MediaType mediaType : mediaTypes)   if (specificness(mediaType) < 0)   newList.add(mediaType);
  return newList;
}
