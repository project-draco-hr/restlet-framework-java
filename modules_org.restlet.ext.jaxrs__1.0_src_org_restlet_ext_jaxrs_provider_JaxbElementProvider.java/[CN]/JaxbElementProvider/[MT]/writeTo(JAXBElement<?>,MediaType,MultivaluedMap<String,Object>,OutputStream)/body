{
  try {
    JAXBContext jaxbContext=getJaxbContext(jaxbElement.getDeclaredType());
    Marshaller marshaller=jaxbContext.createMarshaller();
    marshaller.marshal(jaxbElement.getValue(),entityStream);
  }
 catch (  JAXBException e) {
    throw AbstractProvider.logAndIOExc(logger,"Could not marshal the " + jaxbElement.getDeclaredType().getName(),e);
  }
}
