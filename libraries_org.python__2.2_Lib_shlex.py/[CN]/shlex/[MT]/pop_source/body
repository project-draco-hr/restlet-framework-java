def pop_source(self):
    'Pop the input source stack.'
    self.instream.close()
    (self.infile, self.instream, self.lineno) = self.filestack[0]
    self.filestack = self.filestack[1:]
    if self.debug:
        print ('shlex: popping to %s, line %d' % (self.instream, self.lineno))
    self.state = ' '
