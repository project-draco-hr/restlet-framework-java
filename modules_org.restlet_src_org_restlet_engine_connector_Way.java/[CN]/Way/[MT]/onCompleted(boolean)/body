{
  if (getLogger().isLoggable(Level.FINEST)) {
    if (this instanceof OutboundWay) {
      getLogger().log(Level.FINEST,"OutboundWay#onCompleted: " + endDetected);
    }
 else {
      getLogger().log(Level.FINEST,"InboundWay#onCompleted: " + endDetected);
    }
  }
  setIoState(IoState.IDLE);
  setMessageState(MessageState.IDLE);
  setMessage(null);
  setHeaders(null);
}
