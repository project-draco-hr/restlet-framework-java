{
  ResourceClass rc;
synchronized (this.resourceClasses) {
    rc=resourceClasses.get(jaxRsResourceClass);
  }
  if (rc == null) {
    rc=new ResourceClass(jaxRsResourceClass,tlContext,entityProviders,allCtxResolvers,logger);
synchronized (this.resourceClasses) {
      resourceClasses.put(jaxRsResourceClass,rc);
    }
  }
  return rc;
}
