{
  String result=null;
  CharacterSet charSet=null;
  final Object contentType=httpHeaders.getFirst(CONTENT_TYPE);
  if (contentType == null) {
    charSet=defaultCs;
  }
 else {
    charSet=ContentType.parseCharacterSet(contentType.toString());
    if (charSet == null) {
      charSet=defaultCs;
    }
  }
  if (charSet != null) {
    result=charSet.getName();
  }
  return result;
}
