def __init__(self, fmt=None, datefmt=None):
    '\n        Initialize the formatter with specified format strings.\n\n        Initialize the formatter either with the specified format string, or a\n        default as described above. Allow for specialized date formatting with\n        the optional datefmt argument (if omitted, you get the ISO8601 format).\n        '
    if fmt:
        self._fmt = fmt
    else:
        self._fmt = '%(message)s'
    self.datefmt = datefmt
