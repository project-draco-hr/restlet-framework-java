def _fixupChildren(self, ph, alogger):
    '\n        Ensure that children of the placeholder ph are connected to the\n        specified logger.\n        '
    name = alogger.name
    namelen = len(name)
    for c in ph.loggerMap.keys():
        if (c.parent.name[:namelen] != name):
            alogger.parent = c.parent
            c.parent = alogger
