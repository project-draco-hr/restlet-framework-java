def test_bitop_identities_2(x, y):
    check(((x & y) == (y & x)), 'x & y != y & x for', x, y)
    check(((x | y) == (y | x)), 'x | y != y | x for', x, y)
    check(((x ^ y) == (y ^ x)), 'x ^ y != y ^ x for', x, y)
    check((((x ^ y) ^ x) == y), 'x ^ y ^ x != y for', x, y)
    check(((x & y) == (~ ((~ x) | (~ y)))), 'x & y != ~(~x | ~y) for', x, y)
    check(((x | y) == (~ ((~ x) & (~ y)))), 'x | y != ~(~x & ~y) for', x, y)
    check(((x ^ y) == ((x | y) & (~ (x & y)))), 'x ^ y != (x | y) & ~(x & y) for', x, y)
    check(((x ^ y) == ((x & (~ y)) | ((~ x) & y))), 'x ^ y == (x & ~y) | (~x & y) for', x, y)
    check(((x ^ y) == ((x | y) & ((~ x) | (~ y)))), 'x ^ y == (x | y) & (~x | ~y) for', x, y)
