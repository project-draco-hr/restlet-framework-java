def test_bitop_identities_3(x, y, z):
    check((((x & y) & z) == (x & (y & z))), '(x & y) & z != x & (y & z) for', x, y, z)
    check((((x | y) | z) == (x | (y | z))), '(x | y) | z != x | (y | z) for', x, y, z)
    check((((x ^ y) ^ z) == (x ^ (y ^ z))), '(x ^ y) ^ z != x ^ (y ^ z) for', x, y, z)
    check(((x & (y | z)) == ((x & y) | (x & z))), 'x & (y | z) != (x & y) | (x & z) for', x, y, z)
    check(((x | (y & z)) == ((x | y) & (x | z))), 'x | (y & z) != (x | y) & (x | z) for', x, y, z)
