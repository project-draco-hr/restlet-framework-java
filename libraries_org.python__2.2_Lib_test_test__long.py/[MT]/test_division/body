def test_division(maxdigits=MAXDIGITS):
    if verbose:
        print 'long / * % divmod'
    digits = (range(1, (maxdigits + 1)) + range(KARATSUBA_CUTOFF, (KARATSUBA_CUTOFF + 14)))
    digits.append((KARATSUBA_CUTOFF * 3))
    for lenx in digits:
        x = getran(lenx)
        for leny in digits:
            y = (getran(leny) or 1L)
            test_division_2(x, y)
