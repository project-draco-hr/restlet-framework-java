def _test():
    if (os.name == 'mac'):
        (fss, ok) = macfs.PromptGetFile('File to convert:')
        if (not ok):
            sys.exit(0)
        fname = fss.as_pathname()
    else:
        fname = sys.argv[1]
    binhex(fname, (fname + '.hqx'))
    hexbin((fname + '.hqx'), (fname + '.viahqx'))
    sys.exit(1)
