def testadd(data):
    if verbose:
        print 'add'
    data2 = []
    for d in data:
        str = ''
        for s in d:
            str = (str + chr((ord(s) * 2)))
        data2.append(str)
    if ((audioop.add(data[0], data[0], 1) != data2[0]) or (audioop.add(data[1], data[1], 2) != data2[1]) or (audioop.add(data[2], data[2], 4) != data2[2])):
        return 0
    return 1
