def testratecv(data):
    if verbose:
        print 'ratecv'
    state = None
    (d1, state) = audioop.ratecv(data[0], 1, 1, 8000, 16000, state)
    (d2, state) = audioop.ratecv(data[0], 1, 1, 8000, 16000, state)
    if ((d1 + d2) != '\x00\x00\x01\x01\x02\x01\x00\x00\x01\x01\x02'):
        return 0
    return 1
