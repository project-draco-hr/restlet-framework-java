{
  T result=null;
  if (this.object != null) {
    result=this.object;
  }
 else   if (this.representation != null) {
    try {
      Resource emfResource=createEmfResource();
      emfResource.load(this.representation.getStream(),getLoadOptions());
      result=(T)emfResource.getContents().get(0);
    }
 catch (    IOException e) {
      Context.getCurrentLogger().log(Level.WARNING,"Unable to parse the object with XStream.",e);
    }
  }
  return result;
}
