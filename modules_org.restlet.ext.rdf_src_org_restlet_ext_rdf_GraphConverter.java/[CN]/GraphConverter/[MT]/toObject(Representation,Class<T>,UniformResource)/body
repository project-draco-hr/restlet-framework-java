{
  T result=null;
  try {
    if (source instanceof RdfRepresentation) {
      result=(T)((RdfRepresentation)source).getGraph();
    }
 else {
      result=(T)(new RdfRepresentation(source)).getGraph();
    }
  }
 catch (  Exception e) {
    Context.getCurrentLogger().warning("Cannot convert a source representation into a Graph object.");
  }
  return result;
}
