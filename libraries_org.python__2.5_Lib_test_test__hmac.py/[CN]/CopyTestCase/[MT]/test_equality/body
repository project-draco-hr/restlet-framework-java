def test_equality(self):
    h1 = hmac.HMAC('key')
    h1.update('some random text')
    h2 = h1.copy()
    self.failUnless((h1.digest() == h2.digest()), "Digest of copy doesn't match original digest.")
    self.failUnless((h1.hexdigest() == h2.hexdigest()), "Hexdigest of copy doesn't match original hexdigest.")
