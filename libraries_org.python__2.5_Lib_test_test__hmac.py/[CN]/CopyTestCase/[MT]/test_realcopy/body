def test_realcopy(self):
    h1 = hmac.HMAC('key')
    h2 = h1.copy()
    self.failUnless((id(h1) != id(h2)), 'No real copy of the HMAC instance.')
    self.failUnless((id(h1.inner) != id(h2.inner)), "No real copy of the attribute 'inner'.")
    self.failUnless((id(h1.outer) != id(h2.outer)), "No real copy of the attribute 'outer'.")
