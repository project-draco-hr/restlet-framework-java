{
  uriBuilder1Enc.host("test.domain.org");
  assertEqualsURI("http://test.domain.org/path1/path2",uriBuilder1Enc);
  try {
    uriBuilder1Enc.host("test.domain .org a");
    fail();
  }
 catch (  IllegalArgumentException iae) {
  }
  assertEqualsURI("http://test.domain.org/path1/path2",uriBuilder1Enc);
}
