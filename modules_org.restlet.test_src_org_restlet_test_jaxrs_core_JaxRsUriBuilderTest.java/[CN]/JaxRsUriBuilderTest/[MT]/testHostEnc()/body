{
  this.uriBuilder.host("test.domain.org");
  assertEqualsURI("http://test.domain.org/path1/path2",this.uriBuilder,true);
  try {
    this.uriBuilder.host("test.domain .org a");
    fail();
  }
 catch (  final IllegalArgumentException iae) {
  }
  assertEqualsURI("http://test.domain.org/path1/path2",this.uriBuilder,true);
}
