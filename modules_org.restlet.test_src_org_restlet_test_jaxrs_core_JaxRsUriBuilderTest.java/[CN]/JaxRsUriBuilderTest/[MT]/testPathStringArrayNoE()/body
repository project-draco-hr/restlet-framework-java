{
  uriBuilder1NoE.path("jjj","kkk","ll");
  assertEqualsURI(URI_1 + "/jjj/kkk/ll",uriBuilder1NoE);
  uriBuilder1NoE.path("mno");
  assertEqualsURI(URI_1 + "/jjj/kkk/ll/mno",uriBuilder1NoE);
  try {
    uriBuilder1NoE.path("$");
    fail();
  }
 catch (  IllegalArgumentException iae) {
  }
  assertEqualsURI(URI_1 + "/jjj/kkk/ll/mno",uriBuilder1NoE);
}
