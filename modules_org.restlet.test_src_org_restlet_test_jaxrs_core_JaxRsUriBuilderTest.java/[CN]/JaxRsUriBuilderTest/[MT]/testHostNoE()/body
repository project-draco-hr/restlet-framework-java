{
  uriBuilder1NoE.host("test.domain.org");
  assertEqualsURI("http://test.domain.org/path1/path2",uriBuilder1NoE);
  try {
    uriBuilder1NoE.host("test.domain .org a");
    fail();
  }
 catch (  IllegalArgumentException iae) {
  }
  assertEqualsURI("http://test.domain.org/path1/path2",uriBuilder1NoE);
}
