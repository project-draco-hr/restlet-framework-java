{
  Representation result=null;
  try {
    result=getStatusService().toRepresentation(status,throwable,request,response,getConverterService(),getConnegService(),getMetadataService());
  }
 catch (  Exception e) {
    getLogger().log(Level.WARNING,"Unable to get the custom status representation",e);
  }
  if (result == null) {
    result=getDefaultRepresentation(status,request,response);
  }
  return result;
}
