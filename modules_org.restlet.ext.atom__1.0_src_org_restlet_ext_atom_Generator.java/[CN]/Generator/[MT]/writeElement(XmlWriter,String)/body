{
  AttributesImpl attributes=new AttributesImpl();
  if (getUri() != null && getUri().toString() != null) {
    attributes.addAttribute("","uri",null,"atomUri",getUri().toString());
  }
  if (getVersion() != null) {
    attributes.addAttribute("","version",null,"text",getVersion());
  }
  if (getName() != null) {
    writer.dataElement(namespace,"generator",null,attributes,getName());
  }
 else {
    writer.emptyElement(namespace,"generator",null,attributes);
  }
}
