def test_sf_793826(self):

    def mutatingtuple(tuple1, f, tuple2):

        def g(value, first=[1]):
            if first:
                del first[:]
                f(z.next())
            return value
        items = list(tuple2)
        items[1:1] = list(tuple1)
        gen = imap(g, items)
        z = izip(*([gen] * len(tuple1)))
        z.next()

    def f(t):
        global T
        T = t
        first[:] = list(T)
    first = []
    mutatingtuple((1, 2, 3), f, (4, 5, 6))
    second = list(T)
    self.assertEqual(first, second)
