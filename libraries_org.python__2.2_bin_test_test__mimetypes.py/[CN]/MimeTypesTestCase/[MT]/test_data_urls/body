def test_data_urls(self):
    self.assertEqual(self.db.guess_type('data:,thisIsTextPlain'), ('text/plain', None))
    self.assertEqual(self.db.guess_type('data:;base64,thisIsTextPlain'), ('text/plain', None))
    self.assertEqual(self.db.guess_type('data:text/x-foo,thisIsTextXFoo'), ('text/x-foo', None))
