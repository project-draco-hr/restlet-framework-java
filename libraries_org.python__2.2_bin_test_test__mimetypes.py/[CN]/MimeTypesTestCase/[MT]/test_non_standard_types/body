def test_non_standard_types(self):
    self.assertEqual(self.db.guess_type('foo.xul', strict=1), (None, None))
    self.assertEqual(self.db.guess_extension('image/jpg', strict=1), None)
    self.assertEqual(self.db.guess_type('foo.xul', strict=0), ('text/xul', None))
    self.assertEqual(self.db.guess_extension('image/jpg', strict=0), '.jpg')
