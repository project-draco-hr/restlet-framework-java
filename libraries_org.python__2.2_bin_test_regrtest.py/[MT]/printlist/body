def printlist(x, width=70, indent=4):
    'Print the elements of a sequence to stdout.\n\n    Optional arg width (default 70) is the maximum line length.\n    Optional arg indent (default 4) is the number of blanks with which to\n    begin each line.\n    '
    line = (' ' * indent)
    for one in map(str, x):
        w = (len(line) + len(one))
        if (line[(-1):] == ' '):
            pad = ''
        else:
            pad = ' '
            w += 1
        if (w > width):
            print line
            line = ((' ' * indent) + one)
        else:
            line += (pad + one)
    if (len(line) > indent):
        print line
