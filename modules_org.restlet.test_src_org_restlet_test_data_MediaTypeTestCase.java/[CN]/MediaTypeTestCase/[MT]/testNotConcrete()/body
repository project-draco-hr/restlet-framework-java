{
  assertMediaType("","*","*",false);
  assertMediaType("  ","*","*",false);
  assertMediaType("*/","*","*",false);
  assertMediaType("*/  ","*","*",false);
  assertMediaType(" * /","*","*",false);
  assertMediaType("/*","*","*",false);
  assertMediaType("  /*","*","*",false);
  assertMediaType("/ * ","*","*",false);
  assertMediaType("  / * ","*","*",false);
  assertMediaType("*/*","*","*",false);
  assertMediaType(" * /*","*","*",false);
  assertMediaType("*/ * ","*","*",false);
  assertMediaType(" * / * ","*","*",false);
  assertMediaType("/xml","*","xml",false);
  assertMediaType("/ xml ","*","xml",false);
  assertMediaType("  /xml","*","xml",false);
  assertMediaType("  / xml ","*","xml",false);
  assertMediaType("*/xml","*","xml",false);
  assertMediaType(" * /xml","*","xml",false);
  assertMediaType("*/ xml ","*","xml",false);
  assertMediaType(" * / xml ","*","xml",false);
  assertMediaType("application","application","*",false);
  assertMediaType(" application ","application","*",false);
  assertMediaType("application/","application","*",false);
  assertMediaType(" application /","application","*",false);
  assertMediaType(" application /  ","application","*",false);
  assertMediaType("application/*","application","*",false);
  assertMediaType(" application /*","application","*",false);
  assertMediaType("application/ * ","application","*",false);
  assertMediaType(" application /*","application","*",false);
}
