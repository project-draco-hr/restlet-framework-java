{
  assertEquals(MediaType.TEXT_ALL,MediaType.getMostSpecific(MediaType.ALL,MediaType.TEXT_ALL));
  assertEquals(MediaType.TEXT_ALL,MediaType.getMostSpecific(MediaType.TEXT_ALL,MediaType.ALL));
  assertEquals(MediaType.TEXT_PLAIN,MediaType.getMostSpecific(MediaType.ALL,MediaType.TEXT_ALL,MediaType.TEXT_PLAIN));
  assertEquals(MediaType.TEXT_PLAIN,MediaType.getMostSpecific(MediaType.ALL,MediaType.TEXT_PLAIN,MediaType.TEXT_ALL));
  assertEquals(MediaType.TEXT_PLAIN,MediaType.getMostSpecific(MediaType.TEXT_ALL,MediaType.ALL,MediaType.TEXT_PLAIN));
  assertEquals(MediaType.TEXT_PLAIN,MediaType.getMostSpecific(MediaType.TEXT_ALL,MediaType.TEXT_PLAIN,MediaType.ALL));
  assertEquals(MediaType.TEXT_PLAIN,MediaType.getMostSpecific(MediaType.TEXT_PLAIN,MediaType.ALL,MediaType.TEXT_ALL));
  assertEquals(MediaType.TEXT_PLAIN,MediaType.getMostSpecific(MediaType.TEXT_PLAIN,MediaType.TEXT_ALL,MediaType.ALL));
}
