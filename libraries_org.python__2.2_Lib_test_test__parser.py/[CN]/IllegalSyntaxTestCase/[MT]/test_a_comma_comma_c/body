def test_a_comma_comma_c(self):
    'Illegal input: a,,c'
    tree = (258, (311, (290, (291, (292, (293, (295, (296, (297, (298, (299, (300, (301, (302, (303, (1, 'a')))))))))))))), (12, ','), (12, ','), (290, (291, (292, (293, (295, (296, (297, (298, (299, (300, (301, (302, (303, (1, 'c'))))))))))))))), (4, ''), (0, ''))
    self.check_bad_tree(tree, 'a,,c')
