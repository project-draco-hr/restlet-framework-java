def test_yield_statement(self):
    self.check_suite('from __future__ import generators\ndef f(): yield 1')
    self.check_suite('from __future__ import generators\ndef f(): return; yield 1')
    self.check_suite('from __future__ import generators\ndef f(): yield 1; return')
    self.check_suite('from __future__ import generators\ndef f():\n    for x in range(30):\n        yield x\n')
