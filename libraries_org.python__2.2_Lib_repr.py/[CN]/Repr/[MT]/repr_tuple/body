def repr_tuple(self, x, level):
    n = len(x)
    if (n == 0):
        return '()'
    if (level <= 0):
        return '(...)'
    s = ''
    for i in range(min(n, self.maxtuple)):
        if s:
            s = (s + ', ')
        s = (s + self.repr1(x[i], (level - 1)))
    if (n > self.maxtuple):
        s = (s + ', ...')
    elif (n == 1):
        s = (s + ',')
    return (('(' + s) + ')')
