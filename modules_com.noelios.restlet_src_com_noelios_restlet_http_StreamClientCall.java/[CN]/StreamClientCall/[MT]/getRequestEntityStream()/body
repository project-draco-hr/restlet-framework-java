{
  if (requestEntityStream == null) {
    if (isRequestChunked() && isKeepAlive()) {
      requestEntityStream=new ChunkedOutputStream(new KeepAliveOutputStream(getRequestHeadStream()));
    }
 else     if (isRequestChunked()) {
      requestEntityStream=new ChunkedOutputStream(getRequestHeadStream());
    }
 else     if (isKeepAlive()) {
      requestEntityStream=new KeepAliveOutputStream(getRequestHeadStream());
    }
 else {
      requestEntityStream=new KeepAliveOutputStream(getRequestHeadStream());
    }
  }
  return requestEntityStream;
}
