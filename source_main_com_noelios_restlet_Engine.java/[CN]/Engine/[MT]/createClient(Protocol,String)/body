{
  Client result=null;
  if (Protocols.HTTP.equals(protocol)) {
    result=new HttpClientImpl(name);
  }
 else   if (Protocols.HTTPS.equals(protocol)) {
    result=new HttpClientImpl(name);
  }
 else   if (Protocols.SMTP.equals(protocol)) {
    result=new JavaMailClient(name);
  }
  return result;
}
