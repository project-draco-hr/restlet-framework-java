{
  Server result=null;
  if (Protocols.AJP.equals(protocol)) {
    result=new JettyServer(protocol,name,target,address,port);
  }
 else   if (Protocols.HTTP.equals(protocol)) {
    result=new JettyServer(protocol,name,target,address,port);
  }
 else   if (Protocols.HTTPS.equals(protocol)) {
    result=new JettyServer(protocol,name,target,address,port);
  }
  return result;
}
