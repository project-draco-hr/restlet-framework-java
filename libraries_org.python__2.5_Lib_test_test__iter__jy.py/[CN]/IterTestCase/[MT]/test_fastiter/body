def test_fastiter(self):


    class MyList(list):

        def __getitem__(self, index):
            return (str(index) + '!')


    class MyTuple(tuple):

        def __getitem__(self, index):
            return (str(index) + '!')
    self.assertEqual(iter(MyList(['a', 'b'])).next(), 'a')
    self.assertEqual(iter(MyTuple(['a', 'b'])).next(), 'a')
