{
  float result=-1.0F;
  if (Object.class.isAssignableFrom(target) || boolean.class.isAssignableFrom(target) || byte.class.isAssignableFrom(target)|| char.class.isAssignableFrom(target)|| double.class.isAssignableFrom(target)|| float.class.isAssignableFrom(target)|| int.class.isAssignableFrom(target)|| long.class.isAssignableFrom(target)|| short.class.isAssignableFrom(target)) {
    if (VARIANT_JSON.isCompatible(source)) {
      result=0.8F;
    }
 else     if (VARIANT_APPLICATION_ALL_XML.isCompatible(source) || VARIANT_APPLICATION_XML.isCompatible(source) || VARIANT_TEXT_XML.isCompatible(source)) {
      result=0.8F;
    }
 else {
      result=0.5F;
    }
  }
  return result;
}
