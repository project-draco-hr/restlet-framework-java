{
  float result=-1.0F;
  if (target != null) {
    if (VARIANT_JSON.isCompatible(source)) {
      result=0.8F;
    }
 else     if (VARIANT_APPLICATION_ALL_XML.isCompatible(source) || VARIANT_APPLICATION_XML.isCompatible(source) || VARIANT_TEXT_XML.isCompatible(source)) {
      result=0.8F;
    }
  }
  return result;
}
