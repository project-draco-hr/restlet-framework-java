{
  float result=-1.0F;
  if (source != null && (source instanceof JibxRepresentation<?> || isJibxBoundClass(source.getClass()))) {
    if (target == null) {
      result=0.5F;
    }
 else     if (isCompatible(target.getMediaType())) {
      result=1.0F;
    }
 else {
      result=0.5F;
    }
  }
  return result;
}
