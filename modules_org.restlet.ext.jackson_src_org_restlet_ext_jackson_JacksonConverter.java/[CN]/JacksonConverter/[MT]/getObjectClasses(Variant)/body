{
  List<Class<?>> result=null;
  if (VARIANT_JSON.isCompatible(source) || VARIANT_JSON_SMILE.isCompatible(source) || VARIANT_XML.isCompatible(source)) {
    result=addObjectClass(result,Object.class);
    result=addObjectClass(result,JacksonRepresentation.class);
    result=addObjectClass(result,JacksonSmileRepresentation.class);
  }
  return result;
}
