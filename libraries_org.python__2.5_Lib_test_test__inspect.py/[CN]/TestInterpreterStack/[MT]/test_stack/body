def test_stack(self):
    self.assert_((len(mod.st) >= 5))
    self.assertEqual(mod.st[0][1:], (modfile, 16, 'eggs', ['    st = inspect.stack()\n'], 0))
    self.assertEqual(mod.st[1][1:], (modfile, 9, 'spam', ['    eggs(b + d, c + f)\n'], 0))
    self.assertEqual(mod.st[2][1:], (modfile, 43, 'argue', ['            spam(a, b, c)\n'], 0))
    self.assertEqual(mod.st[3][1:], (modfile, 39, 'abuse', ['        self.argue(a, b, c)\n'], 0))
