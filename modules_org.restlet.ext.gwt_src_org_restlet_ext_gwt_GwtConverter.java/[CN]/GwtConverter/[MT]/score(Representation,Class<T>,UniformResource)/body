{
  float result=-1.0F;
  if (target != null) {
    if (ObjectRepresentation.class.isAssignableFrom(target)) {
      result=1.0F;
    }
 else     if (Serializable.class.isAssignableFrom(target)) {
      if (MediaType.APPLICATION_JAVA_OBJECT_GWT.equals(source.getMediaType())) {
        result=1.0F;
      }
 else       if (MediaType.APPLICATION_JAVA_OBJECT_GWT.isCompatible(source.getMediaType())) {
        result=0.6F;
      }
    }
  }
  return result;
}
