{
  try {
    return this.reader.isReadable(type,genericType,annotations);
  }
 catch (  NullPointerException e) {
    if (genericType == null || annotations == null) {
      return false;
    }
    throw e;
  }
catch (  IllegalArgumentException e) {
    if (genericType == null || annotations == null) {
      return false;
    }
    throw e;
  }
}
