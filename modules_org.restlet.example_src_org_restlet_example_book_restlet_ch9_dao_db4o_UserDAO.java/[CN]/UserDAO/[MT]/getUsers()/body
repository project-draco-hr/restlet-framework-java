{
  Predicate<User> predicate=new Predicate<User>(){
    static final long serialVersionUID=1l;
    @Override public boolean match(    User arg0){
      return true;
    }
  }
;
  QueryComparator<User> comparator=new QueryComparator<User>(){
    static final long serialVersionUID=1l;
    public int compare(    User arg0,    User arg1){
      return arg0.getLastName().compareToIgnoreCase(arg1.getLastName());
    }
  }
;
  List<User> result=new ArrayList<User>();
  ObjectSet<User> list=objectContainer.query(predicate,comparator);
  result.addAll(list);
  return result;
}
