{
  float result=-1.0F;
  float current;
  if (mediaType != null) {
    for (    Preference<MediaType> pref : getMediaTypePrefs()) {
      current=score(mediaType,pref.getMetadata());
      if (current != -1.0F) {
        current*=pref.getQuality();
      }
      if (current > result) {
        result=current;
      }
    }
  }
 else {
    result=0.0F;
  }
  return result;
}
