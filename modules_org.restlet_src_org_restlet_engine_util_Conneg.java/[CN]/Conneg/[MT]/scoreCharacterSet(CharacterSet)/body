{
  float result=-1.0F;
  float current;
  if (characterSet != null) {
    for (    Preference<CharacterSet> pref : getCharacterSetPrefs()) {
      if (characterSet.equals(pref.getMetadata())) {
        current=1.0F * pref.getQuality();
      }
 else {
        current=-1.0F;
      }
      if (current > result) {
        result=current;
      }
    }
  }
 else {
    result=0.0F;
  }
  return result;
}
