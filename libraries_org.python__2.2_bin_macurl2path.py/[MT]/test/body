def test():
    for url in ['index.html', 'bar/index.html', '/foo/bar/index.html', '/foo/bar/', '/']:
        print `url`, '->', `url2pathname(url)`
    for path in ['drive:', 'drive:dir:', 'drive:dir:file', 'drive:file', 'file', ':file', ':dir:', ':dir:file']:
        print `path`, '->', `pathname2url(path)`
