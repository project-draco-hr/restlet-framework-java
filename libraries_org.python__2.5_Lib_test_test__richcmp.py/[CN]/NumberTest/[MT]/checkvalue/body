def checkvalue(self, opname, a, b, expres):
    for typea in (int, Number):
        for typeb in (int, Number):
            ta = typea(a)
            tb = typeb(b)
            for op in opmap[opname]:
                realres = op(ta, tb)
                realres = getattr(realres, 'x', realres)
                self.assert_((realres is expres))
