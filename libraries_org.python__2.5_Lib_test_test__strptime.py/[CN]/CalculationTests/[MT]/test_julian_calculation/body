def test_julian_calculation(self):
    format_string = '%Y %m %d %H %M %S %w %Z'
    result = _strptime.strptime(time.strftime(format_string, self.time_tuple), format_string)
    self.failUnless((result.tm_yday == self.time_tuple.tm_yday), ('Calculation of tm_yday failed; %s != %s' % (result.tm_yday, self.time_tuple.tm_yday)))
