def test_locale_data_w_regex_metacharacters(self):
    locale_time = _strptime.LocaleTime()
    locale_time.timezone = (frozenset(('utc', 'gmt', 'Tokyo (standard time)')), frozenset('Tokyo (daylight time)'))
    time_re = _strptime.TimeRE(locale_time)
    self.failUnless(time_re.compile('%Z').match('Tokyo (standard time)'), 'locale data that contains regex metacharacters is not properly escaped')
