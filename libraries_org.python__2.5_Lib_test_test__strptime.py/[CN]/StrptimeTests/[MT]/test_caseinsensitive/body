def test_caseinsensitive(self):
    strf_output = time.strftime('%B', self.time_tuple)
    self.failUnless(_strptime.strptime(strf_output.upper(), '%B'), 'strptime does not handle ALL-CAPS names properly')
    self.failUnless(_strptime.strptime(strf_output.lower(), '%B'), 'strptime does not handle lowercase names properly')
    self.failUnless(_strptime.strptime(strf_output.capitalize(), '%B'), 'strptime does not handle capword names properly')
