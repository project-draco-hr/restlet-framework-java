{
  Filter filter=getFilter();
  assertFalse(filter.hasNext());
  filter.setNext(getRestlet());
  filter.start();
  assertTrue(filter.isStarted());
  assertFalse(filter.isStopped());
  Call call=getCall();
  filter.handle(call);
  assertTrue(filter.hasNext());
  filter.setNext(null);
  assertFalse(filter.hasNext());
}
