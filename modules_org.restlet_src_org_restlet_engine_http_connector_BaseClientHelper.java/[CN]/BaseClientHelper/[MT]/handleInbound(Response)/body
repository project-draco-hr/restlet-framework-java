{
  if (response != null) {
    if (response.getOnReceived() != null) {
      response.getOnReceived().handle(response.getRequest(),response);
    }
    CountDownLatch latch=(CountDownLatch)response.getRequest().getAttributes().get("org.restlet.engine.http.connector.latch");
    if (latch != null) {
      latch.countDown();
    }
  }
}
