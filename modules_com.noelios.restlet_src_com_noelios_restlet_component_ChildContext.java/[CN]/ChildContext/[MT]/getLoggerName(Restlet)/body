{
  String result=null;
  if (child != null) {
    Context context=child.getContext();
    if (context != null) {
      result=child.getClass().getCanonicalName();
      if (result == null) {
        result="org.restlet.restlet (";
      }
      result=result + child.hashCode() + ")";
    }
  }
 else {
    result="org.restlet.restlet";
  }
  return result;
}
