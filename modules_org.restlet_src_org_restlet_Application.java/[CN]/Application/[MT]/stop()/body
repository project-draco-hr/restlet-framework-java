{
  if (isStarted()) {
    if (getOutboundRoot() != null) {
      getOutboundRoot().stop();
    }
    if (getInboundRoot() != null) {
      getInboundRoot().stop();
    }
    getServices().stop();
    if (getHelper() != null) {
      getHelper().stop();
    }
    AnnotationUtils.getInstance().clearCache();
    super.stop();
  }
}
