{
  if (isStarted()) {
    if (getOutboundRoot() != null) {
      getOutboundRoot().stop();
    }
    if (getInboundRoot() != null) {
      getInboundRoot().stop();
    }
    for (    Service service : getServices()) {
      service.stop();
    }
    if (getHelper() != null) {
      getHelper().stop();
    }
    AnnotationUtils.clearCache();
    super.stop();
  }
}
