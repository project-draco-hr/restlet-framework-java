{
  try {
    skipSpaces();
    V nextValue=readValue();
    while (nextValue != null) {
      if (canAdd(nextValue,values)) {
        values.add(nextValue);
      }
      skipValueSeparator();
      nextValue=readValue();
    }
  }
 catch (  IOException ioe) {
    Context.getCurrentLogger().log(Level.INFO,"Unable to read a header",ioe);
  }
}
