{
  Parameter result=null;
  String name=readToken();
  int nextChar=read();
  if (name.length() > 0) {
    if (nextChar == '=') {
      result=createParameter(name,readParameterValue());
    }
 else {
      unread();
      result=createParameter(name);
    }
  }
 else {
    throw new IOException("Parameter or extension has no name. Please check your value");
  }
  return result;
}
