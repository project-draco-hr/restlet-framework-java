def test_au(h, f):
    if (h[:4] == '.snd'):
        f = get_long_be
    elif (h[:4] in ('\x00ds.', 'dns.')):
        f = get_long_le
    else:
        return None
    type = 'au'
    hdr_size = f(h[4:8])
    data_size = f(h[8:12])
    encoding = f(h[12:16])
    rate = f(h[16:20])
    nchannels = f(h[20:24])
    sample_size = 1
    if (encoding == 1):
        sample_bits = 'U'
    elif (encoding == 2):
        sample_bits = 8
    elif (encoding == 3):
        sample_bits = 16
        sample_size = 2
    else:
        sample_bits = '?'
    frame_size = (sample_size * nchannels)
    return (type, rate, nchannels, (data_size / frame_size), sample_bits)
