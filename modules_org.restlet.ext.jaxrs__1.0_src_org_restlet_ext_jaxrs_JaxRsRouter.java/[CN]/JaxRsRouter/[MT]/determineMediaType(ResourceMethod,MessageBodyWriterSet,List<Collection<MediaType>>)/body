{
  Collection<MediaType> p=new HashSet<MediaType>();
  p=resourceMethod.getProducedMimes();
  if (p.size() == 1)   return p;
  if (p.isEmpty()) {
    for (    MessageBodyWriter messageBodyWriter : mbwsForEntityClass)     p=messageBodyWriter.getProducedMimes();
  }
  if (p.size() == 1)   return p;
  if (true)   throw new NotYetImplementedException("The determinig of the mediaType for an entity is not ready implemented.");
  if (p.isEmpty())   p.add(MediaType.ALL);
  accMediaTypes.size();
  return Collections.singleton(MediaType.ALL);
}
