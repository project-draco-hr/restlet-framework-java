{
  Response restletResponse=callContext.getResponse();
  restletResponse.setStatus(Status.valueOf(jaxRsResponse.getStatus()));
  Object mediaTypeStr=jaxRsResponse.getMetadata().getFirst(HttpHeaders.CONTENT_TYPE);
  MediaType respMediaType=null;
  if (mediaTypeStr != null)   respMediaType=MediaType.valueOf(mediaTypeStr.toString());
  Object jaxRsEntity=jaxRsResponse.getEntity();
  Representation repr=convertToRepresentation(jaxRsEntity,jaxRsMethod,callContext,respMediaType,jaxRsResponse.getMetadata());
  restletResponse.setEntity(repr);
  Util.copyResponseHeaders(jaxRsResponse.getMetadata(),restletResponse,getLogger());
}
