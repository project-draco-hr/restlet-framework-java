{
  Map<ResourceMethod,List<MediaType>> returnMethods=new HashMap();
  for (  ResourceMethod resourceMethod : resourceMethods) {
    List<MediaType> mimes=getConsOrProdMimes(resourceMethod,inOut);
    for (    MediaType resMethMediaType : mimes) {
      for (      Collection<MediaType> mediaTypes : mediaTypess)       for (      MediaType mediaType : mediaTypes)       if (resMethMediaType.equals(mediaType,true))       returnMethods.put(resourceMethod,mimes);
    }
  }
  return returnMethods;
}
