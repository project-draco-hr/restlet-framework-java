{
  Object result;
  try {
    result=resourceMethod.invoke(resourceObject,callContext,this);
  }
 catch (  WebApplicationException e) {
    throw e;
  }
catch (  InstantiateParameterException e) {
    throw new WebApplicationException(e,404);
  }
catch (  InvocationTargetException ite) {
    throw handleInvokeException(ite,resourceMethod,callContext,"invoke","Exception in resource method");
  }
catch (  RequestHandledException e) {
    throw e;
  }
catch (  Exception e) {
    throw handleInvokeException(e,resourceMethod,callContext,"invoke","Can not invoke the resource method");
  }
  Response restletResponse=callContext.getResponse();
  if (result == null) {
    restletResponse.setStatus(Status.SUCCESS_NO_CONTENT);
    restletResponse.setEntity(null);
    return;
  }
 else {
    restletResponse.setStatus(Status.SUCCESS_OK);
    if (result instanceof javax.ws.rs.core.Response) {
      jaxRsRespToRestletResp((javax.ws.rs.core.Response)result,callContext,resourceMethod);
    }
 else     if (result instanceof javax.ws.rs.core.Response.ResponseBuilder) {
      javax.ws.rs.core.Response jaxRsResponse=((javax.ws.rs.core.Response.ResponseBuilder)result).build();
      jaxRsRespToRestletResp(jaxRsResponse,callContext,resourceMethod);
    }
 else {
      Representation entity=convertToRepresentation(result,resourceMethod,callContext,null,null);
      restletResponse.setEntity(entity);
    }
  }
}
