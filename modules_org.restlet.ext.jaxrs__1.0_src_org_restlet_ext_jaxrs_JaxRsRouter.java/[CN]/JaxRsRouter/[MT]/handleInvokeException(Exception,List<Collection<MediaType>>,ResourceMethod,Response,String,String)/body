{
  if (exception.getCause() instanceof WebApplicationException) {
    WebApplicationException webAppExc=(WebApplicationException)exception.getCause();
    handleWebAppExc(webAppExc,restletResponse,accMediaTypes,resourceMethod);
  }
  restletResponse.setStatus(Status.SERVER_ERROR_INTERNAL);
  getLogger().logp(Level.WARNING,this.getClass().getName(),methodName,logMessage,exception.getCause());
  exception.printStackTrace();
  throw new RequestHandledException();
}
