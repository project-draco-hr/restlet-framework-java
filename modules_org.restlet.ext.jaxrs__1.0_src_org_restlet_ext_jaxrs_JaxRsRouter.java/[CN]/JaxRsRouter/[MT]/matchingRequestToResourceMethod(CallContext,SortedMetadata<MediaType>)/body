{
  Request restletRequest=callContext.getRequest();
  String uriRemainingPart=restletRequest.getResourceRef().getRemainingPart();
  RrcAndRemPath rcat=identifyRootResourceClass(uriRemainingPart,callContext);
  ResObjAndRemPath resourceObjectAndPath=obtainObjectThatHandleRequest(rcat,callContext);
  MediaType givenMediaType=restletRequest.getEntity().getMediaType();
  ResObjAndMeth method=identifyMethodThatHandleRequest(resourceObjectAndPath,callContext,givenMediaType,accMediaTypes);
  return method;
}
