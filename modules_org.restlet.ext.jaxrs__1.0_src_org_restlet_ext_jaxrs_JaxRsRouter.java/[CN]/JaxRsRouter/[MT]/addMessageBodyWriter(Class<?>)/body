{
  Constructor<?> constructor=RootResourceClass.findJaxRsConstructor(messageBodyWriterClass);
  Object provider;
  try {
    provider=RootResourceClass.createInstance(constructor,null,null,null,authenticator);
  }
 catch (  Exception e) {
    throw new IllegalArgumentException("Provider could not be instantiated: " + e.getMessage(),e);
  }
  this.messageBodyWriters.add(new MessageBodyWriter((javax.ws.rs.ext.MessageBodyWriter<?>)provider));
}
