{
  try {
    this.addProvider(jaxRsProviderClass);
  }
 catch (  IllegalArgumentException e) {
    throw new ImplementationException(e);
  }
catch (  InstantiateProviderException e) {
    throw new ImplementationException(e);
  }
catch (  InjectException e) {
    throw new ImplementationException(e);
  }
}
