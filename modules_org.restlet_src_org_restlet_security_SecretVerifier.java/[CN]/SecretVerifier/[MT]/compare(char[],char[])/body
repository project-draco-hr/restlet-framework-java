{
  boolean result=false;
  if ((secret1 != null) && (secret2 != null)) {
    if (secret1.length == secret2.length) {
      boolean equals=true;
      for (int i=0; (i < secret1.length) && equals; i++) {
        equals=(secret1[i] == secret2[i]);
      }
      result=equals;
    }
  }
  return result;
}
