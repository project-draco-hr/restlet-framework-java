{
  Object result=null;
  try {
    if (source instanceof RdfRepresentation) {
      result=((RdfRepresentation)source).getGraph();
    }
 else {
      result=(new RdfRepresentation(source)).getGraph();
    }
  }
 catch (  Exception e) {
    Context.getCurrentLogger().warning("Cannot convert a source representation into a Graph object.");
  }
  return target.cast(result);
}
