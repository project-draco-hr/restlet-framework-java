{
  String result=null;
  if (isAvailable() && getSize() > 0) {
    final java.io.ByteArrayOutputStream baos=new java.io.ByteArrayOutputStream();
    write(baos);
    if (getCharacterSet() != null) {
      result=baos.toString(getCharacterSet().getName());
    }
 else {
      result=baos.toString();
    }
  }
  return result;
}
