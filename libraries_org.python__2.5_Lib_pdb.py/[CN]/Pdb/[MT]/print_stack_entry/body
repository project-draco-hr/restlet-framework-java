def print_stack_entry(self, frame_lineno, prompt_prefix=line_prefix):
    (frame, lineno) = frame_lineno
    if (frame is self.curframe):
        print  >> self.stdout, '>',
    else:
        print  >> self.stdout, ' ',
    print  >> self.stdout, self.format_stack_entry(frame_lineno, prompt_prefix)
