def do_down(self, arg):
    if ((self.curindex + 1) == len(self.stack)):
        print  >> self.stdout, '*** Newest frame'
    else:
        self.curindex = (self.curindex + 1)
        self.curframe = self.stack[self.curindex][0]
        self.print_stack_entry(self.stack[self.curindex])
        self.lineno = None
