def test_join(self):
    self.assertEqual(posixpath.join('/foo', 'bar', '/bar', 'baz'), '/bar/baz')
    self.assertEqual(posixpath.join('/foo', 'bar', 'baz'), '/foo/bar/baz')
    self.assertEqual(posixpath.join('/foo/', 'bar/', 'baz/'), '/foo/bar/baz/')
    self.assertRaises(TypeError, posixpath.join)
