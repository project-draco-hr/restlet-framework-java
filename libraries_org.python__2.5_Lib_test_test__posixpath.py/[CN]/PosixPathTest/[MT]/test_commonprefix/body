def test_commonprefix(self):
    self.assertEqual(posixpath.commonprefix([]), '')
    self.assertEqual(posixpath.commonprefix(['/home/swenson/spam', '/home/swen/spam']), '/home/swen')
    self.assertEqual(posixpath.commonprefix(['/home/swen/spam', '/home/swen/eggs']), '/home/swen/')
    self.assertEqual(posixpath.commonprefix(['/home/swen/spam', '/home/swen/spam']), '/home/swen/spam')
