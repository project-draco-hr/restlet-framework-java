def test_split(self):
    self.assertEqual(posixpath.split('/foo/bar'), ('/foo', 'bar'))
    self.assertEqual(posixpath.split('/'), ('/', ''))
    self.assertEqual(posixpath.split('foo'), ('', 'foo'))
    self.assertEqual(posixpath.split('////foo'), ('////', 'foo'))
    self.assertEqual(posixpath.split('//foo//bar'), ('//foo', 'bar'))
    self.assertRaises(TypeError, posixpath.split)
