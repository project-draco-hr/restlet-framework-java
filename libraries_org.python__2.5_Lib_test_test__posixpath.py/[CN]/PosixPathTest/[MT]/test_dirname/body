def test_dirname(self):
    self.assertEqual(posixpath.dirname('/foo/bar'), '/foo')
    self.assertEqual(posixpath.dirname('/'), '/')
    self.assertEqual(posixpath.dirname('foo'), '')
    self.assertEqual(posixpath.dirname('////foo'), '////')
    self.assertEqual(posixpath.dirname('//foo//bar'), '//foo')
    self.assertRaises(TypeError, posixpath.dirname)
