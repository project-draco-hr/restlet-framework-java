def test_basename(self):
    self.assertEqual(posixpath.basename('/foo/bar'), 'bar')
    self.assertEqual(posixpath.basename('/'), '')
    self.assertEqual(posixpath.basename('foo'), 'foo')
    self.assertEqual(posixpath.basename('////foo'), 'foo')
    self.assertEqual(posixpath.basename('//foo//bar'), 'bar')
    self.assertRaises(TypeError, posixpath.basename)
