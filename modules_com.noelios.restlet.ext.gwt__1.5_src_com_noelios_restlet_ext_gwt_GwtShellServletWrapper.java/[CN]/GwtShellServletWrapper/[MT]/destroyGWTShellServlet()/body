{
  try {
    gwtShellServlet.getClass().getMethod("destroy").invoke(gwtShellServlet);
  }
 catch (  IllegalAccessException x) {
    log("[Noelios Restlet Engine] - Unable to destroy GWTShellServlet",x);
  }
catch (  InvocationTargetException x) {
    log("[Noelios Restlet Engine] - Unable to destroy GWTShellServlet",x);
  }
catch (  NoSuchMethodException x) {
    log("[Noelios Restlet Engine] - Unable to destroy GWTShellServlet",x);
  }
}
