{
  this.logger=(logger == null) ? Logger.getLogger(getClass().getCanonicalName()) : logger;
  this.pattern=pattern;
  this.defaultVariable=new Variable(defaultType,defaultDefaultValue,defaultRequired,defaultFixed);
