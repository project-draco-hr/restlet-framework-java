{
  float result=-1.0F;
  if (source instanceof String) {
    result=1.0F;
  }
 else   if (source instanceof File) {
    result=1.0F;
  }
 else   if (source instanceof Form) {
    if (MediaType.APPLICATION_WWW_FORM.isCompatible(target.getMediaType())) {
      result=1.0F;
    }
 else {
      result=0.5F;
    }
  }
 else   if (source instanceof InputStream) {
    result=1.0F;
  }
 else   if (source instanceof Reader) {
    result=1.0F;
  }
 else   if (source instanceof Representation) {
    result=1.0F;
  }
 else   if (source instanceof Serializable) {
    if (MediaType.APPLICATION_JAVA_OBJECT.isCompatible(target.getMediaType())) {
      result=1.0F;
    }
 else     if (MediaType.APPLICATION_JAVA_OBJECT_XML.isCompatible(target.getMediaType())) {
      result=1.0F;
    }
 else {
      result=0.5F;
    }
  }
  return result;
}
