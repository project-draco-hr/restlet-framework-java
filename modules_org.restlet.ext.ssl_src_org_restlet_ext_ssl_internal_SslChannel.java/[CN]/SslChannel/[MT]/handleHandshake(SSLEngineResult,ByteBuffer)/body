{
  getManager().setHandshakeStatus(sslResult.getHandshakeStatus());
switch (sslResult.getHandshakeStatus()) {
case FINISHED:
    onHandshakeFinished(sslResult);
  break;
case NEED_TASK:
runTask(sslResult,applicationBuffer);
break;
case NEED_UNWRAP:
onUnwrap(sslResult);
break;
case NEED_WRAP:
onWrap(sslResult);
break;
case NOT_HANDSHAKING:
break;
}
}
