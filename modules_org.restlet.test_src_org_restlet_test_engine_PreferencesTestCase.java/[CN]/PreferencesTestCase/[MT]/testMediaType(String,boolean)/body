{
  final AcceptReader<MediaType> pr=new AcceptReader<MediaType>(AcceptReader.TYPE_MEDIA_TYPE,headerValue);
  final List<Preference<MediaType>> prefs=new ArrayList<Preference<MediaType>>();
  Preference<MediaType> pref=pr.readValue();
  while (pref != null) {
    prefs.add(pref);
    pref=pr.readValue();
  }
  final String newHeaderValue=PreferenceUtils.format(prefs);
  if (testEquals) {
    assertEquals(headerValue,newHeaderValue);
  }
}
