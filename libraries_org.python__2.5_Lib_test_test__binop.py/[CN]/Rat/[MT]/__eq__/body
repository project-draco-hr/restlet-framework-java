def __eq__(self, other):
    'Compare two Rats for equality.'
    if isint(other):
        return ((self.__den == 1) and (self.__num == other))
    if isRat(other):
        return ((self.__num == other.__num) and (self.__den == other.__den))
    if isnum(other):
        return (float(self) == other)
    return NotImplemented
