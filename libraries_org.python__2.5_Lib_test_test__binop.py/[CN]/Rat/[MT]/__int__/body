def __int__(self):
    'Convert a Rat to an int; self.den must be 1.'
    if (self.__den == 1):
        try:
            return int(self.__num)
        except OverflowError:
            raise OverflowError, ('%s too large to convert to int' % repr(self))
    raise ValueError, ("can't convert %s to int" % repr(self))
