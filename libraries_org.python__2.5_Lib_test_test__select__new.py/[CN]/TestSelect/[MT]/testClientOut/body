def testClientOut(self):
    self.client.verify_only_writable()
    self.handler.verify_only_writable()
    written = self.client.write()
    self.handler.verify_readable()
    self.handler.read((written / 2))
    self.handler.verify_readable()
    self.handler.read((written / 2))
    self.handler.verify_not_readable()
