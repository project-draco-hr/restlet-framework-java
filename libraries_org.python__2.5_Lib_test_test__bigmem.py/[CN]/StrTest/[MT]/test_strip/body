@bigmemtest(minsize=_2G, memuse=1)
def test_strip(self, size):
    SUBSTR = '   abc def ghi   '
    s = SUBSTR.rjust(size)
    self.assertEquals(len(s), size)
    self.assertEquals(s.strip(), SUBSTR.strip())
    del s
    s = SUBSTR.ljust(size)
    self.assertEquals(len(s), size)
    self.assertEquals(s.strip(), SUBSTR.strip())
