@bigmemtest(minsize=(_2G + 10), memuse=8)
def test_remove(self, size):
    l = ([10] * size)
    self.assertEquals(len(l), size)
    l.remove(10)
    size -= 1
    self.assertEquals(len(l), size)
    l.append(5)
    size += 1
    self.assertEquals(len(l), size)
    self.assertEquals(l[(-2):], [10, 5])
    l.remove(5)
    size -= 1
    self.assertEquals(len(l), size)
    self.assertEquals(l[(-2):], [10, 10])
