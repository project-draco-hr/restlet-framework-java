def testunop(a, res, expr='len(a)', meth='__len__'):
    if verbose:
        print 'checking', expr
    dict = {'a': a, }
    vereq(eval(expr, dict), res)
    t = type(a)
    m = getattr(t, meth)
    while (meth not in t.__dict__):
        t = t.__bases__[0]
    vereq(m, t.__dict__[meth])
    vereq(m(a), res)
    bm = getattr(a, meth)
    vereq(bm(), res)
