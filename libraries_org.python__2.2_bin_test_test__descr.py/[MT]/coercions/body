def coercions():
    if verbose:
        print 'Testing coercions...'


    class I(int):
        pass
    coerce(I(0), 0)
    coerce(0, I(0))


    class L(long):
        pass
    coerce(L(0), 0)
    coerce(L(0), 0L)
    coerce(0, L(0))
    coerce(0L, L(0))


    class F(float):
        pass
    coerce(F(0), 0)
    coerce(F(0), 0L)
    coerce(F(0), 0.0)
    coerce(0, F(0))
    coerce(0L, F(0))
    coerce(0.0, F(0))


    class C(complex):
        pass
    coerce(C(0), 0)
    coerce(C(0), 0L)
    coerce(C(0), 0.0)
    coerce(C(0), 0j)
    coerce(0, C(0))
    coerce(0L, C(0))
    coerce(0.0, C(0))
    coerce(0j, C(0))
