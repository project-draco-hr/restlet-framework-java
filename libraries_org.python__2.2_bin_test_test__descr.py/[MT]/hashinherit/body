def hashinherit():
    if verbose:
        print 'Testing hash of mutable subclasses...'


    class mydict(dict):
        pass
    d = mydict()
    try:
        hash(d)
    except TypeError:
        pass
    else:
        raise TestFailed, 'hash() of dict subclass should fail'


    class mylist(list):
        pass
    d = mylist()
    try:
        hash(d)
    except TypeError:
        pass
    else:
        raise TestFailed, 'hash() of list subclass should fail'
