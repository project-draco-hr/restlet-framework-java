def __init__(self, dict=None, populate=None, keyTransform=None):
    'dict: starting dictionary of values\n        populate: function that returns the populated dictionary\n        keyTransform: function to normalize the keys (e.g., toupper/None)\n        '
    UserDict.__init__(self, dict)
    self._populated = 0
    self.__populateFunc = (populate or (lambda : {}))
    self._keyTransform = (keyTransform or (lambda key: key))
