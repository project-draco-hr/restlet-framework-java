{
  boolean send=true;
  if ((getNoneMatch() != null) && (getNoneMatch().size() > 0)) {
    boolean matched=false;
    if (representation.getTag() != null) {
      Tag tag;
      for (Iterator<Tag> iter=getNoneMatch().iterator(); !matched && iter.hasNext(); ) {
        tag=iter.next();
        matched=tag.equals(representation.getTag()) || tag.equals(Tag.ALL);
      }
    }
    send=!matched;
  }
 else {
    Date modifiedSince=getModifiedSince();
    send=((modifiedSince == null) || (representation.getModificationDate() == null) || DateUtils.after(modifiedSince,representation.getModificationDate()));
  }
  return send;
}
