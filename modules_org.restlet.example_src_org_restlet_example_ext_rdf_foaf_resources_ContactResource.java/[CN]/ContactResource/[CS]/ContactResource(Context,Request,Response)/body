{
  super(context,request,response);
  final String userId=Reference.decode((String)request.getAttributes().get("userId"));
  this.user=getObjectsFacade().getUserById(userId);
  if (user != null) {
    final String contactId=(String)request.getAttributes().get("contactId");
    this.contact=getObjectsFacade().getContactById(contactId);
  }
  getVariants().add(new Variant(MediaType.TEXT_HTML));
}
