def test_basic_roundtrip(self):
    e = self.e
    sdv = self.sdv
    assert (long(sdv(0L, e)) == 0)
    assert (e[0] == 0)
    assert (long(sdv(1L, e)) == 1)
    assert (e[0] == 0)
    assert (long(sdv((-1L), e)) == (-1))
    assert (e[0] == 0)
    assert (long(sdv(self.v, e)) == self.v)
    assert (e[0] == 0)
