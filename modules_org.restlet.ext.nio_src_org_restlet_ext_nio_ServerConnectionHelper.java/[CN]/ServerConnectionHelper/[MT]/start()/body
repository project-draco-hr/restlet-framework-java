{
  this.serverSocketChannel=createServerSocketChannel();
  setEphemeralPort(this.serverSocketChannel.socket());
  getLogger().info("Starting the Restlet NIO " + getProtocols() + " server on port "+ getHelped().getActualPort());
  SslContextFactory factory=SslUtils.getSslContextFactory(this);
  setSslContext(factory.createSslContext());
  super.start();
  try {
    getController().await();
  }
 catch (  InterruptedException ex) {
    getLogger().log(Level.WARNING,"Interrupted while waiting for starting latch. Stopping...",ex);
    stop();
  }
}
