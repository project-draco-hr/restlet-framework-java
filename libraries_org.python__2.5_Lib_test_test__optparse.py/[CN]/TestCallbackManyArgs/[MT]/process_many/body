def process_many(self, option, opt, value, parser_):
    if (opt == '-a'):
        self.assertEqual(value, ('foo', 'bar'))
    elif (opt == '--apple'):
        self.assertEqual(value, ('ding', 'dong'))
    elif (opt == '-b'):
        self.assertEqual(value, (1, 2, 3))
    elif (opt == '--bob'):
        self.assertEqual(value, ((-666), 42, 0))
