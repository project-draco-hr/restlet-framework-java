{
  sslSocket.setNeedClientAuth(getContextFactory().isNeedClientAuthentication());
  sslSocket.setWantClientAuth(getContextFactory().isWantClientAuthentication());
  sslSocket.setEnabledCipherSuites(getContextFactory().getSelectedCipherSuites(sslSocket.getSupportedCipherSuites()));
  sslSocket.setEnabledProtocols(new String[]{getContextFactory().getSslProtocol()});
  return sslSocket;
}
