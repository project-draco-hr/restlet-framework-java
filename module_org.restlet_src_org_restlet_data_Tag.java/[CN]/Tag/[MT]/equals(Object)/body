{
  boolean result=(object != null) && (object instanceof Tag);
  if (result) {
    Tag that=(Tag)object;
    result=(that.isWeak() == isWeak());
    if (getOpaqueTag() == null) {
      result=(that.getOpaqueTag() == null);
    }
 else {
      result=getOpaqueTag().equals(that.getOpaqueTag());
    }
  }
  return result;
}
