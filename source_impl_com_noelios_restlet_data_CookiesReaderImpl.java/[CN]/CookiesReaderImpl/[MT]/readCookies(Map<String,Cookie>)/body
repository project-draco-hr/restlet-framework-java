{
  Cookie cookie=readCookie();
  while (cookie != null) {
    if (cookies.containsKey(cookie.getName())) {
      cookies.put(cookie.getName(),cookie);
    }
    cookie=readCookie();
  }
  try {
    close();
  }
 catch (  IOException ioe) {
    throw new RestletException("Error while closing the input stream",ioe);
  }
}
