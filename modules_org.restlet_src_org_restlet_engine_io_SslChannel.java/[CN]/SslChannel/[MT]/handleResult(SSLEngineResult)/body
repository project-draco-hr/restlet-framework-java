{
  if (Context.getCurrentLogger().isLoggable(Level.INFO)) {
    Context.getCurrentLogger().log(Level.INFO,"SSL I/O result" + sslResult);
  }
switch (sslResult.getStatus()) {
case BUFFER_OVERFLOW:
    doBufferOverflow(sslResult);
  break;
case BUFFER_UNDERFLOW:
doBufferUnderflow(sslResult);
break;
case CLOSED:
doClosed(sslResult);
break;
case OK:
switch (sslResult.getHandshakeStatus()) {
case FINISHED:
doHandshakeFinished(sslResult);
break;
case NEED_TASK:
doTask(sslResult);
break;
case NEED_UNWRAP:
doUnwrap(sslResult);
break;
case NEED_WRAP:
doWrap(sslResult);
break;
case NOT_HANDSHAKING:
break;
}
break;
}
}
