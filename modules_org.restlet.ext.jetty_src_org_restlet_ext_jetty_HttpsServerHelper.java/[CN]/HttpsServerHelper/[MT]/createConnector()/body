{
  AbstractConnector result=null;
  SslContextFactory sslContextFactory=null;
  try {
    sslContextFactory=new RestletSslContextFactory(org.restlet.engine.ssl.SslUtils.getSslContextFactory(this));
  }
 catch (  Exception e) {
    getLogger().log(Level.WARNING,"Unable to create the Jetty SSL context factory",e);
  }
  if (sslContextFactory != null) {
switch (getType()) {
case 1:
      result=new SslSelectChannelConnector(sslContextFactory);
    break;
case 2:
  result=new SslSocketConnector(sslContextFactory);
break;
}
}
return result;
}
