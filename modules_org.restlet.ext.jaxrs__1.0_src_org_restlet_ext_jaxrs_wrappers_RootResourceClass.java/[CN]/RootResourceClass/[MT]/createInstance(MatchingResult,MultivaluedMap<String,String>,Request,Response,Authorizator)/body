{
  Object[] args;
  if (constructor.getParameterTypes().length == 0)   args=new Object[0];
 else   args=getParameterValues(constructor.getParameterAnnotations(),constructor.getParameterTypes(),matchingResult,restletRequ,restletResponse,allTemplParamsEnc,authorizator);
  return new ResourceObject(constructor.newInstance(args),this);
}
