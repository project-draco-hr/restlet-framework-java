{
  Object[] args;
  Constructor constr=findConstructor(getJaxRsClass());
  if (constr.getParameterTypes().length == 0)   args=new Object[0];
 else   args=getParameterValues(constr.getParameterAnnotations(),constr.getParameterTypes(),matchingResult,restletRequ);
  return new ResourceObject(constr.newInstance(args),this);
}
