def __helper(self, alo, ahi, blo, bhi, answer):
    (i, j, k) = x = self.find_longest_match(alo, ahi, blo, bhi)
    if k:
        if ((alo < i) and (blo < j)):
            self.__helper(alo, i, blo, j, answer)
        answer.append(x)
        if (((i + k) < ahi) and ((j + k) < bhi)):
            self.__helper((i + k), ahi, (j + k), bhi, answer)
