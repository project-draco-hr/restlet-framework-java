def setdefault(self, name, default=''):
    lowername = name.lower()
    if (lowername in self.dict):
        return self.dict[lowername]
    else:
        text = ((name + ': ') + default)
        for line in text.split('\n'):
            self.headers.append((line + '\n'))
        self.dict[lowername] = default
        return default
