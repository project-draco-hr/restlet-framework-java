{
  try {
    if (!this.socket.isClosed()) {
      if (!(this.socket instanceof SSLSocket)) {
        this.socket.shutdownOutput();
      }
      this.socket.close();
    }
  }
 catch (  IOException ex) {
    this.log.log(Level.WARNING,"An error occured closing the client socket",ex);
  }
  super.release();
}
