def test_basic(self):
    self.check('as = 4', 'as')
    self.check('with = 4', 'with')
    self.check('class as: pass', 'as')
    self.check('class with: pass', 'with')
    self.check('obj.as = 4', 'as')
    self.check('with.obj = 4', 'with')
    self.check('def with(): pass', 'with')
    self.check('do(); with = 23', 'with')
