def testWithContinue(self):
    mock = mock_contextmanager_generator()
    counter = 0
    while True:
        counter += 1
        if (counter > 2):
            break
        with mock:
            counter += 10
            continue
        counter += 100
    self.assertEqual(counter, 12)
    self.assertAfterWithManagerInvariantsNoError(mock)
