def test_printing(self):


    class BitBucket:

        def write(self, text):
            pass
    out = BitBucket()
    print  >> out, u'abc'
    print  >> out, u'abc', u'def'
    print  >> out, u'abc', 'def'
    print  >> out, 'abc', u'def'
    print  >> out, u'abc\n'
    print  >> out, u'abc\n',
    print  >> out, u'abc\n',
    print  >> out, u'def\n'
    print  >> out, u'def\n'
