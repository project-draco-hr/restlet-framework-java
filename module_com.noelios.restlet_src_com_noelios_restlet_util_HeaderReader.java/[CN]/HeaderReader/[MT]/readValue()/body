{
  StringBuilder sb=null;
  int next=read();
  while ((next != -1) && isValueSeparator(next)) {
    next=read();
  }
  while ((next != -1) && !isValueSeparator(next)) {
    if (sb == null)     sb=new StringBuilder();
    sb.append((char)next);
    next=read();
  }
  return (sb == null) ? null : sb.toString();
}
