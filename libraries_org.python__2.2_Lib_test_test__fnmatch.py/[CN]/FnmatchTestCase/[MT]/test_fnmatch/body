def test_fnmatch(self):
    check = self.check_match
    check('abc', 'abc')
    check('abc', '?*?')
    check('abc', '???*')
    check('abc', '*???')
    check('abc', '???')
    check('abc', '*')
    check('abc', 'ab[cd]')
    check('abc', 'ab[!de]')
    check('abc', 'ab[de]', 0)
    check('a', '??', 0)
    check('a', 'b', 0)
    check('\\', '[\\]')
    check('a', '[!\\]')
    check('\\', '[!\\]', 0)
