def setUp(self):
    self.name = '01234567890123456789012345678901234567890123456789'
    self.name += '01234567890123456789012345678901234567890123456789'
    self.tar = tarfile.open(tmpname(), 'w')
    t = tarfile.TarInfo(self.name)
    self.tar.addfile(t)
    self.tar.close()
    self.tar = tarfile.open(tmpname())
