def test_no_name_attribute(self):
    fp = open(testtar, 'rb')
    data = fp.read()
    fp.close()
    fobj = StringIO.StringIO(data)
    self.assertRaises(AttributeError, getattr, fobj, 'name')
    self.tar.close()
    self.tar = tarfile.open(fileobj=fobj, mode='r')
    self.assertEqual(self.tar.name, None)
