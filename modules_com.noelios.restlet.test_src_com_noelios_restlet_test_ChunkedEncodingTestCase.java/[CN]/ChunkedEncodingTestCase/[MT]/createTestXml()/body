{
  Document doc=createDocument();
  Element root=doc.createElement("root");
  doc.appendChild(root);
  for (int i=0; i < 2; i++) {
    Element e=doc.createElement("child-" + i);
    e.setAttribute("name","name-" + i);
    root.appendChild(e);
  }
  return new DomRepresentation(MediaType.TEXT_XML,doc);
}
