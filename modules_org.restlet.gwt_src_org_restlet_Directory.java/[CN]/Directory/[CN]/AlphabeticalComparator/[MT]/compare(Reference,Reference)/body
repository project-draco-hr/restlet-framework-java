{
  boolean bRep0Null=(rep0.getIdentifier() == null);
  boolean bRep1Null=(rep1.getIdentifier() == null);
  if (bRep0Null && bRep1Null) {
    return 0;
  }
 else {
    if (bRep0Null) {
      return -1;
    }
 else {
      if (bRep1Null) {
        return 1;
      }
 else {
        return compare(rep0.toString(false,false),rep1.toString(false,false));
      }
    }
  }
}
