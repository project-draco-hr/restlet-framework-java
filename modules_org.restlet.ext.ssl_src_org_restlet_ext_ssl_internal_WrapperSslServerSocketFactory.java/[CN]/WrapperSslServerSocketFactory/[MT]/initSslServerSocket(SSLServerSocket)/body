{
  sslServerSocket.setNeedClientAuth(getContextFactory().isNeedClientAuthentication());
  sslServerSocket.setWantClientAuth(getContextFactory().isWantClientAuthentication());
  sslServerSocket.setEnabledCipherSuites(getContextFactory().getSelectedCipherSuites(sslServerSocket.getSupportedCipherSuites()));
  sslServerSocket.setEnabledProtocols(new String[]{getContextFactory().getSslProtocol()});
  return sslServerSocket;
}
