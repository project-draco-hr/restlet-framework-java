{
  if (this.acceptedMediaTypes == null) {
    List<Preference<org.restlet.data.MediaType>> restletAccMediaTypes=restletRequest.getClientInfo().getAcceptedMediaTypes();
    List<MediaType> accMediaTypes=new ArrayList<MediaType>(restletAccMediaTypes.size());
    for (    Preference<org.restlet.data.MediaType> mediaTypePref : restletAccMediaTypes)     accMediaTypes.add(createJaxRsMediaType(mediaTypePref));
    this.acceptedMediaTypes=accMediaTypes;
  }
  return this.acceptedMediaTypes;
}
