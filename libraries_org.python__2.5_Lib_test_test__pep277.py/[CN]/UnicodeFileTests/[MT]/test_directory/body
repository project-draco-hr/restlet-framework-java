def test_directory(self):
    dirname = os.path.join(test_support.TESTFN, u'Gr\xfc\xdf-\u66e8\u66e9\u66eb')
    filename = u'\xdf-\u66e8\u66e9\u66eb'
    oldwd = os.getcwd()
    os.mkdir(dirname)
    os.chdir(dirname)
    f = open(filename, 'w')
    f.write((filename + '\n').encode('utf-8'))
    f.close()
    print repr(filename)
    os.access(filename, os.R_OK)
    os.remove(filename)
    os.chdir(oldwd)
    os.rmdir(dirname)
