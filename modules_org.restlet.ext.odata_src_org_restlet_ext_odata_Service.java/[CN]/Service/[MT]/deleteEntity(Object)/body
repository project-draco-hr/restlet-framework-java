{
  if (getMetadata() == null) {
    return;
  }
  ClientResource resource=new ClientResource(new Reference(serviceRef.toString() + getSubpath(entity)));
  resource.setChallengeResponse(getCredentials());
  try {
    resource.delete();
  }
 catch (  ResourceException re) {
    throw new ResourceException(re.getStatus(),"Can't delete this entity " + resource.getReference());
  }
 finally {
    this.latestRequest=resource.getRequest();
    this.latestResponse=resource.getResponse();
  }
}
