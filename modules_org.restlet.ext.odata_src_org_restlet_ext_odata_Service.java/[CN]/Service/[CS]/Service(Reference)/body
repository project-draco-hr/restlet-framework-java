{
  try {
    ClientResource cr=new ClientResource(serviceRef);
    cr.setFollowingRedirects(false);
    cr.get();
    if (cr.getStatus().isRedirection()) {
      this.serviceRef=cr.getLocationRef();
    }
 else {
      this.serviceRef=cr.getReference();
    }
  }
 catch (  Throwable e) {
    this.serviceRef=serviceRef;
  }
}
