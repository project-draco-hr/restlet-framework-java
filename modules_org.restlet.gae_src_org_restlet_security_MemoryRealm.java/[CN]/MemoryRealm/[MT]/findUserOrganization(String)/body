{
  Organization result=null;
  int at=identifier.indexOf('@');
  String domainName=(at == -1) ? null : identifier.substring(at + 1);
  if (domainName == null) {
    if (getOrganizations().size() == 1) {
      result=getOrganizations().get(0);
    }
 else {
      Context.getCurrentLogger().info("Unable to identify an unqualified user. Multiple organizations were bounded.");
    }
  }
 else {
    result=findOrganization(domainName);
  }
  return result;
}
