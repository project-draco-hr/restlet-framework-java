{
  final Application appConfig=new Application(){
    @Override @SuppressWarnings("unchecked") public Set<Object> getSingletons(){
      return (Set)TestUtils.createSet(new ThrowWebAppExcProvider());
    }
    @Override @SuppressWarnings("unchecked") public Set<Class<?>> getClasses(){
      return (Set)Collections.singleton(SimpleResource.class);
    }
  }
;
  return appConfig;
}
