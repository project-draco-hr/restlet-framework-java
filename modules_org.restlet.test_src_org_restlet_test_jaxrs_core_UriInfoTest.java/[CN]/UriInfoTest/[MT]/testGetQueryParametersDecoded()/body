{
  assertEquals("123",createUriInfo7().getQueryParameters(true).getFirst("def"));
  assertEquals("456",createUriInfo7().getQueryParameters(true).getFirst("ghi"));
  assertEquals(2,createUriInfo7().getQueryParameters(true).size());
  assertEquals("1 23",createUriInfo8().getQueryParameters(true).getFirst("def"));
  assertEquals("45 6",createUriInfo8().getQueryParameters(true).getFirst("gh i"));
  assertEquals(2,createUriInfo8().getQueryParameters(true).size());
}
