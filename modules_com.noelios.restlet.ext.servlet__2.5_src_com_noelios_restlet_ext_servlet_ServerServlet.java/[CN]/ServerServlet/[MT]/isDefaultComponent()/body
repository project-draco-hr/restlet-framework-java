{
  final String configPath=getServletContext().getRealPath("/WEB-INF/restlet.xml");
  final File configFile=new File(configPath);
  final String componentAttributeName=getInitParameter(COMPONENT_KEY,null);
  if (((configPath != null) && configFile.exists()) || (componentAttributeName != null)) {
    return false;
  }
  return true;
}
